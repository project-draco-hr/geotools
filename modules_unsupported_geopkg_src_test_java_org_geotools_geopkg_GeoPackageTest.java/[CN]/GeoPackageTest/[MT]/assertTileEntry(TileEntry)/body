{
  assertContentEntry(entry);
  Connection cx=geopkg.getDataSource().getConnection();
  try {
    PreparedStatement ps=cx.prepareStatement("SELECT * FROM tile_table_metadata WHERE t_table_name = ?");
    ps.setString(1,entry.getTableName());
    ResultSet rs=ps.executeQuery();
    assertTrue(rs.next());
    assertEquals(entry.isTimesTwoZoom(),rs.getBoolean("is_times_two_zoom"));
    rs.close();
    ps.close();
    ps=cx.prepareStatement("SELECT count(*) from tile_matrix_metadata WHERE t_table_name = ?");
    ps.setString(1,entry.getTableName());
    rs=ps.executeQuery();
    rs.next();
    assertEquals(rs.getInt(1),entry.getTileMatricies().size());
    rs.close();
    ps.close();
  }
  finally {
    cx.close();
  }
}
