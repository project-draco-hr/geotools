{
  if ("type".equals(key) && (delegate == NULL || delegate == proxy)) {
    delegate=UNINITIALIZED;
    return true;
  }
 else   if ("coordinates".equals(key) && delegate == NULL) {
    proxy=new RecordingHandler();
    delegate=proxy;
    return super.startObjectEntry(key);
  }
 else {
    return super.startObjectEntry(key);
  }
}
