{
  if (nameIdentifier.equalsIgnoreCase("WGS84")) {
    final ReferenceIdentifier[] identifiers={new NamedIdentifier(Citations.OGC,"WGS84"),new NamedIdentifier(Citations.ORACLE,"WGS 84"),new NamedIdentifier(null,"WGS_84"),new NamedIdentifier(null,"WGS 1984"),new NamedIdentifier(Citations.EPSG,"WGS_1984"),new NamedIdentifier(Citations.ESRI,"D_WGS_1984"),new NamedIdentifier(Citations.EPSG,"World Geodetic System 1984")};
    return identifiers;
  }
  return null;
}
