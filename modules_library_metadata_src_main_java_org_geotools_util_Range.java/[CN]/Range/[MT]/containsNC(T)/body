{
  if (minValue != null) {
    final int c=minValue.compareTo(value);
    if (c >= 0) {
      if (c != 0 || !isMinIncluded) {
        return false;
      }
    }
  }
  if (maxValue != null) {
    final int c=maxValue.compareTo(value);
    if (c <= 0) {
      if (c != 0 || !isMaxIncluded) {
        return false;
      }
    }
  }
  return true;
}
