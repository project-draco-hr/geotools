{
  try {
    if (typeNames == null || typeNames.isEmpty()) {
      List<Name> names=new ArrayList<>();
      for (      String name : layerMapper.createTypeNames(solrServer)) {
        names.add(new NameImpl(namespaceURI,name));
      }
      typeNames=names;
    }
  }
 catch (  Exception ex) {
    LOGGER.log(Level.SEVERE,ex.getMessage(),ex);
  }
  return typeNames;
}
