{
  RandomIter iterSource;
  if (extender != null) {
    RenderedOp op=BorderDescriptor.create(src,leftPad,rightPad,topPad,bottomPad,extender,GeoTools.getDefaultHints());
    iterSource=RandomIterFactory.create(op,op.getBounds());
  }
 else {
    iterSource=RandomIterFactory.create(src,src.getBounds());
  }
  return iterSource;
}
