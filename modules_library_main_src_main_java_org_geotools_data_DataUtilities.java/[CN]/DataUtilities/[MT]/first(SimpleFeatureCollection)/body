{
  if (simpleFeatureCollection == null) {
    return null;
  }
  SimpleFeatureIterator iter=simpleFeatureCollection.features();
  try {
    while (iter.hasNext()) {
      SimpleFeature feature=iter.next();
      if (feature != null) {
        return feature;
      }
    }
    return null;
  }
  finally {
    iter.close();
  }
}
