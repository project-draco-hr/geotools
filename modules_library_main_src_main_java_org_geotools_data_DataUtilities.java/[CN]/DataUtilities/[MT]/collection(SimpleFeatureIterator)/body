{
  SimpleFeatureCollection collection=FeatureCollections.newCollection();
  try {
    while (reader.hasNext()) {
      try {
        collection.add(reader.next());
      }
 catch (      NoSuchElementException e) {
        throw (IOException)new IOException("EOF").initCause(e);
      }
    }
  }
  finally {
    reader.close();
  }
  return collection;
}
