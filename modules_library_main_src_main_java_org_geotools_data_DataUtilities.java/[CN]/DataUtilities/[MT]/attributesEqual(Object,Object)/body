{
  if (att == null) {
    if (otherAtt != null) {
      return false;
    }
  }
 else {
    if (!att.equals(otherAtt)) {
      if (att instanceof Geometry && otherAtt instanceof Geometry) {
        if (!((Geometry)att).equals((Geometry)otherAtt)) {
          return false;
        }
      }
 else {
        return false;
      }
    }
  }
  return true;
}
