{
  Unit unit=null;
  Field[] fields=class1.getDeclaredFields();
  for (int i=0; i < fields.length; i++) {
    Field field=fields[i];
    String name2=field.getName();
    if ((field.getType().isAssignableFrom(BaseUnit.class) || field.getType().isAssignableFrom(DerivedUnit.class)) && name2.equalsIgnoreCase(name)) {
      try {
        unit=(Unit)field.get(unit);
        return unit;
      }
 catch (      Exception e) {
      }
    }
  }
  return unit;
}
