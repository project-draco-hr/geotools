{
  if (newTitle != title) {
    NotificationChain msgs=null;
    if (title != null)     msgs=((InternalEObject)title).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Wps10Package.INPUT_TYPE__TITLE,null,msgs);
    if (newTitle != null)     msgs=((InternalEObject)newTitle).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Wps10Package.INPUT_TYPE__TITLE,null,msgs);
    msgs=basicSetTitle(newTitle,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Wps10Package.INPUT_TYPE__TITLE,newTitle,newTitle));
}
