{
  boolean found=false;
  final int productsNum=HDF4TeraScanProperties.terascanProducts.getNProducts();
  for (int i=0; i < productsNum; i++) {
    if (dataset.findVariable(HDF4TeraScanProperties.terascanProducts.get(i).getProductName()) != null) {
      found=true;
      break;
    }
  }
  return found;
}
