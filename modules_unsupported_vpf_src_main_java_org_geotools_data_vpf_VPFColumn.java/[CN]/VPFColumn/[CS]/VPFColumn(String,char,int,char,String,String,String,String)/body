{
  this.typeChar=type;
  this.elementsNumber=elementsNumber;
  this.keyType=keyType;
  this.colDesc=colDesc;
  this.valDescTableName=valDescTableName;
  this.thematicIdx=thematicIdx;
  this.narrTable=narrTable;
  attribute=new AttributeTypeBuilder().length(getColumnSize()).binding(getColumnClass()).nillable(true).buildDescriptor(name);
}
