{
  String rootXpath=((XmlFeatureTypeMapping)mapping).itemXpath + XPATH_LEFT_INDEX_BRACKET + xmlResponse.getValidFeatureIndex().get(indexCounter - 1)+ XPATH_RIGHT_INDEX_BRACKET;
  if (((XmlFeatureTypeMapping)mapping).rootAttribute.getInstanceXpath() != null) {
    rootXpath+=XPATH_SEPARATOR + ((XmlFeatureTypeMapping)mapping).rootAttribute.getInstanceXpath();
  }
  return rootXpath;
}
