{
  try {
    return XmlXpathUtilites.getSingleXPathValue(mapping.getNamespaces(),createIndexedItemXpathString() + XPATH_SEPARATOR + idXpath,xmlResponse.getDoc());
  }
 catch (  RuntimeException e) {
    if (e.getCause() instanceof JXPathException) {
      LOGGER.info("Feature id is not mapped for: " + mapping.getTargetFeature().getName());
    }
 else {
      throw e;
    }
  }
  return null;
}
