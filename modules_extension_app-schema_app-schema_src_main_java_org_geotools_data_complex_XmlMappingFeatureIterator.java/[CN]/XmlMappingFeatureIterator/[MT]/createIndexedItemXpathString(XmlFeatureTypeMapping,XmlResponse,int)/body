{
  String rootXpath=mapping.itemXpath + XPATH_LEFT_INDEX_BRACKET + xmlResponse.getValidFeatureIndex().get(indexCounter - 1)+ XPATH_RIGHT_INDEX_BRACKET;
  if (mapping.rootAttribute.getInstanceXpath() != null) {
    rootXpath+=XPATH_SEPARATOR + mapping.rootAttribute.getInstanceXpath();
  }
  return rootXpath;
}
