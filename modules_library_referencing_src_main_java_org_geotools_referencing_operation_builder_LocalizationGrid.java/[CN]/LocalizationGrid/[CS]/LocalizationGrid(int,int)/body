{
  if (width < 2) {
    throw new IllegalArgumentException(String.valueOf(width));
  }
  if (height < 2) {
    throw new IllegalArgumentException(String.valueOf(height));
  }
  this.width=width;
  this.height=height;
  this.grid=new double[width * height * CP_LENGTH];
  Arrays.fill(grid,Float.NaN);
}
