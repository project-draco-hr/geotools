{
  if ((obj == null) || !(obj instanceof TestSuiteDTO)) {
    return false;
  }
  boolean r=true;
  TestSuiteDTO ts=(TestSuiteDTO)obj;
  if (name != null) {
    r=r && (name.equals(ts.getName()));
  }
  if (description != null) {
    r=r && (description.equals(ts.getDescription()));
  }
  if (tests == null) {
    if (ts.getTests() != null) {
      return false;
    }
  }
 else {
    if (ts.getTests() != null) {
      r=r && tests.equals(ts.getTests());
    }
 else {
      return false;
    }
  }
  return r;
}
