{
  Connection cx=geopkg.getDataSource().getConnection();
  Statement st=cx.createStatement();
  try {
    ResultSet rs=st.executeQuery("PRAGMA application_id;");
    assertEquals(rs.getInt(1),0x47503130);
  }
 catch (  Exception e) {
    fail(e.getMessage());
  }
 finally {
    st.close();
    cx.close();
  }
}
