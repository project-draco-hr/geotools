{
  Node srsNameNode=root.getAttributes().getNamedItem("srsName");
  CoordinateReferenceSystem crs=null;
  if (srsNameNode != null) {
    String srs=srsNameNode.getTextContent();
    try {
      crs=CRS.decode(srs);
    }
 catch (    Exception e) {
      LOGGER.warning("Failed to parse the specified SRS " + srs);
    }
  }
  Geometry g=_gml(root);
  if (crs != null) {
    g.setUserData(crs);
  }
  return g;
}
