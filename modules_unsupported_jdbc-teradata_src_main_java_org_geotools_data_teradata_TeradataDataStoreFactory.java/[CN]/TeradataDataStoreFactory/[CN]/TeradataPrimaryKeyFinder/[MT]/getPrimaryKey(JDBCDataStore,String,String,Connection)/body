{
  List<PrimaryKeyColumn> columns=tryForPrimaryKey1(schema,table,cx);
  if (columns.isEmpty()) {
    columns=tryForPrimaryKey(schema,table,cx);
  }
  if (columns.isEmpty()) {
    columns=tryForSequence(schema,table,cx);
  }
  if (columns.isEmpty()) {
    return null;
  }
 else {
    return new PrimaryKey(table,columns);
  }
}
