    c=Class.forName("org.geotools.gui.swing.image.OperationTreeBrowser");
  }
 catch (  ClassNotFoundException cause) {
    final HeadlessException e;
    e=new HeadlessException("The \"gt2-widgets-swing.jar\" file is required.");
    e.initCause(cause);
