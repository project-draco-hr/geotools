{
  String text="{" + "  'type': 'Feature'," + "  'geometry': {"+ "     'type': 'Point',"+ "     'coordinates': [" + (val + 0.1) + ","+ (val + 0.1)+ "]"+ "   }, "+ "'  properties': {"+ "     'int': "+ val+ ","+ "     'double': "+ (val + 0.1)+ ","+ "     'string': '"+ toString(val)+ "'"+ "   },"+ "   'id':'feature."+ val+ "'"+ "}";
  return text;
}
