{
  String json=strip("{" + "  'type': 'FeatureCollection'," + "  'features': ["+ "    {"+ "      'type': 'Feature',"+ "      'properties': {"+ "      },"+ "      'id': 'xyz.1'"+ "    }"+ "  ]"+ "}");
  SimpleFeatureSource fs=DataUtilities.source(fjson.readFeatureCollection(json));
  fjson.toString(fs.getFeatures());
}
