{
  String text="{" + "  'type': 'Feature'," + "'  properties': {"+ "     'int': " + val + ","+ "     'double': "+ (val + 0.1)+ ","+ "     'string': '"+ toString(val)+ "'"+ "   },"+ "  'geometry': {"+ "     'type': 'Point',"+ "     'coordinates': ["+ (val + 0.1)+ ","+ (val + 0.1)+ "]"+ "   }, "+ "   'id':'feature."+ val+ "'"+ "}";
  return text;
}
