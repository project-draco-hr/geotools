{
  String json=strip("{" + "  'type': 'FeatureCollection'," + "  'features': ["+ "    {"+ "      'type': 'Feature',"+ "      'geometry': null,"+ "      'properties': {"+ "      },"+ "      'id': 'xyz.1'"+ "    }"+ "  ]"+ "}");
  SimpleFeatureType type=fjson.readFeatureCollectionSchema(json,true);
  assertNull(type.getGeometryDescriptor());
}
