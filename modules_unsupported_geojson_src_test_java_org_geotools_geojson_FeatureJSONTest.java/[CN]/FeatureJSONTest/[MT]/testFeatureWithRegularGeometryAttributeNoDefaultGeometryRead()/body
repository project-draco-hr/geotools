{
  SimpleFeature f=fjson.readFeature(reader(strip("{" + "   'type': 'Feature'," + "   'properties': {"+ "     'int': 1,"+ "     'double': 0.1,"+ "     'string': 'one',"+ "     'otherGeometry': {"+ "        'type': 'LineString',"+ "        'coordinates': [[1.1, 1.2], [1.3, 1.4]]"+ "     }"+ "   },"+ "   'id': 'feature.0'"+ " }")));
  assertNotNull(f);
  assertTrue(f.getDefaultGeometry() instanceof LineString);
  LineString l=(LineString)f.getDefaultGeometry();
  assertTrue(new GeometryFactory().createLineString(new Coordinate[]{new Coordinate(1.1,1.2),new Coordinate(1.3,1.4)}).equals(l));
  assertTrue(f.getAttribute("otherGeometry") instanceof LineString);
  assertTrue(new GeometryFactory().createLineString(new Coordinate[]{new Coordinate(1.1,1.2),new Coordinate(1.3,1.4)}).equals((LineString)f.getAttribute("otherGeometry")));
  assertEquals(1,((Number)f.getAttribute("int")).intValue());
  assertEquals(0.1,((Number)f.getAttribute("double")).doubleValue());
  assertEquals("one",f.getAttribute("string"));
}
