{
  super(metadata,null,SpatioTemporalMetadataFormat.MD_RECTIFIEDGRID);
  rasterLayoutLimits=new MetadataAccessor(metadata,new StringBuilder(SpatioTemporalMetadataFormat.MD_RECTIFIEDGRID).append(SEPARATOR).append(SpatioTemporalMetadataFormat.MD_RG_LIMITS).append(SEPARATOR).append(SpatioTemporalMetadataFormat.MD_RG_LI_RASTERLAYOUT).toString(),null);
  originPoint=new OriginPoint(metadata);
  axesName=new ChildList.AxesNames(metadata);
  offsetVectors=new ChildList.OffsetVectors(metadata);
}
