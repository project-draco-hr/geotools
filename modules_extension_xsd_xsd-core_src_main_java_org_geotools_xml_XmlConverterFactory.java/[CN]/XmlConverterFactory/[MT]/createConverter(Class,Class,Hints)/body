{
  boolean canHandleTarget=String.class.equals(target) || java.util.Date.class.isAssignableFrom(target) || Calendar.class.equals(target);
  boolean canHandleSource=String.class.equals(source) || java.util.Date.class.isAssignableFrom(source) || Calendar.class.equals(source);
  if (canHandleSource && canHandleTarget) {
    return new XmlConverter();
  }
  return null;
}
