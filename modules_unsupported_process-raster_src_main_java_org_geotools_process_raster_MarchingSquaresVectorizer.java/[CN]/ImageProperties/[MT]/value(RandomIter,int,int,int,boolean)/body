{
  int sum=0;
  if (isSet(iter,x - 1,y - 1,dataType,forceSetting)) {
    sum|=1;
  }
  if (isSet(iter,x,y - 1,dataType,forceSetting)) {
    sum|=2;
  }
  if (isSet(iter,x - 1,y,dataType,forceSetting)) {
    sum|=4;
  }
  if (isSet(iter,x,y,dataType,forceSetting)) {
    sum|=8;
  }
  if (sum == 0) {
    if (LOGGER.isLoggable(Level.INFO)) {
      LOGGER.info("x " + x + " y"+ y);
    }
  }
  return sum;
}
