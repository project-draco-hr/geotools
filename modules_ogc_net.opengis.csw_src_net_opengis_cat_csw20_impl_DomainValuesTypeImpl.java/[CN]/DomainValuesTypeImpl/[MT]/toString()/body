{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (propertyName: ");
  result.append(propertyName);
  result.append(", parameterName: ");
  result.append(parameterName);
  result.append(", type: ");
  result.append(type);
  result.append(", uom: ");
  result.append(uom);
  result.append(')');
  return result.toString();
}
