{
  if (params == null) {
    return false;
  }
  Param arrayParameters[]=getParametersInfo();
  for (int i=0; i < arrayParameters.length; i++) {
    Param param=arrayParameters[i];
    Object value;
    if (!params.containsKey(param.key)) {
      if (param.required) {
        return false;
      }
 else {
        continue;
      }
    }
    try {
      value=param.lookUp(params);
    }
 catch (    IOException e) {
      return false;
    }
    if (value == null) {
      if (param.required) {
        return (false);
      }
    }
 else {
      if (!param.type.isInstance(value)) {
        return false;
      }
    }
  }
  return true;
}
