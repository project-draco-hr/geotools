{
  if (srcPts != dstPts || srcOff >= dstOff) {
    while (--numPts >= 0) {
      dstPts[dstOff++]=(float)(Math.log10(srcPts[srcOff++]) + offset);
    }
  }
 else {
    srcOff+=numPts;
    dstOff+=numPts;
    while (--numPts >= 0) {
      dstPts[--dstOff]=(float)(Math.log10(srcPts[srcOff++]) + offset);
    }
  }
}
