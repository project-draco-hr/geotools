{
  if (srcPts != dstPts || srcOff >= dstOff) {
    while (--numPts >= 0) {
      dstPts[dstOff++]=Math.log(srcPts[srcOff++]) / lnBase + offset;
    }
  }
 else {
    srcOff+=numPts;
    dstOff+=numPts;
    while (--numPts >= 0) {
      dstPts[--dstOff]=Math.log(srcPts[srcOff++]) / lnBase + offset;
    }
  }
}
