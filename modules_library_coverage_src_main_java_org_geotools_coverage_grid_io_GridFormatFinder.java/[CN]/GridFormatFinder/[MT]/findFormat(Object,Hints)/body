{
  final Set<AbstractGridFormat> formats=findFormats(o,hints);
  for (  AbstractGridFormat f : formats) {
    if ("GeoTIFF".equals(f.getName())) {
      return f;
    }
  }
  final Iterator<AbstractGridFormat> it=formats.iterator();
  if (it.hasNext()) {
    return it.next();
  }
  return new UnknownFormat();
}
