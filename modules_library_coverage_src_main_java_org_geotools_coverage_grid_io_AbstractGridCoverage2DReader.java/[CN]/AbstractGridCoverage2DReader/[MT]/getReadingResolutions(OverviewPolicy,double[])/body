{
  double[] result;
  if (numOverviews > 0) {
    int imageIdx=getOverviewImage(policy,requestedResolution);
    result=overViewResolutions[imageIdx];
  }
 else {
    result=highestRes;
  }
  double[] clone=new double[result.length];
  System.arraycopy(result,0,clone,0,result.length);
  return clone;
}
