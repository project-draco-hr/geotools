{
  if (!checkName(coverageName)) {
    throw new IllegalArgumentException("The specified coverageName " + coverageName + "is not supported");
  }
  if (dtLayout == null) {
    return numOverviews;
  }
  return dtLayout.getNumInternalOverviews() + (dtLayout.getNumExternalOverviews() > 0 ? dtLayout.getNumExternalOverviews() : 0);
}
