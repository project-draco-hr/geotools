{
  List<Class> helperClasses=InfoToolHelperLookup.getProviders();
  for (  Class c : helperClasses) {
    try {
      InfoToolHelper helper=(InfoToolHelper)c.newInstance();
      if (helper.isSupportedLayer(layer)) {
        return helper;
      }
    }
 catch (    Exception ex) {
      throw new RuntimeException(ex);
    }
  }
  return null;
}
