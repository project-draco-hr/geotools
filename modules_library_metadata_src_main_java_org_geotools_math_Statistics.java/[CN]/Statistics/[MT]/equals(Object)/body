{
  if (obj != null && getClass().equals(obj.getClass())) {
    final Statistics cast=(Statistics)obj;
    return n == cast.n && Double.doubleToLongBits(min) == Double.doubleToLongBits(cast.min) && Double.doubleToLongBits(max) == Double.doubleToLongBits(cast.max) && Double.doubleToLongBits(sum) == Double.doubleToLongBits(cast.sum) && Double.doubleToLongBits(sum2) == Double.doubleToLongBits(cast.sum2);
  }
  return false;
}
