{
  if (newAddress != address) {
    NotificationChain msgs=null;
    if (address != null)     msgs=((InternalEObject)address).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Ows20Package.CONTACT_TYPE__ADDRESS,null,msgs);
    if (newAddress != null)     msgs=((InternalEObject)newAddress).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Ows20Package.CONTACT_TYPE__ADDRESS,null,msgs);
    msgs=basicSetAddress(newAddress,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Ows20Package.CONTACT_TYPE__ADDRESS,newAddress,newAddress));
}
