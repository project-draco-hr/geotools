{
  if (crs == null) {
    return null;
  }
  final Set<ReferenceIdentifier> identifiers=crs.getIdentifiers();
  if (identifiers.isEmpty()) {
    final ReferenceIdentifier name=crs.getName();
    if (name != null) {
      return name.toString();
    }
  }
 else {
    return identifiers.iterator().next().toString();
  }
  return null;
}
