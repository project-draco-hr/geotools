  builder.append(longBarbLength);
  builder.append(", shortBarbLength=");
  builder.append(shortBarbLength);
  builder.append(", zeroWindRadius=");
  builder.append(zeroWindRadius);
  builder.append("]");
  return builder.toString();
}
