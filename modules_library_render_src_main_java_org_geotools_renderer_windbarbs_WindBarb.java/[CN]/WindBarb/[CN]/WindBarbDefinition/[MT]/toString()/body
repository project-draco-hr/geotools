{
  StringBuilder builder=new StringBuilder();
  builder.append("WindBarbDefinition [vectorLength=");
  builder.append(vectorLength);
  builder.append(", basePennantLength=");
  builder.append(basePennantLength);
  builder.append(", elementsSpacing=");
  builder.append(elementsSpacing);
