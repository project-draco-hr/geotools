{
  if (typeName.equals(sampleName)) {
    if (sample1b == null)     return getOriginalFeatures(sampleName);
 else     return getUpdatedFeatures(sampleName);
  }
 else   if (typeName.equals(testName)) {
    if (test1b == null)     return getOriginalFeatures(testName);
 else     return getUpdatedFeatures(testName);
  }
  return new ArrayList<SimpleFeature>();
}
