{
  if (FES.ValueReference.equals(name)) {
    if (e1 instanceof PropertyName) {
      return e1;
    }
 else     if (e2 instanceof PropertyName) {
      return e2;
    }
  }
  if (FES.expression.equals(name)) {
    if (e1 instanceof PropertyName) {
      return e2;
    }
 else     if (e2 instanceof PropertyName) {
      return e1;
    }
  }
  return null;
}
