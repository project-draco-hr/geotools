{
  if (!super.canProcess(params)) {
    return false;
  }
  try {
    URL url=(URL)URLP.lookUp(params);
    if (canProcess(url)) {
      return true;
    }
 else {
      Object fileType=FILE_TYPE.lookUp(params);
      File dir=DataUtilities.urlToFile(url);
      return dir.isDirectory() && (fileType == null || "shapefile".equals(fileType));
    }
  }
 catch (  IOException e) {
    return false;
  }
}
