{
  if ((firstParameter == null && secondParameter == null) || (firstParameter != null && firstParameter.equals(secondParameter)) || (secondParameter != null && secondParameter.equals(firstParameter))) {
    throw new IllegalArgumentException("The parameters '" + firstParameterName + "' and '"+ secondParameterName+ "' should not be equal, but they were '"+ firstParameter+ "' and '"+ secondParameter+ "'.");
  }
}
