{
  if (object == this) {
    return true;
  }
  if (super.equals(object,compareMetadata)) {
    final DefaultGeodeticDatum that=(DefaultGeodeticDatum)object;
    if (equals(this.ellipsoid,that.ellipsoid,compareMetadata) && equals(this.primeMeridian,that.primeMeridian,compareMetadata)) {
      return !compareMetadata || Arrays.equals(this.bursaWolf,that.bursaWolf);
    }
  }
  return false;
}
