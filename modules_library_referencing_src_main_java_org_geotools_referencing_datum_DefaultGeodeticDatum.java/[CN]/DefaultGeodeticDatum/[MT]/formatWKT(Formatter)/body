{
  formatter.append(ellipsoid);
  if (bursaWolf != null) {
    for (int i=0; i < bursaWolf.length; i++) {
      final BursaWolfParameters transformation=bursaWolf[i];
      if (isWGS84(transformation.targetDatum)) {
        formatter.append(transformation);
        break;
      }
    }
  }
  return "DATUM";
}
