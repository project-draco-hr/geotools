{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (value: ");
  result.append(value);
  result.append(", traverseXlinkDepth: ");
  result.append(traverseXlinkDepth);
  result.append(", traverseXlinkExpiry: ");
  result.append(traverseXlinkExpiry);
  result.append(')');
  return result.toString();
}
