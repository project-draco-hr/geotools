{
  if (unit.equals(newUnit)) {
    return this;
  }
  if (unit.isCompatible(newUnit)) {
    return new DefaultCoordinateSystemAxis(getProperties(this,null),abbreviation,direction,newUnit,minimum,maximum,rangeMeaning);
  }
  throw new IllegalArgumentException(Errors.format(ErrorKeys.INCOMPATIBLE_UNIT_$1,newUnit));
}
