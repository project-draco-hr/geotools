{
  if (literal instanceof Date) {
    Calendar c=Calendar.getInstance();
    c.setTime((Date)literal);
    out.write(String.format("STR_TO_DATE('%d,%d,%d,%d,%d,%d,%d'",c.get(Calendar.YEAR),c.get(Calendar.MONTH) + 1,c.get(Calendar.DAY_OF_MONTH),c.get(Calendar.HOUR_OF_DAY),c.get(Calendar.MINUTE),c.get(Calendar.SECOND),c.get(Calendar.MILLISECOND) * 1000) + ",'%Y,%m,%d,%H,%i,%s,%f')");
  }
 else {
    super.writeLiteral(literal);
  }
}
