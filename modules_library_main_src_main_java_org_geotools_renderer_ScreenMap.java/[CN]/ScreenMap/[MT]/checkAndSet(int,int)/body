{
  if ((x - minx) < 0 || (x - minx) > width - 1 || (y - miny) < 0 || (y - miny) > height - 1)   return true;
  int bit=bit(x - minx,y - miny);
  int index=bit / 32;
  int offset=bit % 32;
  int mask=1 << offset;
  try {
    if ((pixels[index] & mask) != 0) {
      return true;
    }
 else {
      pixels[index]=pixels[index] | mask;
      return false;
    }
  }
 catch (  Exception e) {
    return true;
  }
}
