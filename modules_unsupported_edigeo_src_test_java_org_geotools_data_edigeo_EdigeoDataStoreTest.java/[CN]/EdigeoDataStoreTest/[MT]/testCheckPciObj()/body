{
  assertEquals("BORNE_id should be an existing Edigeo Object","BORNE_id",EdigeoDataStore.checkPciObj("BORNE_id"));
  try {
    assertEquals("FOO_id",EdigeoDataStore.checkPciObj("FOO_id"));
  }
 catch (  Exception e) {
    assertTrue("FOO_id is not an existing Edigeo object",e instanceof IllegalArgumentException);
  }
}
