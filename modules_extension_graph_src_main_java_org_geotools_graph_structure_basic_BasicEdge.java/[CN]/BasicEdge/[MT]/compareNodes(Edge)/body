{
  if (m_nodeA.equals(other.getNodeA()) && m_nodeB.equals(other.getNodeB()))   return (EQUAL_NODE_ORIENTATION);
  if ((m_nodeA.equals(other.getNodeA()) && m_nodeB.equals(other.getNodeB())) || (m_nodeA.equals(other.getNodeB()) && m_nodeB.equals(other.getNodeA())))   return (OPPOSITE_NODE_ORIENTATION);
  return (UNEQUAL_NODE_ORIENTATION);
}
