{
  if (type != null && type.eIsProxy()) {
    InternalEObject oldType=(InternalEObject)type;
    type=(SimpleLiteral)eResolveProxy(oldType);
    if (type != oldType) {
      if (eNotificationRequired())       eNotify(new ENotificationImpl(this,Notification.RESOLVE,Csw20Package.SUMMARY_RECORD_TYPE__TYPE,oldType,type));
    }
  }
  return type;
}
