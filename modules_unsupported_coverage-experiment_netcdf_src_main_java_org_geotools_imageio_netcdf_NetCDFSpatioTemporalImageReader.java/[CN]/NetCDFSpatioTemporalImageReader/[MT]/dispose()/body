{
  directReader.dispose();
  directReader=null;
  metadataMap.clear();
  metadataMap=null;
  variableMap.clear();
  variableMap=null;
  coordinatesMap.clear();
  coordinatesMap=null;
  spatioTemporalMetadataMap.clear();
  spatioTemporalMetadataMap=null;
  try {
    dataset.close();
  }
 catch (  IOException e) {
    if (LOGGER.isLoggable(Level.FINE))     LOGGER.log(Level.FINE,"Error closing the netcdf dataset",e);
  }
}
