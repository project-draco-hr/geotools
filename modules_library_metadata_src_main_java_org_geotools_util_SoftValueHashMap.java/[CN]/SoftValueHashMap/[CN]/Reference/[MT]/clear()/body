{
  if (cleaner != null) {
    final Object value=get();
    if (value != null) {
      try {
        cleaner.clean(value);
      }
 catch (      Throwable t) {
        LOGGER.log(Level.SEVERE,"Exception occurred while cleaning soft referenced object",t);
      }
    }
  }
  super.clear();
synchronized (hash) {
    final Object old=hash.remove(key);
    if (old != this && old != null) {
      hash.put(key,old);
    }
  }
}
