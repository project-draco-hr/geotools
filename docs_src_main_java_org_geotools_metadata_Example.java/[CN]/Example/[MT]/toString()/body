{
  StringBuilder build=new StringBuilder();
  build.append("Example[");
  if (field1 != null) {
    build.append(" field1=");
    build.append(field1);
  }
  build.append(" field2=");
  build.append(field2);
  if (array != null) {
    build.append(" array=");
    build.append(Utilities.deepToString(array));
  }
  build.append("]");
  return build.toString();
}
