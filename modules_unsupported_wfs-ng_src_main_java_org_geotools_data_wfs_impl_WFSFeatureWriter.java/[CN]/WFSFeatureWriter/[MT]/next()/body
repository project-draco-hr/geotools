{
  checkClosed();
  SimpleFeatureType type=getFeatureType();
  if (hasNext()) {
    return super.next();
  }
 else {
    live=null;
    next=null;
    featureBuilder.reset();
    String id="new" + diff.nextFID;
    current=featureBuilder.buildFeature(id,new Object[type.getAttributeCount()]);
    diff.nextFID++;
    return current;
  }
}
