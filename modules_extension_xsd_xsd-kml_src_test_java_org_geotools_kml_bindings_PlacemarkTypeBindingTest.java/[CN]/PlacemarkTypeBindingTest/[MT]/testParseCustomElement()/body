{
  String xml="<kml>" + "<Schema name=\"fooelement\">" + "<SimpleField type=\"int\" name=\"quux\"></SimpleField>"+ "</Schema>"+ "<fooelement>"+ "<name>name</name>"+ "<description>description</description>"+ "<Point>"+ "<coordinates>1,2</coordinates>"+ "</Point>"+ "<ExtendedData>"+ "<SchemaData schemaUrl=\"#foo\">"+ "<SimpleData name=\"quux\">morx</SimpleData>"+ "</SchemaData>"+ "<Data name=\"foo\"><value>bar</value></Data>"+ "</ExtendedData>"+ "</fooelement></kml>";
  buildDocument(xml);
  SimpleFeature placemark=parsePlacemark();
  SimpleFeatureType featureType=placemark.getFeatureType();
  assertEquals(Integer.class,featureType.getDescriptor("quux").getType().getBinding());
  assertEquals("morx",placemark.getAttribute("quux"));
  Map<Object,Object> userData=placemark.getUserData();
  @SuppressWarnings("unchecked") Map<String,String> untypedData=(Map<String,String>)userData.get("UntypedExtendedData");
  assertEquals("bar",untypedData.get("foo"));
}
