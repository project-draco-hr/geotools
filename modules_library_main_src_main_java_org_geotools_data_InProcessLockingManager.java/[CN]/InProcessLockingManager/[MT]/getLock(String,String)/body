{
  Map locks=locks(typeName);
synchronized (locks) {
    if (locks.containsKey(featureID)) {
      Lock lock=(Lock)locks.get(featureID);
      if (lock.isExpired()) {
        locks.remove(featureID);
        return null;
      }
 else {
        return lock;
      }
    }
 else {
      return null;
    }
  }
}
