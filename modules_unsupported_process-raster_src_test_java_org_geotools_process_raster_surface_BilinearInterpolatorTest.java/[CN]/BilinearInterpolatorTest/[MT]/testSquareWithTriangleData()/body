{
  float NO_DATA=-99.0f;
  float[][] input=new float[2][2];
  input[0][0]=0;
  input[0][1]=NO_DATA;
  input[1][0]=10;
  input[1][1]=20;
  BilinearInterpolator interp=new BilinearInterpolator(input,NO_DATA);
  float[][] output=interp.interpolate(10,10,true);
  printGrid(output);
  assertTrue(isMonotonic(output,NO_DATA));
}
