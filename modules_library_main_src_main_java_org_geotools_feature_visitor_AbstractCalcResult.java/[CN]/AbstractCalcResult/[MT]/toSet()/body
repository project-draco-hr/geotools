{
  Object value=getValue();
  if (value == null) {
    return null;
  }
  if (value instanceof Set) {
    Set set=(Set)value;
    return set;
  }
  if (value.getClass().isArray()) {
    Set set=new HashSet(Arrays.asList((Object[])value));
    return set;
  }
  if (value instanceof Collection) {
    Set set=new HashSet((Collection)value);
    return set;
  }
  return null;
}
