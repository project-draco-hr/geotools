{
  if (tag == GeoTIFFTagSet.TAG_GEO_ASCII_PARAMS && this.geoKeyAsciiParams != null)   return this.geoKeyAsciiParams;
  if (tag == GeoTIFFTagSet.TAG_GEO_DOUBLE_PARAMS && this.geoKeyDoubleParams != null)   return this.geoKeyDoubleParams;
  if (tag == GeoTIFFTagSet.TAG_GEO_KEY_DIRECTORY && this.geoKeyDir != null)   return this.geoKeyDir;
  Node child=null;
  Node number=null;
  for (int i=0; i < numTiffTasEntries; i++) {
    child=tiffTagsEntries.item(i);
    number=child.getAttributes().getNamedItem(GeoTiffConstants.NUMBER_ATTRIBUTE);
    if (number != null) {
      if (tag == Integer.parseInt(number.getNodeValue()))       return (IIOMetadataNode)child;
    }
  }
  return null;
}
