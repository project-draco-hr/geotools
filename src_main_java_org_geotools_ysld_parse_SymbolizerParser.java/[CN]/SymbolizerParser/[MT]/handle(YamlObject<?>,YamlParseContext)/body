{
  YamlMap map=obj.map();
  sym.setName(map.str("name"));
  if (map.has("geometry")) {
    sym.setGeometry(Util.expression(map.str("geometry"),factory));
  }
  if (map.has("uom")) {
    sym.setUnitOfMeasure(UomOgcMapping.get(map.str("uom")).getUnit());
  }
  sym.getOptions().putAll(Util.vendorOptions(map));
}
