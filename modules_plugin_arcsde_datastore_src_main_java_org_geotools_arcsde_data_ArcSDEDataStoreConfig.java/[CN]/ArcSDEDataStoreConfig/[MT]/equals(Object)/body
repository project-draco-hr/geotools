{
  if (o == this) {
    return true;
  }
  if (!(o instanceof ArcSDEDataStoreConfig)) {
    return false;
  }
  ArcSDEDataStoreConfig config=(ArcSDEDataStoreConfig)o;
  return config.getServerName().equals(getServerName()) && config.getPortNumber().equals(getPortNumber()) && config.getUserName().equals(getUserName());
}
