{
  ShapeType t;
switch (id) {
case 0:
    t=NULL;
  break;
case 1:
t=POINT;
break;
case 11:
t=POINTZ;
break;
case 21:
t=POINTM;
break;
case 3:
t=ARC;
break;
case 13:
t=ARCZ;
break;
case 23:
t=ARCM;
break;
case 5:
t=POLYGON;
break;
case 15:
t=POLYGONZ;
break;
case 25:
t=POLYGONM;
break;
case 8:
t=MULTIPOINT;
break;
case 18:
t=MULTIPOINTZ;
break;
case 28:
t=MULTIPOINTM;
break;
default :
t=UNDEFINED;
break;
}
return t;
}
