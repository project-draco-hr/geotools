{
  StringBuffer s=new StringBuffer();
  s.append("DXFVertex [");
  s.append("bulge: ");
  s.append(b + ", ");
  s.append("x: ");
  s.append(x + ", ");
  s.append("y: ");
  s.append(y + ", ");
  s.append("c: ");
  s.append(c + ", ");
  s.append("visibility: ");
  s.append(visibility);
  s.append("]");
  return s.toString();
}
