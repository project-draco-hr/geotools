{
synchronized (queue) {
    Iterator<NodeIdentifier> it=queue.keySet().iterator();
    while (it.hasNext()) {
      NodeIdentifier node=it.next();
      if (!node.isLocked()) {
        try {
          node.writeLock();
          try {
            tree.evict(node);
            queue.remove(node);
          }
  finally {
            node.writeUnLock();
          }
        }
 catch (        Exception ex) {
          ex.printStackTrace();
          return false;
        }
        return true;
      }
    }
  }
  return false;
}
