{
  if (this == object)   return true;
  if (!(object instanceof LinearColorMapElement))   return false;
  final LinearColorMapElement that=(LinearColorMapElement)object;
  if (getEquivalenceClass() != that.getEquivalenceClass())   return false;
  if (Arrays.equals(this.getColors(),that.getColors()))   return false;
  return super.equals(that);
}
