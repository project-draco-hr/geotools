{
  if (newOperations != operations) {
    NotificationChain msgs=null;
    if (operations != null)     msgs=((InternalEObject)operations).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - WfsPackage.FEATURE_TYPE_TYPE__OPERATIONS,null,msgs);
    if (newOperations != null)     msgs=((InternalEObject)newOperations).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - WfsPackage.FEATURE_TYPE_TYPE__OPERATIONS,null,msgs);
    msgs=basicSetOperations(newOperations,msgs);
    if (msgs != null)     msgs.dispatch();
  }
