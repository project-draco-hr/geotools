{
  if (JTDS_CLOB == null) {
    LOGGER.fine("Failed to find JTDS jar");
    return null;
  }
  if (!(String.class.equals(target))) {
    LOGGER.fine("Target not a string");
    return null;
  }
  if (!JTDS_CLOB.isAssignableFrom(source)) {
    LOGGER.fine("Source not a clob");
    return null;
  }
  return converter;
}
