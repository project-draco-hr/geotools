{
  final int numImages=checkImageIndex(index);
  String name;
  final File file=asFile(input);
  if (file != null) {
    name=file.getName();
  }
 else {
    name=Vocabulary.getResources(locale).getString(VocabularyKeys.UNTITLED);
  }
  if (numImages != 1) {
    name=name + " [" + index+ ']';
  }
  return name;
}
