{
  HashMap<String,String> info=new HashMap<String,String>();
  info.put("name","ArcGrid");
  info.put("description","Arc Grid Coverage Format");
  info.put("vendor","Geotools");
  info.put("docURL","http://gdal.velocet.ca/projects/aigrid/index.html");
  info.put("version","1.0");
  mInfo=info;
  writeParameters=new ParameterGroup(new DefaultParameterDescriptorGroup(mInfo,new GeneralParameterDescriptor[]{GRASS,GEOTOOLS_WRITE_PARAMS,FORCE_CELLSIZE}));
  readParameters=new ParameterGroup(new DefaultParameterDescriptorGroup(mInfo,new GeneralParameterDescriptor[]{READ_GRIDGEOMETRY2D}));
}
