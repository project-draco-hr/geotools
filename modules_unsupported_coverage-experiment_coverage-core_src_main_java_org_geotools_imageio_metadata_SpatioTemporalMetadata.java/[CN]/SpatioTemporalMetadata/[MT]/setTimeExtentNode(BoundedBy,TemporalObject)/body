{
  if (bb == null)   throw new IllegalArgumentException("Provided BoundedBy element is null");
  if (timeExtent != null) {
    if (timeExtent instanceof Period) {
      Period p=(Period)timeExtent;
      p.getBeginning().getPosition().getDateTime().toString();
      bb.setTemporalExtent(new String[]{p.getBeginning().getPosition().getDateTime().toString(),p.getEnding().getPosition().getDateTime().toString()});
    }
 else     if (timeExtent instanceof Instant) {
      bb.setTemporalExtent(((Instant)timeExtent).getPosition().getDateTime().toString());
    }
 else     throw new IllegalArgumentException("Unhandled temporal object: " + timeExtent.getClass().getName());
  }
 else {
    throw new IllegalArgumentException("Provided TemporalObject is null");
  }
}
