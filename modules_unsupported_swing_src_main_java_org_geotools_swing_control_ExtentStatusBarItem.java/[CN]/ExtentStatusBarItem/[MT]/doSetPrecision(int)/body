{
  lock.lock();
  try {
    this.numDecimals=numDecimals;
    StringBuilder sb=new StringBuilder();
    sb.append("x=[%.").append(numDecimals).append("f, ");
    sb.append("%.").append(numDecimals).append("f] ");
    sb.append("y=[%.").append(numDecimals).append("f, ");
    sb.append("%.").append(numDecimals).append("f]");
    numFormat=sb.toString();
  }
  finally {
    lock.unlock();
  }
}
