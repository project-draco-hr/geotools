{
  if ((dxgrid == null) || (dxgrid.length == 0)) {
    final int xNumCells=gridParameters.getXNumber();
    final int yNumCells=gridParameters.getYNumber();
    final double xStep=gridParameters.getXStep();
    final double yStep=gridParameters.getYStep();
    final double xStart=gridParameters.getXStart();
    final double yStart=gridParameters.getYStart();
    final float[] warpPositions;
    warpPositions=getWarpPositions();
    dxgrid=new float[yNumCells + 1][xNumCells + 1];
    for (int i=0; i <= yNumCells; i++) {
      for (int j=0; j <= xNumCells; j++) {
        dxgrid[yNumCells - i][j]=new Double(warpPositions[(int)((i * (1 + xNumCells) * 2) + (2 * j))] - (j * xStep) - xStart).floatValue();
      }
    }
  }
  return dxgrid;
}
