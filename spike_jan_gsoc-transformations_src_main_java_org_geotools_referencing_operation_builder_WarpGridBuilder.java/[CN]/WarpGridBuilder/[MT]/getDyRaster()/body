{
  float[][] values;
  values=getDyGrid();
  WritableRaster raster=RasterFactory.createBandedRaster(DataBuffer.TYPE_FLOAT,values[0].length,values.length,1,new Point(0,0));
  for (int i=0; i < values.length; i++) {
    for (int j=0; j < values[0].length; j++) {
      raster.setSample(j,i,0,values[i][j]);
    }
  }
  return raster;
}
