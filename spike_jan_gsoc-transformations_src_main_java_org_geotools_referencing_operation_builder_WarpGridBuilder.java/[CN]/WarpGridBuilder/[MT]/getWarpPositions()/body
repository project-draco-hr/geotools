{
  if (warpPositions == null) {
    try {
      warpPositions=computeWarpGrid(gridParameters);
    }
 catch (    TransformException e) {
      throw new FactoryException(Errors.format(ErrorKeys.CANT_TRANSFORM_VALID_POINTS),e);
    }
    return warpPositions;
  }
 else {
    return warpPositions;
  }
}
