{
  this.height=height;
  try {
    this.gridParameters=GridParameters.createGridParameters(envelope,this.envelope.getSpan(0) / width,this.envelope.getSpan(1) / height,(AffineTransform2D)worldToGrid,true);
  }
 catch (  TransformException e) {
    e.printStackTrace();
  }
  this.dxgrid=null;
  this.dygrid=null;
  this.warpPositions=null;
}
