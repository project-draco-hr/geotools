{
  StringBuilder buf=new StringBuilder();
  buf.append(getClass().getName());
  buf.append("[");
  if (title != null && title.length() != 0) {
    buf.append(getTitle());
  }
  if (visible) {
    buf.append(", VISIBLE");
  }
 else {
    buf.append(", HIDDEN");
  }
  buf.append("]");
  return buf.toString();
}
