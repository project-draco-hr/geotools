{
  final String WC_MULTI="%";
  final String WC_SINGLE="_";
  final String ESCAPE="\\";
  try {
    org.opengis.filter.expression.Expression pattern=resultStack.popExpression();
    org.opengis.filter.expression.Expression expr=resultStack.popExpression();
    PropertyIsLike f=filterFactory.like(expr,pattern.toString(),WC_MULTI,WC_SINGLE,ESCAPE);
    return f;
  }
 catch (  IllegalFilterException ife) {
    throw new CQLException("Exception building LikeFilter: " + ife.getMessage(),this.cqlSource);
  }
}
