{
  start("ShadedRelief");
  if (sr.isBrightnessOnly())   element("BrightnessOnly","true");
 else   element("BrightnessOnly","false");
  if (sr.getReliefFactor() != null) {
    Literal l=(Literal)sr.getReliefFactor();
    element("ReliefFactor",l.getValue().toString());
  }
  end("ShadedRelief");
}
