{
  int size=-1;
  if (getSize) {
    size=features.size();
    if (size > -1) {
      assertEquals(expectedSize,size);
    }
  }
  size=0;
  SimpleFeatureIterator reader=features.features();
  SimpleFeature sf=null;
  try {
    while (reader.hasNext()) {
      if (sf == null) {
        sf=reader.next();
      }
 else {
        reader.next().getIdentifier();
      }
      size++;
    }
  }
  finally {
    reader.close();
  }
  assertEquals(expectedSize,size);
  return sf;
}
