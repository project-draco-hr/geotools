{
  for (Iterator it=fids.iterator(); it.hasNext(); ) {
    Object next=it.next();
    if (!(next instanceof Identifier))     throw new ClassCastException("Fids must implement Identifier, " + next.getClass() + " does not");
  }
  this.fids=fids;
  for (  Identifier identifier : this.fids) {
    ids.add(identifier.getID().toString());
  }
}
