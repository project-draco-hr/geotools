{
  if (origional == null) {
    return newList();
  }
  if (origional == Collections.EMPTY_LIST) {
    return newList();
  }
  try {
    return (List)origional.getClass().newInstance();
  }
 catch (  InstantiationException e) {
    return newList();
  }
catch (  IllegalAccessException e) {
    return newList();
  }
}
