{
  if (listener != null)   listener.started();
  if (listener != null && listener.isCanceled())   return null;
  try {
    return Collections.singleton(new ReferencedEnvelope((org.opengis.geometry.Envelope)access.gridGeometry2DMap.get(GRIB1Source.this.name).getEnvelope2D()));
  }
 catch (  Throwable e) {
    listener.exceptionOccurred(e);
    return null;
  }
 finally {
    if (listener != null)     listener.complete();
  }
}
