{
  if (listener != null)   listener.started();
  if (listener != null && listener.isCanceled())   return new TreeSet<NumberRange<Double>>();
  try {
    final TreeSet<NumberRange<Double>> retVal=new TreeSet<NumberRange<Double>>(access.verticalExtentMap.get(name));
    return retVal;
  }
 catch (  Throwable e) {
    listener.exceptionOccurred(e);
    return new TreeSet<NumberRange<Double>>();
  }
 finally {
    if (listener != null)     listener.complete();
  }
}
