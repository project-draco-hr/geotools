{
  if (listener != null)   listener.started();
  if (listener != null && listener.isCanceled())   return null;
  try {
    final MathTransform2D g2w=access.gridGeometry2DMap.get(NetCDFSource.this.name).getGridToCRS2D(PixelOrientation.CENTER);
    return g2w;
  }
 catch (  Throwable e) {
    listener.exceptionOccurred(e);
    return null;
  }
 finally {
    if (listener != null)     listener.complete();
  }
}
