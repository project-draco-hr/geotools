{
  if (isATime(classDataType)) {
    return DataType.DOUBLE;
  }
 else   if (classDataType.endsWith("Integer")) {
    return DataType.INT;
  }
 else   if (classDataType.endsWith("Double")) {
    return DataType.DOUBLE;
  }
 else   if (classDataType.endsWith("String")) {
    return DataType.STRING;
  }
  return DataType.STRING;
}
