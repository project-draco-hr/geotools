{
  if (iterator != null) {
    if (iterator instanceof Closeable) {
      try {
        ((Closeable)iterator).close();
      }
 catch (      IOException e) {
        Logger log=Logger.getLogger(collection.getClass().getPackage().getName());
        log.log(Level.FINE,e.getMessage(),e);
      }
    }
    iterator=null;
    collection=null;
  }
}
