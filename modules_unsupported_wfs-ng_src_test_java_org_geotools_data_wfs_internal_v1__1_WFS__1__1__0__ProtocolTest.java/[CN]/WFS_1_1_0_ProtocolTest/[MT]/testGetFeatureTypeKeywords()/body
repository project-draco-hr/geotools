{
  StrictWFS_1_1_Strategy strategy=createTestProtocol(GEOS_ARCHSITES_11.CAPABILITIES,new StrictWFS_1_1_Strategy());
  try {
    strategy.getFeatureTypeKeywords("nonExistentTypeName");
    fail("Expected IAE");
  }
 catch (  IllegalArgumentException e) {
    assertTrue(true);
  }
  Set<String> keywords;
  keywords=strategy.getFeatureTypeKeywords(GEOS_ARCHSITES_11.FEATURETYPENAME);
  assertNotNull(keywords);
  assertEquals(1,keywords.size());
  assertTrue(keywords.contains("archsites sfArchsites"));
}
