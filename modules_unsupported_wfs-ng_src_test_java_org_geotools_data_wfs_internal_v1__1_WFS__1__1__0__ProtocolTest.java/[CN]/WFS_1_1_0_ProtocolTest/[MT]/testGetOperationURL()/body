{
  StrictWFS_1_1_Strategy strategy=createTestProtocol(GEOS_ARCHSITES_11.CAPABILITIES,new StrictWFS_1_1_Strategy());
  URL operationURL=strategy.getOperationURL(DESCRIBE_FEATURETYPE,false);
  assertNotNull(operationURL);
  assertEquals("http://localhost:8080/geoserver/wfs?",operationURL.toExternalForm());
  assertNull(strategy.getOperationURL(DESCRIBE_FEATURETYPE,true));
}
