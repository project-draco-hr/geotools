{
  try {
    super.runBare();
  }
 catch (  ConnectException e) {
    e.printStackTrace(System.err);
  }
catch (  UnknownHostException e) {
    e.printStackTrace(System.err);
  }
catch (  NoRouteToHostException e) {
    e.printStackTrace(System.err);
  }
catch (  FileNotFoundException e) {
    e.printStackTrace(System.err);
  }
catch (  IOException e) {
    e.printStackTrace(System.err);
  }
}
