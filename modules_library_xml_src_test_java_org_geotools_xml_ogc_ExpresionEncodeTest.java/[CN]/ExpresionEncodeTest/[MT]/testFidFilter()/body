{
  FilterFactory ff=FilterFactoryFinder.createFilterFactory();
  FidFilter fif=ff.createFidFilter();
  fif.addFid("f1");
  fif.addFid("f2");
  fif.addFid("f3");
  fif.addFid("f4");
  StringWriter output=new StringWriter();
