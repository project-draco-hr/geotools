{
  try {
    String envelope=rs.getString(column);
    if (envelope != null) {
      return new WKTReader().read(envelope).getEnvelopeInternal();
    }
 else {
      return new Envelope();
    }
  }
 catch (  ParseException e) {
    throw (IOException)new IOException("Error occurred parsing the bounds WKT").initCause(e);
  }
}
