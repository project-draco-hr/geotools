{
  double distSquare=getDistanceSquare(c0,c1);
  if (distSquare > maxSpacingSquare) {
    int n=(int)Math.ceil(Math.sqrt(distSquare / maxSpacingSquare));
    double r=1.0 / n;
    double[][] result=new double[n - 1][];
    for (int i=1; i < n; ++i) {
      result[i - 1]=evaluate(c0,c1,i * r);
    }
    return result;
  }
  return null;
}
