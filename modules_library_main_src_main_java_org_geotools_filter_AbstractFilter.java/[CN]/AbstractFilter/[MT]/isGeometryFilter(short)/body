{
  return ((filterType == GEOMETRY_BBOX) || (filterType == GEOMETRY_EQUALS) || (filterType == GEOMETRY_DISJOINT)|| (filterType == GEOMETRY_TOUCHES)|| (filterType == GEOMETRY_INTERSECTS)|| (filterType == GEOMETRY_CROSSES)|| (filterType == GEOMETRY_WITHIN)|| (filterType == GEOMETRY_CONTAINS)|| (filterType == GEOMETRY_OVERLAPS)|| (filterType == GEOMETRY_DWITHIN)|| (filterType == GEOMETRY_BEYOND));
}
