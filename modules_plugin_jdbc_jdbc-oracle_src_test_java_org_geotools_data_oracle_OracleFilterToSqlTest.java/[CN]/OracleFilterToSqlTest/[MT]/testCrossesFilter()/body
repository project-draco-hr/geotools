{
  Crosses crosses=ff.crosses(ff.property("GEOM"),ff.literal(gf.createLineString(new Coordinate[]{new Coordinate(-10.0d,-10.0d),new Coordinate(10d,10d)})));
  String encoded=encoder.encodeToString(crosses);
  assertEquals("WHERE SDO_RELATE(\"GEOM\", ?, 'mask=overlapbdydisjoint querytype=WINDOW') = 'TRUE' ",encoded);
}
