{
  m_node.add(m_edge);
  assertTrue(m_node.getEdges(m_otherNode).contains(m_edge));
  m_node.add(m_edge);
  assertTrue(m_node.getEdges(m_otherNode).size() == 2);
  m_node.add(m_otherEdge);
  assertTrue(m_node.getEdges(m_otherNode).contains(m_edge));
  assertTrue(m_node.getEdges(m_otherNode).contains(m_otherEdge));
}
