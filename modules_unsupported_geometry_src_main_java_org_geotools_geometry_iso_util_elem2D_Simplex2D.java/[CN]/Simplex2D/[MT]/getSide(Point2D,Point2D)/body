{
  int flag=0;
  int n=n();
  for (int i=0; i < n; ++i) {
    if (p0.equals(point[i])) {
      flag|=(1 << i);
      break;
    }
  }
  for (int i=0; i < n; ++i) {
    if (p1.equals(point[i])) {
      flag|=(1 << i);
      break;
    }
  }
  for (int i=0; i < n; ++i) {
    if (flag == sideBits(i)) {
      return i;
    }
  }
  return -1;
}
