{
  final Set<?> validValues=descriptor.getValidValues();
  if (validValues != null && !validValues.contains(value)) {
    return false;
  }
  @SuppressWarnings("unchecked") final Comparable<Object> min=(Comparable)descriptor.getMinimumValue();
  if (min != null && min.compareTo(value) > 0) {
    return false;
  }
  @SuppressWarnings("unchecked") final Comparable<Object> max=(Comparable)descriptor.getMaximumValue();
  if (max != null && max.compareTo(value) < 0) {
    return false;
  }
  return true;
}
