{
  WKTMarkFactory wmf=new WKTMarkFactory();
  try {
    Literal exp=ff.literal(WKTMarkFactory.WKT_PREFIX + "MULTILINESTRING((0.25 0.25, 0.5 0.75, 0.75 0.25, 0.25 0.25), (0.25 0.75, 0.5 0.25, 0.75 0.75, 0.25 0.75))");
    wmf.getShape(null,exp,feature);
  }
 catch (  Exception e) {
    assertTrue(false);
    return;
  }
  assertTrue(true);
}
