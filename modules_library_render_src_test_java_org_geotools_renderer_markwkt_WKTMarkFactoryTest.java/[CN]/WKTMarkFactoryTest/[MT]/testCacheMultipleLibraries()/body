{
  WKTMarkFactoryPublic wmfp=new WKTMarkFactoryPublic();
  wmfp.setRoot(rootDir);
  try {
    Literal exp=ff.literal("wkt.properties");
    wmfp.addToCachePublic(exp.evaluate(feature,String.class));
    exp=ff.literal("wkt2.properties");
    wmfp.addToCachePublic(exp.evaluate(feature,String.class));
    assertEquals(wmfp.getFromCachePublic("wkt.properties","ls"),"LINESTRING(0.0 0.25, 0.25 0.25, 0.5 0.75, 0.75 0.25, 1.00 0.25)");
    assertEquals(wmfp.getFromCachePublic("wkt2.properties","ls2"),"LINESTRING(0.0 0.5, 0.5 0.5, 0.75 1.0, 1.0 0.5, 1.25 0.5)");
    assertTrue(true);
  }
 catch (  Exception e) {
    assertTrue(false);
  }
}
