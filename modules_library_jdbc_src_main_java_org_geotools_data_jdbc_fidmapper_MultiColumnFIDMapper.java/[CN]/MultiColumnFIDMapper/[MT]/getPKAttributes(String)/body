{
  String[] attributes=FID.split("&");
  if (attributes.length != colNames.length) {
    throw new DataSourceException("The FID is not compatible with MultiColumnFIDMapper, was expecting " + colNames.length + " URL-encoded columns and got "+ attributes.length+ " columns");
  }
  for (int i=0; i < attributes.length; i++) {
    attributes[i]=URLDecoder.decode(attributes[i],UTF8);
  }
  return attributes;
}
