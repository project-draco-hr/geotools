{
  StringBuffer sb=new StringBuffer();
  try {
    for (int i=0; i < attributes.length; i++) {
      sb.append(URLEncoder.encode(attributes[i].toString(),UTF8));
      if (i < (attributes.length - 1)) {
        sb.append("&");
      }
    }
  }
 catch (  UnsupportedEncodingException e) {
  }
  return sb.toString();
}
