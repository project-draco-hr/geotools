{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (value: ");
  result.append(value);
  result.append(", dataType: ");
  result.append(dataType);
  result.append(", uom: ");
  result.append(uom);
  result.append(')');
  return result.toString();
}
