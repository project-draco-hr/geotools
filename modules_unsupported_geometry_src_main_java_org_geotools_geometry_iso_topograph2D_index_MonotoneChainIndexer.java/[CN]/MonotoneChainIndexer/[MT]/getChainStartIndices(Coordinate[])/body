{
  int start=0;
  List startIndexList=new ArrayList();
  startIndexList.add(new Integer(start));
  do {
    int last=findChainEnd(pts,start);
    startIndexList.add(new Integer(last));
    start=last;
  }
 while (start < pts.length - 1);
  int[] startIndex=toIntArray(startIndexList);
  return startIndex;
}
