{
  StringBuffer sb=new StringBuffer();
  sb.append("GridNode ******");
  sb.append("\tMBR= " + this.getIdentifier().getShape() + "\n");
  sb.append("\t#Data= " + this.data.size() + "\n");
  for (Iterator<GridData> it=data.iterator(); it.hasNext(); ) {
    sb.append("\t\t" + it.next().getData().toString() + "\n");
  }
  return sb.toString();
}
