{
  FeatureIterator<? extends SimpleFeature> iter=collection.features();
  try {
    while (iter.hasNext()) {
      try {
        SimpleFeature feature=iter.next();
        ReferencedEnvelope bounds=ReferencedEnvelope.reference(feature.getBounds());
        index.insert(bounds,feature);
      }
 catch (      Throwable t) {
      }
    }
  }
  finally {
    iter.close();
  }
  return false;
}
