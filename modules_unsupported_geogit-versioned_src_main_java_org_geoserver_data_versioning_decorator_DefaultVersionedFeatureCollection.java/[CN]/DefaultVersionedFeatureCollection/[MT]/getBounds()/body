{
  if (bounds == null) {
    bounds=new ReferencedEnvelope();
    for (Iterator<SimpleFeature> i=contents.values().iterator(); i.hasNext(); ) {
      BoundingBox geomBounds=i.next().getBounds();
      if (!geomBounds.isEmpty()) {
        bounds.include(geomBounds);
      }
    }
  }
  return bounds;
}
