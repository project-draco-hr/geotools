{
  if (!standardDecimalFlag) {
    coordString=coordString.replace(decimalDelimeter.charAt(0),'.');
  }
switch (insideInnerFlag) {
case INSIDE_X:
    x=new Double(coordString.trim());
  break;
case INSIDE_Y:
y=new Double(coordString.trim());
break;
case INSIDE_Z:
z=new Double(coordString.trim());
break;
default :
break;
}
}
