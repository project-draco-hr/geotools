{
  int nnodes=100;
  Node[] ends=GraphTestUtil.buildCircular(builder(),nnodes);
  DijkstraShortestPathFinder pfinder=new DijkstraShortestPathFinder(builder().getGraph(),ends[0],costFunction());
  pfinder.calculate();
  Path p=pfinder.getPath(ends[1]);
  assertTrue(p.size() == 2);
  assertTrue(p.get(0) == ends[1]);
  assertTrue(p.get(1) == ends[0]);
}
