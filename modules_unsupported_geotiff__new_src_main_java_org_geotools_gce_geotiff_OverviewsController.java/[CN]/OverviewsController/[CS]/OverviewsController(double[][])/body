{
  double[] highestRes=overviewsResolution[0];
  for (int i=0; i < overviewsResolution.length; i++) {
    resolutionsLevels.add(new OverviewLevel(i == 0 ? 1 : overviewsResolution[i][0] / highestRes[0],overviewsResolution[i][0],overviewsResolution[i][1],i));
  }
  Collections.sort(resolutionsLevels);
}
