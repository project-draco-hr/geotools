{
  final int n=level.intValue();
switch (n / 100) {
default :
{
      if (n < 0 || n == Integer.MAX_VALUE)       break;
    }
case 10:
  severe(message);
break;
case 9:
warning(message);
break;
case 8:
info(message);
break;
case 7:
config(message);
break;
case 6:
case 5:
fine(message);
break;
case 4:
finer(message);
break;
case 3:
finest(message);
break;
case 2:
case 1:
case 0:
break;
}
}
