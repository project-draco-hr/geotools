{
  boolean result1=GpxUtils.isOnPrimeMeridian(0.255,0.30);
  boolean result2=GpxUtils.isOnPrimeMeridian(0.012,0.008);
  if (result1 == false) {
    fail("Method failed to recognize a longitude on the prime " + "meridian.");
  }
  if (result2 == true) {
    fail("Method recognized a longitude not on the primae meridian as" + "being on the prime meridian.");
  }
}
