{
  boolean result1=GpxUtils.isEastLongitude(52.3662);
  boolean result2=GpxUtils.isEastLongitude(-45.3662);
  if (result1 == false) {
    fail("Method failed to recognize a longitude in the " + "eastern hemisphere.");
  }
  if (result2 == true) {
    fail("Method recognized a longitude in the " + "western hemisphere as being in the eastern " + "hemisphere.");
  }
}
