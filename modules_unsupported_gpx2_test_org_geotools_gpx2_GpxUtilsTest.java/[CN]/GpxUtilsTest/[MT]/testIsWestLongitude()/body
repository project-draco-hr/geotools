{
  boolean result1=GpxUtils.isWestLongitude(52.3662);
  boolean result2=GpxUtils.isWestLongitude(-45.3662);
  if (result1 == true) {
    fail("Method recognized a longitude in the " + "eastern hemisphere as being in the western " + "hemisphere.");
  }
  if (result2 == false) {
    fail("Method failed to recognize a longitude in the " + "western hemisphere.");
  }
}
