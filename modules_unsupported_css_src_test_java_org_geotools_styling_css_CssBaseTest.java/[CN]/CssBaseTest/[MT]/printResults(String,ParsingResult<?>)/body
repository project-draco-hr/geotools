{
  Object value=result.parseTreeRoot.getValue();
  if (value != null) {
    String str=value.toString();
    int ix=str.indexOf('|');
    if (ix >= 0)     str=str.substring(ix + 2);
    System.out.println(input + " = " + str+ '\n');
  }
  if (value instanceof GraphNode) {
    System.out.println("\nAbstract Syntax Tree:\n" + GraphUtils.printTree((GraphNode)value,new ToStringFormatter(null)) + '\n');
  }
 else {
    System.out.println("\nParse Tree:\n" + ParseTreeUtils.printNodeTree(result) + '\n');
  }
}
