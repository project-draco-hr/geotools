{
  if (this == obj) {
    return true;
  }
  if (obj instanceof LinePlacementImpl) {
    LinePlacementImpl other=(LinePlacementImpl)obj;
    return Utilities.equals(perpendicularOffset,other.perpendicularOffset) && Utilities.equals(repeated,other.repeated) && Utilities.equals(generalized,other.generalized)&& Utilities.equals(aligned,other.aligned)&& Utilities.equals(initialGap,other.initialGap)&& Utilities.equals(gap,other.gap);
  }
  return false;
}
