{
  if (placement == null) {
    return null;
  }
 else   if (placement instanceof LinePlacementImpl) {
    return (LinePlacementImpl)placement;
  }
 else   if (placement instanceof LinePlacement) {
    LinePlacementImpl copy=new LinePlacementImpl((LinePlacement)placement);
    return copy;
  }
  return null;
}
