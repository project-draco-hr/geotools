{
  WKTMarkFactory wmf=new WKTMarkFactory();
  try {
    this.exp=ff.literal("wkt://CURVEPOLYGON(COMPOUNDCURVE(CIRCULARSTRING(0 0,2 0, 2 1, 2 3, 4 3),(4 3, 4 5, 1 4, 0 0)))");
    wmf.getShape(null,this.exp,this.feature);
  }
 catch (  Exception e) {
    assertTrue(false);
    return;
  }
  assertTrue(true);
}
