{
  WKTMarkFactory wmf=new WKTMarkFactory();
  try {
    this.exp=ff.literal("xxx://POLYGON((0.25 0.25, 0.5 0.75, 0.75 0.25,))");
    if (wmf.getShape(null,this.exp,this.feature) == null) {
      assertTrue(true);
      return;
    }
  }
 catch (  Exception e) {
    assertTrue(false);
    return;
  }
  assertTrue(false);
}
