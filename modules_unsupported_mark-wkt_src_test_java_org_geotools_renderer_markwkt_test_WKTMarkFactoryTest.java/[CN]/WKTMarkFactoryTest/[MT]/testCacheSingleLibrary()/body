{
  WKTMarkFactoryPublic wmfp=new WKTMarkFactoryPublic();
  wmfp.setRoot(this.rootDir);
  try {
    this.exp=ff.literal("wkt.properties");
    wmfp.addToCachePublic(this.exp.evaluate(this.feature,String.class));
    assertEquals(wmfp.getFromCachePublic("wkt.properties","ls"),"LINESTRING(0.0 0.25, 0.25 0.25, 0.5 0.75, 0.75 0.25, 1.00 0.25)");
    assertEquals(wmfp.getFromCachePublic("wkt.properties","xx"),null);
    assertTrue(true);
  }
 catch (  Exception e) {
    assertTrue(false);
  }
}
