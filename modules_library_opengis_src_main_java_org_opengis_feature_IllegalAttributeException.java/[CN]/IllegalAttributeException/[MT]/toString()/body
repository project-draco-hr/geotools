{
  String s=getClass().getName();
  String message=getLocalizedMessage();
  StringBuffer buf=new StringBuffer();
  buf.append(s);
  if (message != null) {
    buf.append(":");
    buf.append(message);
  }
  if (descriptor != null) {
    buf.append(":");
    buf.append(descriptor.getName());
  }
  buf.append(" value:");
  buf.append(value);
  return buf.toString();
}
