{
  Geometry bbox=(Geometry)expression.getValue();
  Envelope e=bbox.getEnvelopeInternal();
  double x1=e.getMinX();
  double x2=e.getMaxX();
  double y1=e.getMinY();
  double y2=e.getMaxY();
  out.write("( " + XMinColumnName + " < "+ x2+ " and "+ XMaxColumnName+ " > "+ x1+ " and "+ YMinColumnName+ " < "+ y2+ " and "+ YMaxColumnName+ " > "+ y1+ " )");
}
