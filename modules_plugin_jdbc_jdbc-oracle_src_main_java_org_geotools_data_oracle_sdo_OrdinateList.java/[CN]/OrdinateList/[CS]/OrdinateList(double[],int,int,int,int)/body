{
  START=start;
  END=end;
  ARRAY=array;
  OFFSET=offset;
  LEN=len;
  SIZE=Math.abs(START - END) / LEN;
  STEP=START < END ? LEN : -LEN;
  if (ARRAY.length % LEN != 0) {
    throw new IllegalArgumentException("You have requested Coordinates of " + LEN + " ordinates. "+ "This is inconsistent with an array of length "+ ARRAY.length);
  }
}
