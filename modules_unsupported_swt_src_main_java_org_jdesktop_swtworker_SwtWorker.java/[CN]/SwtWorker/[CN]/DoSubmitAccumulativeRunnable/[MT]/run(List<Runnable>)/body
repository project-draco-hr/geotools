{
  int i=0;
  try {
    for (    Runnable runnable : args) {
      i++;
      runnable.run();
    }
  }
  finally {
    if (i < args.size()) {
      Runnable argsTail[]=new Runnable[args.size() - i];
      for (int j=0; j < argsTail.length; j++) {
        argsTail[j]=args.get(i + j);
      }
      add(true,argsTail);
    }
  }
}
