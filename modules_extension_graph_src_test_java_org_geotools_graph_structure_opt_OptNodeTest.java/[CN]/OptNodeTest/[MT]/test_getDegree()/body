{
  assertTrue(m_node.getDegree() == 0);
  m_node.setDegree(1);
  m_node.add(m_edge);
  assertTrue(m_node.getDegree() == 1);
  m_node.setDegree(2);
  m_node.add(m_edge);
  m_node.add(m_edge);
  assertTrue(m_node.getDegree() == 2);
  m_node.setDegree(3);
  m_node.add(m_edge);
  m_node.add(m_edge);
  m_node.add(m_otherEdge);
  assertTrue(m_node.getDegree() == 3);
}
