{
  assertTrue(!m_node.getRelated().hasNext());
  m_node.setDegree(1);
  m_node.add(m_edge);
  Iterator itr=m_node.getRelated();
  assertSame(itr.next(),m_otherNode);
  assertTrue(!itr.hasNext());
  m_node.setDegree(2);
  m_node.add(m_edge);
  m_node.add(m_edge);
  itr=m_node.getRelated();
  assertSame(itr.next(),m_otherNode);
  assertSame(itr.next(),m_otherNode);
  assertTrue(!itr.hasNext());
}
