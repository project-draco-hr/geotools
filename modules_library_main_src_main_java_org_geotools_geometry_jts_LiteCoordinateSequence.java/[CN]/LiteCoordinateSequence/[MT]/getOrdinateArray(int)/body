{
  if (dimensions == this.dimension) {
    return coords;
  }
  int n=size();
  double[] result=new double[n * dimensions];
  int minDimensions=Math.min(dimensions,this.dimension);
  for (int t=0; t < n; t++) {
    for (int d=0; d < minDimensions; d++) {
      result[t * 2 + d]=getOrdinate(t,d);
    }
  }
  return result;
}
