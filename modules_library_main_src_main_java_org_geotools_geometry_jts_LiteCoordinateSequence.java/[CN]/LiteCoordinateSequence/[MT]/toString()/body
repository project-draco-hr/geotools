{
  if (size > 0) {
    StringBuffer strBuf=new StringBuffer((9 * dimension) * size);
    strBuf.append('(');
    for (int i=0; i < size; i++) {
      for (int j=0; j < dimension; j++) {
        strBuf.append(coords[i * dimension + j]);
        if (j < dimension - 1) {
          strBuf.append(" ");
        }
      }
      if (i < size - 1) {
        strBuf.append(", ");
      }
    }
    strBuf.append(')');
    return strBuf.toString();
  }
 else {
    return "()";
  }
}
