{
  StringBuilder sb=new StringBuilder("(");
  for (int i=0; i < values.length; i++) {
    Object v=values[i];
    if (v != null) {
      if (v instanceof Color) {
        sb.append(Util.serializeColor((Color)v));
      }
 else {
        sb.append(v);
      }
    }
    sb.append(",");
  }
  sb.setLength(sb.length() - 1);
  return sb.append(")").toString();
}
