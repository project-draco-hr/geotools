{
  if (this.crs == null) {
synchronized (this) {
      final AxisType axisType=coordinateAxis.getAxisType();
switch (axisType) {
case GeoZ:
case Height:
case Pressure:
        String axisName=getName();
      if (UnidataCRSUtilities.VERTICAL_AXIS_NAMES.contains(axisName)) {
        this.crs=UnidataCRSUtilities.buildVerticalCrs(coordinateAxis);
      }
    break;
}
}
}
return crs;
}
