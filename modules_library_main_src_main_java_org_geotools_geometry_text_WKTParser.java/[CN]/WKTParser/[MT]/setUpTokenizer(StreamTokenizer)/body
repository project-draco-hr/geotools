{
  final int char128=128;
  final int skip32=32;
  final int char255=255;
  tokenizer.resetSyntax();
  tokenizer.wordChars('a','z');
  tokenizer.wordChars('A','Z');
  tokenizer.wordChars(char128 + skip32,char255);
  tokenizer.wordChars('0','9');
  tokenizer.wordChars('-','-');
  tokenizer.wordChars('+','+');
  tokenizer.wordChars('.','.');
  tokenizer.whitespaceChars(0,' ');
  tokenizer.commentChar('#');
}
