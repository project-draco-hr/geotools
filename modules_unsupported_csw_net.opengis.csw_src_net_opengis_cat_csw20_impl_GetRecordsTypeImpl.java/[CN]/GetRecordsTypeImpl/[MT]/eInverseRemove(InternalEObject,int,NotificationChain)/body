{
switch (featureID) {
case Csw20Package.GET_RECORDS_TYPE__DISTRIBUTED_SEARCH:
    return basicSetDistributedSearch(null,msgs);
case Csw20Package.GET_RECORDS_TYPE__ABSTRACT_QUERY_GROUP:
