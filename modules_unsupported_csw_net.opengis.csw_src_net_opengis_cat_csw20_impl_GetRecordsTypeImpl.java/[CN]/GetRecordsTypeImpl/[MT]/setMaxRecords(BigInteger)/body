{
  BigInteger oldMaxRecords=maxRecords;
  maxRecords=newMaxRecords;
  boolean oldMaxRecordsESet=maxRecordsESet;
  maxRecordsESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Csw20Package.GET_RECORDS_TYPE__MAX_RECORDS,oldMaxRecords,maxRecords,!oldMaxRecordsESet));
}
