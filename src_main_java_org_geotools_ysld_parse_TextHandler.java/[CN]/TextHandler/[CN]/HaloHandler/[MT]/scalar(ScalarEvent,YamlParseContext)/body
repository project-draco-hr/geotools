{
  String val=evt.getValue();
  if ("fill".equals(val)) {
    context.push(new FillHandler(factory){
      @Override protected void fill(      Fill fill){
        halo.setFill(fill);
      }
    }
);
  }
 else   if ("radius".equals(val)) {
    context.push(new ExpressionHandler(factory){
      @Override protected void expression(      Expression expr){
        halo.setRadius(expr);
      }
    }
);
  }
}
