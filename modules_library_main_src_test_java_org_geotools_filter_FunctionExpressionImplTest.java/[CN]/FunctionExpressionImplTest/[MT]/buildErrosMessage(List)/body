{
  StringBuffer sb=new StringBuffer("Some function expression implementations violates contract:\n");
  int errorCount=1;
  for (Iterator it=errors.iterator(); it.hasNext(); ) {
    String error=(String)it.next();
    sb.append(errorCount++ + " - ");
    sb.append(error);
    sb.append("\n");
  }
  return sb.toString();
}
