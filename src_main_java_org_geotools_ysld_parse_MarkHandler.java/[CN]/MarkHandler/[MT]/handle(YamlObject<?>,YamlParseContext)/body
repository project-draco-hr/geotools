{
  mark(mark);
  YamlMap map=obj.map();
  if (map.has("shape")) {
    mark.setWellKnownName(Util.expression(map.str("shape"),factory));
  }
  context.push(map,new StrokeHandler(factory){
    @Override protected void stroke(    Stroke stroke){
      mark.setStroke(stroke);
    }
  }
);
  context.push(map,new FillHandler(factory){
    @Override protected void fill(    Fill fill){
      mark.setFill(fill);
    }
  }
);
}
