{
  String val=evt.getValue();
  if ("shape".equals(val)) {
    handlers.push(new ExpressionHandler(factory){
      @Override protected void expression(      Expression expr){
        mark.setWellKnownName(expr);
      }
    }
);
  }
  if ("stroke".equals(val)) {
    handlers.push(new StrokeHandler(factory){
      @Override protected void stroke(      Stroke stroke){
        mark.setStroke(stroke);
      }
    }
);
  }
  if ("fill".equals(val)) {
    handlers.push(new FillHandler(factory){
      @Override protected void fill(      Fill fill){
        mark.setFill(fill);
      }
    }
);
  }
  if ("url".equals(val)) {
  }
  if ("inline".equals(val)) {
  }
}
