{
  ListIterator<T> iter=items.listIterator();
  int index=0;
  boolean found=false;
  while (iter.hasNext()) {
    T anItem=iter.next();
    if (anItem.equals(item)) {
      iter.remove();
      found=true;
      break;
    }
    index++;
  }
  if (found && notify) {
    this.fireIntervalRemoved(this,index,index);
  }
}
