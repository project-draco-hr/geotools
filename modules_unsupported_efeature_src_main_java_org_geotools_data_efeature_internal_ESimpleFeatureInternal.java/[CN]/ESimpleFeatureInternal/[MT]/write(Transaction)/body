{
  if (!isDetached()) {
    throw new IllegalStateException("ESimpleFeature " + getType().getTypeName() + " is not detached");
  }
  List<EFeaturePropertyDelegate<?,? extends Property,? extends EStructuralFeature>> eList=eInternal.getProperties();
  List<Object> eValues=new ArrayList<Object>(eList.size());
  for (  EFeatureProperty<?,? extends Property> it : eList) {
    eValues.add(it.write(eTx));
  }
  return eValues;
}
