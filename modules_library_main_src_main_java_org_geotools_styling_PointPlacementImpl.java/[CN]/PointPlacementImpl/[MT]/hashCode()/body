{
  final int PRIME=37;
  int result=17;
  if (anchorPoint != null) {
    result=(result * PRIME) + anchorPoint.hashCode();
  }
  if (displacement != null) {
    result=(result * PRIME) + displacement.hashCode();
  }
  if (rotation != null) {
    result=(result * PRIME) + rotation.hashCode();
  }
  return result;
}
