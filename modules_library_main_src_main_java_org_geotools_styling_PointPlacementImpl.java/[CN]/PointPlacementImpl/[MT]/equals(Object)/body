{
  if (this == obj) {
    return true;
  }
  if (obj instanceof PointPlacementImpl) {
    PointPlacementImpl other=(PointPlacementImpl)obj;
    return Utilities.equals(anchorPoint,other.anchorPoint) && Utilities.equals(displacement,other.displacement) && Utilities.equals(rotation,other.rotation);
  }
  return false;
}
