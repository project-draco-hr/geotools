{
  final RasterSymbolizer symbolizer=(RasterSymbolizer)style.featureTypeStyles().get(0).rules().get(0).symbolizers().get(0);
  SubchainStyleVisitorCoverageProcessingAdapter rsh=new RasterSymbolizerHelper(coverage,null);
  rsh.visit(symbolizer);
  GridCoverage2D g=((GridCoverage2D)rsh.execute()).geophysics(false);
  return g;
}
