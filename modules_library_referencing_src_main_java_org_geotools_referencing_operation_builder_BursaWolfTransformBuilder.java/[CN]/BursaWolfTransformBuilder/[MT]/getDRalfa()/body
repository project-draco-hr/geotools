{
  GeneralMatrix dRa=new GeneralMatrix(3,3);
  double[] m0={0,0,0};
  double[] m1={0,-Math.sin(alfa),Math.cos(alfa)};
  double[] m2={0,-Math.cos(alfa),-Math.sin(alfa)};
  dRa.setRow(0,m0);
  dRa.setRow(1,m1);
  dRa.setRow(2,m2);
  dRa.mul(dRa,getRbeta());
  dRa.mul(dRa,getRgamma());
  return specialMul(dRa,x);
}
