{
  BursaWolfParameters parameters=new BursaWolfParameters(Datum);
  parameters.dx=dx;
  parameters.dy=dy;
  parameters.dz=dz;
  parameters.ex=-radiansToSeconds(alfa);
  parameters.ey=-radiansToSeconds(beta);
  parameters.ez=-radiansToSeconds(gamma);
  parameters.ppm=(q - 1) * 1000000;
  return parameters;
}
