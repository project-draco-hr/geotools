{
  if (actual == null)   return "";
  StringBuffer crush=new StringBuffer(actual);
  int ch=0;
  while (ch < crush.length()) {
    char chracter=crush.charAt(ch);
    if (Character.isWhitespace(chracter)) {
      crush.deleteCharAt(ch);
      continue;
    }
    ch++;
  }
  return crush.toString();
}
