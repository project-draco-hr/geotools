{
  if (serviceInfo == null) {
    serviceInfo=new DefaultServiceInfo();
    serviceInfo.setTitle(rasterInfo.getRasterTable());
    serviceInfo.setDescription(rasterInfo.toString());
    Set<String> keywords=new HashSet<String>();
    keywords.add("ArcSDE");
    serviceInfo.setKeywords(keywords);
  }
  return serviceInfo;
}
