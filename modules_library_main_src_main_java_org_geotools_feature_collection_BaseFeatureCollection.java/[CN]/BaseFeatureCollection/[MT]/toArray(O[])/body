{
  int size=size();
  if (a.length < size) {
    a=(O[])java.lang.reflect.Array.newInstance(a.getClass().getComponentType(),size);
  }
  SimpleFeatureIterator it=features();
  try {
    Object[] result=a;
    for (int i=0; i < size; i++)     result[i]=it.next();
    if (a.length > size)     a[size]=null;
    return a;
  }
  finally {
    it.close();
  }
}
