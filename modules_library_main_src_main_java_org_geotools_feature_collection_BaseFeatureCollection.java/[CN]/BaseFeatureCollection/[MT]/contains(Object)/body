{
  SimpleFeatureIterator e=features();
  try {
    if (o == null) {
      while (e.hasNext()) {
        if (e.next() == null) {
          return true;
        }
      }
    }
 else {
      while (e.hasNext()) {
        if (o.equals(e.next())) {
          return true;
        }
      }
    }
    return false;
  }
  finally {
    e.close();
  }
}
