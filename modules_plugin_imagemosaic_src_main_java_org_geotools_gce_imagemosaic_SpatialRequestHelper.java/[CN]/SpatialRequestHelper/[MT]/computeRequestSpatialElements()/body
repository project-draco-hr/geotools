{
  inspectCoordinateReferenceSystems();
  computeCropBBOX();
  if (empty || (cropBBox != null && cropBBox.isEmpty())) {
    if (LOGGER.isLoggable(Level.FINE)) {
      LOGGER.log(Level.FINE,"RequestedBBox empty or null");
    }
    empty=true;
    return;
  }
  computeCropRasterArea();
  if (empty || (destinationRasterArea != null && destinationRasterArea.isEmpty())) {
    if (LOGGER.isLoggable(Level.FINE)) {
      LOGGER.log(Level.FINE,"CropRasterArea empty or null");
    }
    return;
  }
  if (LOGGER.isLoggable(Level.FINER)) {
    StringBuilder sb=new StringBuilder("Adjusted Requested Envelope = ").append(requestedBBox.toString()).append("\n").append("Requested raster dimension = ").append(requestedRasterArea.toString()).append("\n").append("Corresponding raster source region = ").append(requestedRasterArea.toString());
    LOGGER.log(Level.FINER,sb.toString());
  }
  computeRequestedResolution();
}
