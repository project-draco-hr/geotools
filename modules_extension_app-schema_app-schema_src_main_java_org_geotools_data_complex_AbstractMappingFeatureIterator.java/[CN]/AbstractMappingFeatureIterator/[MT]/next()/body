{
  if (!hasNext()) {
    throw new IllegalStateException("there are no more features in this iterator");
  }
  Feature next;
  try {
    next=computeNext();
  }
 catch (  IOException e) {
    close();
    throw new RuntimeException(e);
  }
  ++featureCounter;
  setHasNextCalled(false);
  return next;
}
