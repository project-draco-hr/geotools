{
  out.print("MULTILINESTRING ( ");
  for (int j=0; j < edges.size(); j++) {
    Edge e=(Edge)edges.get(j);
    if (j > 0)     out.print(",");
    out.print("(");
    Coordinate[] pts=e.getCoordinates();
    for (int i=0; i < pts.length; i++) {
      if (i > 0)       out.print(",");
      out.print(pts[i].x + " " + pts[i].y);
    }
    out.println(")");
  }
  out.print(")  ");
}
