{
  if (this.query == null) {
    return query;
  }
 else   if (query instanceof JoiningQuery) {
    JoiningQuery jQuery=new JoiningQuery(super.joinQuery(query),((JoiningQuery)query).hasIdColumn());
    jQuery.setQueryJoins(((JoiningQuery)query).getQueryJoins());
    return jQuery;
  }
 else {
    return super.joinQuery(query);
  }
}
