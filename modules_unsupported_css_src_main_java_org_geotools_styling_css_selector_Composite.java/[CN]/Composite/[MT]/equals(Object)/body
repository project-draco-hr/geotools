{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  Composite other=(Composite)obj;
  if (children == null) {
    if (other.children != null)     return false;
  }
 else   if (!children.equals(other.children))   return false;
  return true;
}
