{
  CountDownLatch latch=latches[type.ordinal()];
  if (latch == null) {
    throw new IllegalStateException("latch not set for " + type);
  }
  boolean result=false;
  try {
    result=latch.await(timeoutMillis,TimeUnit.MILLISECONDS);
  }
 catch (  InterruptedException ex) {
  }
 finally {
    return result;
  }
}
