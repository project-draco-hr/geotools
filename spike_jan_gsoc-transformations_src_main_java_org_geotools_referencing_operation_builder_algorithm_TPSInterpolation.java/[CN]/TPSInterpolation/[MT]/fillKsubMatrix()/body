{
  double alfa=0;
  Object[] positions=getPositions().keySet().toArray();
  for (int i=0; i < number; i++) {
    for (int j=i + 1; j < number; j++) {
      double u=calculateFunctionU((DirectPosition)positions[i],(DirectPosition)positions[j]);
      L.setElement(i,j,u);
      L.setElement(j,i,u);
      alfa=alfa + (u * 2);
    }
  }
  alfa=alfa / (number * number);
}
