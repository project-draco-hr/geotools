{
  if (index < 0 || index >= childCount()) {
    throw new IndexOutOfBoundsException(Errors.format(outOfBounds(),index));
  }
  while (childs.size() <= index) {
    childs.add(null);
  }
  T candidate=childs.get(index);
  if (candidate == null) {
    candidate=newChild(index);
    childs.set(index,candidate);
  }
  return candidate;
}
