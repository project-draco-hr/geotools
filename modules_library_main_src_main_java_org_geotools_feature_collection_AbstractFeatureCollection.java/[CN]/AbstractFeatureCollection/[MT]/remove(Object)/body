{
  Iterator<SimpleFeature> e=iterator();
  try {
    if (o == null) {
      while (e.hasNext()) {
        if (e.next() == null) {
          e.remove();
          return true;
        }
      }
    }
 else {
      while (e.hasNext()) {
        if (o.equals(e.next())) {
          e.remove();
          return true;
        }
      }
    }
    return false;
  }
  finally {
    close(e);
  }
}
