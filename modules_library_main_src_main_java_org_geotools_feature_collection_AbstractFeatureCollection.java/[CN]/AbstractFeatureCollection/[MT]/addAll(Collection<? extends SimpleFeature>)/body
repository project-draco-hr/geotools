{
  boolean modified=false;
  Iterator<? extends SimpleFeature> e=c.iterator();
  try {
    while (e.hasNext()) {
      if (add(e.next()))       modified=true;
    }
  }
  finally {
    if (c instanceof FeatureCollection) {
      FeatureCollection other=(FeatureCollection)c;
      other.close(e);
    }
  }
  return modified;
}
