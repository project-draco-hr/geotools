{
  boolean modified=false;
  Iterator e=iterator();
  try {
    while (e.hasNext()) {
      if (c.contains(e.next())) {
        e.remove();
        modified=true;
      }
    }
    return modified;
  }
  finally {
    if (c instanceof FeatureCollection) {
      FeatureCollection other=(FeatureCollection)c;
      other.close(e);
    }
  }
}
