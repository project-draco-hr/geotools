{
  this(new DXFPoint(newTrace._p1._point.x,newTrace._p1._point.y,newTrace.getColor(),null,0,newTrace.getThickness()),new DXFPoint(newTrace._p2._point.x,newTrace._p2._point.y,newTrace.getColor(),null,0,newTrace.getThickness()),new DXFPoint(newTrace._p3._point.x,newTrace._p3._point.y,newTrace.getColor(),null,0,newTrace.getThickness()),new DXFPoint(newTrace._p4._point.x,newTrace._p4._point.y,newTrace.getColor(),null,0,newTrace.getThickness()),newTrace.getThickness(),newTrace.getColor(),newTrace.getRefLayer(),0,newTrace.getLineType());
  setType(newTrace.getType());
  setStartingLineNumber(newTrace.getStartingLineNumber());
  setUnivers(newTrace.getUnivers());
}
