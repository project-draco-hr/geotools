{
  if (ORA_TIMESTAMP.isInstance(source)) {
    if (java.sql.Date.class.isAssignableFrom(target))     return (T)ORA_TS_DVALUE.invoke(source);
 else     return (T)ORA_TS_TSVALUE.invoke(source);
  }
 else {
    if (java.sql.Date.class.isAssignableFrom(target))     return (T)ORA_DATE_DVALUE.invoke(source);
 else     return (T)ORA_DATE_TSVALUE.invoke(source);
  }
}
