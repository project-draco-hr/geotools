{
  List layers=new ArrayList();
  Layer layer1=new Layer();
  TreeSet set1=new TreeSet();
  set1.add("EPSG:4326");
  set1.add("EPSG:3005");
  set1.add("EPSG:42101");
  layer1.setSrs(set1);
  Layer layer2=new Layer();
  TreeSet set2=new TreeSet();
  set2.add("EPSG:3005");
  set2.add("EPSG:42101");
  layer2.setSrs(set2);
  Layer layer3=new Layer();
  TreeSet set3=new TreeSet();
  set3.add("EPSG:42111");
  layer3.setSrs(set3);
  layers.add(layer1);
  layers.add(layer2);
  Set results1=WMSUtils.findCommonEPSGs(layers);
  assertNotNull(results1);
  assertEquals(2,results1.size());
  assertTrue(results1.contains("EPSG:3005"));
  assertTrue(results1.contains("EPSG:42101"));
  assertFalse(results1.contains("EPSG:4326"));
  layers.clear();
  layers.add(layer1);
  layers.add(layer3);
  Set results2=WMSUtils.findCommonEPSGs(layers);
  assertNotNull(results2);
  assertEquals(0,results2.size());
  assertTrue(results2.isEmpty());
}
