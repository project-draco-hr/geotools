{
  int s1a=UBYTE_MAX_VALUE - sa;
  int d1a=UBYTE_MAX_VALUE - da;
  int sada=sa * da;
  result[RED]=(((2 * sr < sa) ? 2 * sr * dr + sr * d1a + dr * s1a : sada - 2 * (da - dr) * (sa - sr) + sr * d1a + dr * s1a + UBYTE_MAX_VALUE) >> SHIFT8);
  result[GREEN]=(((2 * sg < sa) ? 2 * sg * dg + sg * d1a + dg * s1a : sada - 2 * (da - dg) * (sa - sg) + sg * d1a + dg * s1a + UBYTE_MAX_VALUE) >> SHIFT8);
  result[BLUE]=(((2 * sb < sa) ? 2 * sb * db + sb * d1a + db * s1a : sada - 2 * (da - db) * (sa - sb) + sb * d1a + db * s1a + UBYTE_MAX_VALUE) >> SHIFT8);
  result[ALPHA]=(sa + da - ((sa * da + UBYTE_MAX_VALUE) >> SHIFT8));
}
