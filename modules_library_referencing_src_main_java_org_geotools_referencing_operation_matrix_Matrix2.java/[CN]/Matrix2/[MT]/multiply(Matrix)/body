{
  final Matrix2 k;
  if (matrix instanceof Matrix2) {
    k=(Matrix2)matrix;
  }
 else {
    k=new Matrix2(matrix);
  }
  double m0, m1;
  m0=m00;
  m1=m01;
  m00=m0 * k.m00 + m1 * k.m10;
  m01=m0 * k.m01 + m1 * k.m11;
  m0=m10;
  m1=m11;
  m10=m0 * k.m00 + m1 * k.m10;
  m11=m0 * k.m01 + m1 * k.m11;
}
