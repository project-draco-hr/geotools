{
  File file=null;
  if (source instanceof URL) {
    file=DataUtilities.urlToFile((URL)source);
  }
 else   if (source instanceof File) {
    file=(File)source;
  }
  if (file != null) {
    if (file.isDirectory()) {
      return false;
    }
    if (file.getAbsolutePath().endsWith("nc")) {
      return true;
    }
  }
  return false;
}
