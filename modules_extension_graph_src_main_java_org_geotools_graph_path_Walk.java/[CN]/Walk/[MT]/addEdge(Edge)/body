{
  List edges=getEdges();
  if (isEmpty()) {
    add(e.getNodeA());
    add(e.getNodeB());
  }
 else {
    Node last=getLast();
    if (last.equals(e.getNodeA())) {
      add(e.getNodeB());
    }
 else     if (last.equals(e.getNodeB())) {
      add(e.getNodeA());
    }
 else     return (false);
  }
  edges.add(e);
  m_edges=edges;
  return (true);
}
