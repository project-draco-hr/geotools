{
  ArrayList<SimpleFeature> array=new ArrayList<SimpleFeature>();
  FeatureIterator<SimpleFeature> e=null;
  try {
    e=features();
    while (e.hasNext()) {
      array.add(e.next());
    }
    return array.toArray(new SimpleFeature[array.size()]);
  }
  finally {
    if (e != null) {
      e.close();
    }
  }
}
