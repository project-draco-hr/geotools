{
  super.setUp();
  x=createElement(GML.NAMESPACE,"X",XS.DECIMAL,"12.34");
  y=createElement(GML.NAMESPACE,"Y",XS.DECIMAL,"56.78");
  z=createElement(GML.NAMESPACE,"Z",XS.DECIMAL,"910.11");
