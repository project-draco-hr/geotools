{
  StringBuilder sb=new StringBuilder();
  for (int i=0; i < camelCase.length(); i++) {
    char ch=camelCase.charAt(i);
    if (Character.isUpperCase(ch)) {
      ch=Character.toLowerCase(ch);
      if (sb.length() > 0) {
        sb.append("-");
      }
    }
    sb.append(ch);
  }
  return sb.toString();
}
