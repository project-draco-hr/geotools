{
  P/=10.0;
  final double SR=Math.sqrt(S);
  return (polynome(T,HEAT_CC) + (polynome(T,HEAT_BB) * SR + polynome(T,HEAT_AA)) * S + (((polynome(T,HEAT_C) * P + polynome(T,HEAT_B)) * P + polynome(T,HEAT_A)) * P) + ((((HEAT_J * SR + polynome(T,HEAT_H)) * S * P + (HEAT_G * SR + polynome(T,HEAT_F)) * S) * P + (polynome(T,HEAT_E) * SR + polynome(T,HEAT_D)) * S) * P));
}
