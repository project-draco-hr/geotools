{
  operationMap=new HashMap();
  OperationHandler noOpHandler=new NoOp();
  operationMap.put("contains",new ContainsOp());
  operationMap.put("convexhull",new ConvexHullOp());
  operationMap.put("difference",new DifferenceOp());
