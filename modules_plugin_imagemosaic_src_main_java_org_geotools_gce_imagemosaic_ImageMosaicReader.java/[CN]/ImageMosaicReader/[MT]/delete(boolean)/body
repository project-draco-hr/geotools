{
  String[] coverageNames=getGridCoverageNames();
  for (  String coverageName : coverageNames) {
    removeCoverage(coverageName,deleteData,true);
  }
  dispose();
  if (deleteData) {
    final File[] list=parentDirectory.listFiles();
    for (    File file : list) {
      FileUtils.deleteQuietly(file);
    }
  }
 else {
    finalizeCleanup();
  }
}
