{
  if (eContext == null) {
    throw new NullPointerException("EFeatureContext can not be 'null'");
  }
  EditingDomain eDomain=null;
  eDomain=(eContext != null ? eContext.eGetDomain(eDomainID) : null);
  if (eDomain != null) {
    return eDomain;
  }
  throw new IllegalArgumentException("No EditingDomain cached for " + "eContextID: " + eContext.eContextID() + ", "+ "eDomainID: "+ eDomainID);
}
