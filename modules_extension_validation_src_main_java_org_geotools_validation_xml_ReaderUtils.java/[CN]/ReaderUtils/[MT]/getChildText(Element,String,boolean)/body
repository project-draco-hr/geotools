{
  Element elem=getChildElement(root,childName,mandatory);
  if (elem != null) {
    return getElementText(elem,mandatory);
  }
 else {
    if (mandatory) {
      String msg="Mandatory child " + childName + "not found in "+ " element: "+ root;
      throw new SAXException(msg);
    }
    return null;
  }
}
