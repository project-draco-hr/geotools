{
  if (feature instanceof SimpleFeature && !validate((SimpleFeature)feature))   return false;
  Geometry left;
  Geometry right;
switch (literals) {
case BOTH:
    return cacheValue;
case RIGHT:
{
    left=getLeftGeometry(feature);
    right=rightPreppedGeom.getGeometry();
    return basicEvaluate(left,right);
  }
case LEFT:
{
  return leftPreppedGeom.contains(getRightGeometry(feature));
}
default :
{
left=getLeftGeometry(feature);
right=getRightGeometry(feature);
return basicEvaluate(left,right);
}
}
}
