{
  attributeExtractor.clear();
  width.accept(attributeExtractor,null);
  if (attributeExtractor.isConstantExpression()) {
    Double result=width.evaluate(null,Double.class);
    if (result != null) {
      int size=(int)Math.ceil(result);
      if (size > buffer) {
        buffer=size;
      }
    }
 else {
      estimateAccurate=false;
    }
  }
 else {
    estimateAccurate=false;
  }
}
