{
  this(new DXFPoint(newEllipse._centre._point.x,newEllipse._centre._point.y,newEllipse.getColor(),null,0,(double)newEllipse.getThickness()),new DXFPoint(newEllipse._point._point.x,newEllipse._point._point.y,newEllipse.getColor(),null,0,(double)newEllipse.getThickness()),newEllipse._ratio,newEllipse._start,newEllipse._end,newEllipse.getColor(),newEllipse.getRefLayer(),0,newEllipse.getLineType());
  setType(newEllipse.getType());
  setStartingLineNumber(newEllipse.getStartingLineNumber());
  setUnivers(newEllipse.getUnivers());
}
