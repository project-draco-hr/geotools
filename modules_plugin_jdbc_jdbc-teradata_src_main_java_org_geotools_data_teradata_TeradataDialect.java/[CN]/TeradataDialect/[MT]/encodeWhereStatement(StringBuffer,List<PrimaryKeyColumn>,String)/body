{
  buf.append(" WHERE ");
  for (int i=0; i < ids.size(); i++) {
    encodeColumnName(ids.get(i).getName(),buf);
    buf.append('=');
    buf.append(table).append('.');
    encodeColumnName(ids.get(i).getName(),buf);
    if (i + 1 < ids.size()) {
      buf.append(" AND ");
    }
  }
  buf.append(" ");
}
