{
  SortOrderType oldSortOrder=sortOrder;
  boolean oldSortOrderESet=sortOrderESet;
  sortOrder=SORT_ORDER_EDEFAULT;
  sortOrderESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,Fes20Package.SORT_PROPERTY_TYPE__SORT_ORDER,oldSortOrder,SORT_ORDER_EDEFAULT,oldSortOrderESet));
}
