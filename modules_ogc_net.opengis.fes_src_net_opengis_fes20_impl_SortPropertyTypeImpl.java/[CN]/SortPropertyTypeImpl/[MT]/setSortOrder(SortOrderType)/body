{
  SortOrderType oldSortOrder=sortOrder;
  sortOrder=newSortOrder == null ? SORT_ORDER_EDEFAULT : newSortOrder;
  boolean oldSortOrderESet=sortOrderESet;
  sortOrderESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Fes20Package.SORT_PROPERTY_TYPE__SORT_ORDER,oldSortOrder,sortOrder,!oldSortOrderESet));
}
