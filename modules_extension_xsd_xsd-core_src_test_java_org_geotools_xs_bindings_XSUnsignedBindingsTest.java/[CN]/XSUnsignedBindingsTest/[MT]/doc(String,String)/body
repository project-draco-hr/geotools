{
  File sampleXsd=File.createTempFile("sample","xsd",new File("target"));
  copy(TestSchema.class.getResourceAsStream("sample.xsd"),sampleXsd);
  return String.format("<my:%s xmlns:my='http://localhost/xob/test' " + "schemaLocation='http://localhost/xob/test %s'>%s</my:%s>",elName,DataUtilities.fileToURL(sampleXsd),elValue,elName);
}
