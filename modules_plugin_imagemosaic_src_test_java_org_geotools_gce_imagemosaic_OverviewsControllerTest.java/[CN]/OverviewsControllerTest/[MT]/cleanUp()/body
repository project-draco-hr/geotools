{
  File dir=TestData.file(this,"heterogeneous/");
  File[] files=dir.listFiles((FilenameFilter)FileFilterUtils.notFileFilter(FileFilterUtils.orFileFilter(FileFilterUtils.orFileFilter(FileFilterUtils.suffixFileFilter("tif"),FileFilterUtils.suffixFileFilter("aux")),FileFilterUtils.nameFileFilter("datastore.properties"))));
  for (  File file : files) {
    file.delete();
  }
}
