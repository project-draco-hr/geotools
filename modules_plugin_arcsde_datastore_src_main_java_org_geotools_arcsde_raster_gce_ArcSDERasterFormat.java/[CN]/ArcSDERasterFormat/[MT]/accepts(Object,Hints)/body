{
  StringBuffer url;
  if (input instanceof File) {
    url=new StringBuffer(((File)input).getPath());
  }
 else   if (input instanceof String) {
    url=new StringBuffer((String)input);
  }
 else {
    return false;
  }
  try {
    sdeURLToConnectionConfig(url);
    return true;
  }
 catch (  Exception e) {
    return false;
  }
}
