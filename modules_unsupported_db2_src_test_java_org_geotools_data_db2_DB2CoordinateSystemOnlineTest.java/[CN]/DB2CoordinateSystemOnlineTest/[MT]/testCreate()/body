{
  try {
    DB2CoordinateSystem cs=new DB2CoordinateSystem(this.conn,1);
    assertEquals("GCS_NORTH_AMERICAN_1983=EPSG:4269",cs.toString());
  }
 catch (  SQLException e) {
    fail("DB2CoordinateSystem constructor failed");
  }
  int invalidSrid=-1;
  try {
    @SuppressWarnings("unused") DB2CoordinateSystem cs=new DB2CoordinateSystem(conn,invalidSrid);
    fail("DB2CoordinateSystem constructor sucessful for invalid srid " + invalidSrid);
  }
 catch (  SQLException e) {
  }
}
