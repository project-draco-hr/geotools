{
  GridCoverage gridCoverage=null;
  if (isEmptyRequest)   gridCoverage=null;
 else {
    gridCoverage=Utilities.createCoverage(imageReaderSpi,input,imageIndex,imageReadParam,useJAI,useMultithreading,needTransformation,sampleDimensions,coverageName,coverageFactory,raster2Model,coordinateReferenceSystem,envelope2D);
  }
  return gridCoverage;
}
