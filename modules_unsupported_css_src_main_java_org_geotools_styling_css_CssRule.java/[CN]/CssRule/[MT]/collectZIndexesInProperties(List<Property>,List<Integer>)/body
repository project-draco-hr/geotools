{
  if (zIndexes.size() > 0) {
    zIndexes.clear();
  }
  for (  Property property : properties) {
    if (isZIndex(property)) {
      if (zIndexes.size() > 0) {
        zIndexes.clear();
      }
      List<Value> values=property.getValues();
      for (      Value value : values) {
        if (value instanceof Value.Literal) {
          String body=((Value.Literal)value).body;
          Integer zIndex=Converters.convert(body,Integer.class);
          if (zIndex == null) {
            throw new IllegalArgumentException("Invalid value for z-index, it should be an integer: " + body);
          }
 else {
            zIndexes.add(zIndex);
          }
        }
 else {
          throw new IllegalArgumentException("z-index must be integer literals, they cannot be expressions, multi-values or any other type: " + value);
        }
      }
    }
  }
  if (zIndexes.isEmpty()) {
    zIndexes.add(null);
  }
}
