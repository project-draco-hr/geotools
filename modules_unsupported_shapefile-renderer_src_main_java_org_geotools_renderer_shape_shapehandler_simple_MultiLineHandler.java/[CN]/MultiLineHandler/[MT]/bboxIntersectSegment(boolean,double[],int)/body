{
  if (intersection) {
    return true;
  }
  if (bbox.contains(coords[index - 2],coords[index - 1])) {
    return true;
  }
  if (index < 4) {
    return false;
  }
  return intersect(coords[index - 4],coords[index - 3],coords[index - 2],coords[index - 1],bbox.getMinX(),bbox.getMinY(),bbox.getMaxX(),bbox.getMaxY());
}
