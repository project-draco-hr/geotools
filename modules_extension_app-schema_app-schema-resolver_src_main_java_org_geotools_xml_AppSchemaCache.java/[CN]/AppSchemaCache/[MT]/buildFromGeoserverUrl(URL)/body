{
  if (!geoserverSupportEnabled) {
    return null;
  }
  File file=DataUtilities.urlToFile(url);
  while (true) {
    if (file == null) {
      return null;
    }
    if (isGeoserverDataDirectory(file)) {
      return new AppSchemaCache(new File(file,GEOSERVER_CACHE_DIRECTORY_NAME),true);
    }
    file=file.getParentFile();
  }
}
