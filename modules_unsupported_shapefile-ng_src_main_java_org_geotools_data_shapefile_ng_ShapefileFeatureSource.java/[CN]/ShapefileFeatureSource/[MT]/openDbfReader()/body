{
  ShapefileDataStore ds=getDataStore();
  if (shpFiles.get(ShpFileType.DBF) == null) {
    return null;
  }
  if (shpFiles.isLocal() && !shpFiles.exists(DBF)) {
    return null;
  }
  try {
    return new DbaseFileReader(shpFiles,ds.isMemoryMapped(),ds.getCharset(),ds.getTimeZone());
  }
 catch (  IOException e) {
    return null;
  }
}
