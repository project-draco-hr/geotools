{
  run("CREATE TABLE gt_pk_metadata ( " + "table_schema VARCHAR(32), " + "table_name VARCHAR(32) NOT NULL, "+ "pk_column VARCHAR(32) NOT NULL, "+ "pk_column_idx INTEGER, "+ "pk_policy VARCHAR(32), "+ "pk_sequence VARCHAR(64),"+ "unique (table_name, pk_column),"+ "check (pk_policy in ('sequence', 'assigned', 'autoincrement')))");
}
