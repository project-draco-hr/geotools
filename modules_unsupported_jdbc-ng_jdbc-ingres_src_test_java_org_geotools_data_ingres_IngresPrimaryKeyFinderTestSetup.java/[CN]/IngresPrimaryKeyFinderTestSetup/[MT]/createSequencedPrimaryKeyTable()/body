{
  run("CREATE TABLE \"seqtable\" ( \"key\" integer PRIMARY KEY, " + "\"name\" VARCHAR(256), \"geom\" GEOMETRY)");
  run("CREATE SEQUENCE pksequence START WITH 1");
  run("INSERT INTO \"seqtable\" (\"key\", \"name\",\"geom\" ) VALUES (" + "NEXT VALUE FOR pksequence,'one',NULL)");
  run("INSERT INTO \"seqtable\" (\"key\", \"name\",\"geom\" ) VALUES (" + "NEXT VALUE FOR pksequence,'two',NULL)");
  run("INSERT INTO \"seqtable\" (\"key\", \"name\",\"geom\" ) VALUES (" + "NEXT VALUE FOR pksequence,'three',NULL)");
  run("INSERT INTO gt_pk_metadata VALUES" + "(NULL, 'seqtable', 'key', 0, 'sequence', 'PKSEQUENCE')");
}
