{
  if (TestData.isInteractiveTest()) {
    ImageIOUtilities.visualize(((GridCoverage2D)rsh.getOutput()).getRenderedImage(),rsh.getName().toString());
  }
 else {
    PlanarImage.wrapRenderedImage(((GridCoverage2D)rsh.getOutput()).getRenderedImage()).getTiles();
    rsh.dispose(new Random().nextBoolean() ? true : false);
  }
}
