{
  String itemXpath=xmlResponse.getItemXpath();
  int position=xpathString.indexOf(itemXpath);
  if (position != 0) {
    throw new RuntimeException("xpath passed in does not begin with itemXpath" + "/n xpathString =" + xpathString + "/n itemXpath ="+ itemXpath);
  }
  StringBuffer sb=new StringBuffer(itemXpath).append("[" + xmlResponse.getCount() + "]").append(xpathString.substring(itemXpath.length()));
  return sb.toString();
}
