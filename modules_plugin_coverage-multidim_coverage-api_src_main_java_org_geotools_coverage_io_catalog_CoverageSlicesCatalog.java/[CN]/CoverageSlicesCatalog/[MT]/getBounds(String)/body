{
  final Lock lock=rwLock.readLock();
  try {
    lock.lock();
    checkStore();
    if (bounds == null) {
      bounds=this.slicesIndexStore.getFeatureSource(typeName).getBounds();
    }
  }
 catch (  IOException e) {
    LOGGER.log(Level.FINER,e.getMessage(),e);
    bounds=null;
  }
 finally {
    lock.unlock();
  }
  return bounds;
}
