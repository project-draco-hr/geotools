{
  super.A=new GeneralMatrix(2 * getSourcePoints().length,4);
  int numRow=getSourcePoints().length * 2;
  for (int j=0; j < (numRow / 2); j++) {
    A.setRow(j,new double[]{getSourcePoints()[j].getCoordinate()[0],-getSourcePoints()[j].getCoordinate()[1],1,0});
  }
  for (int j=numRow / 2; j < numRow; j++) {
    A.setRow(j,new double[]{getSourcePoints()[j - (numRow / 2)].getCoordinate()[1],getSourcePoints()[j - (numRow / 2)].getCoordinate()[0],0,1});
  }
}
