{
  double[] c0=getCoordinate(0);
  for (int i=1, n=length(); i < n; i++) {
    double[] c1=getCoordinate(i);
    double[][] newCoords=AlgoPointND.split(c0,c1,maxSpacing);
    if (newCoords != null) {
      for (int j=0; i < newCoords.length; j++, i++, n++) {
        this.set(i,newCoords[j]);
      }
    }
    c0=c1;
  }
}
