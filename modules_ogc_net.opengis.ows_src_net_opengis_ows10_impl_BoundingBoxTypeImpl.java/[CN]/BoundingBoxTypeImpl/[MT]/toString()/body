{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (lowerCorner: ");
  result.append(lowerCorner);
  result.append(", upperCorner: ");
  result.append(upperCorner);
  result.append(", crs: ");
  result.append(crs);
  result.append(", dimensions: ");
  result.append(dimensions);
  result.append(')');
  return result.toString();
}
