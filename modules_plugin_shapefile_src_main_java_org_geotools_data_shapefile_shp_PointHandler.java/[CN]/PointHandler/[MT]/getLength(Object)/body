{
  int length;
  if (shapeType == ShapeType.POINT) {
    length=20;
  }
 else   if (shapeType == ShapeType.POINTM) {
    length=28;
  }
 else   if (shapeType == ShapeType.POINTZ) {
    length=36;
  }
 else {
    throw new IllegalStateException("Expected ShapeType of Point, got" + shapeType);
  }
  return length;
}
