{
  if (!fetched) {
    return builder.length() > 0;
  }
  builder.setLength(0);
  String line=null;
  while ((line=reader.readLine()) != null) {
    line=line.trim();
    if (!"".equals(line))     builder.append(line).append("\n");
    if (line.endsWith(";")) {
      fetched=false;
      break;
    }
  }
  if (line == null && builder.length() > 0) {
    throw new IOException("The file ends with a non ; terminated command");
  }
  return line != null;
}
