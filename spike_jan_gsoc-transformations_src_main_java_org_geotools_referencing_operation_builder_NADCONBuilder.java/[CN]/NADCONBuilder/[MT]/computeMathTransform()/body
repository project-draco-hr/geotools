{
  try {
    String tmpdir=System.getProperty("java.io.tmpdir");
    NADCONTransform trans=new NADCONTransform(this.writeDeltaFile(1,tmpdir + "/nadcon.laa").getAbsolutePath(),this.writeDeltaFile(0,tmpdir + "/nadcon.loa").getAbsolutePath());
    return trans;
  }
 catch (  ParameterNotFoundException e) {
    throw new FactoryException(e);
  }
catch (  IOException e) {
    throw new FactoryException(e);
  }
}
