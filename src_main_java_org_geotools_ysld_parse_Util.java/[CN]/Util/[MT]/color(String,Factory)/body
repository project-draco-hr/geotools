{
  Color color=null;
  Matcher m=HEX_PATTERN.matcher(value);
  if (m.matches()) {
    color=parseColorAsHex(m);
  }
  if (color == null) {
    m=RGB_PATTERN.matcher(value);
    if (m.matches()) {
      color=parseColorAsRGB(m);
    }
  }
  return color != null ? factory.filter.literal(color) : expression(value,factory);
}
