{
  if (listener == null) {
    listener=new NullProgressListener();
  }
  try {
    listener.started();
    if (coverageInfo.containsKey(name)) {
      return coverageInfo.get(name);
    }
    listener.exceptionOccurred(new IOException("Unknown coverage " + name));
    return null;
  }
  finally {
    listener.complete();
  }
}
