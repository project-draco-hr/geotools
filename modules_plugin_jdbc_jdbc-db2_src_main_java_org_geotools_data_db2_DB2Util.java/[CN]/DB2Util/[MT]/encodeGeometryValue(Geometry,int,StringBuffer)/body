{
  if (value == null) {
    sql.append("null");
    return;
  }
  String pattern=PARAMETER_LITERALS.get(value.getClass());
  if (pattern == null) {
    sql.append("null");
    return;
  }
  sql.append(MessageFormat.format(pattern,new Object[]{new WKTWriter().write(value),Integer.toString(srid)}));
}
