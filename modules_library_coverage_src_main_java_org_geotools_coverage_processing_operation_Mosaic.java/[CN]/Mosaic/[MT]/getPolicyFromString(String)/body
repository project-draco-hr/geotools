{
  if (policyString.equalsIgnoreCase(FIRST.name)) {
    return FIRST;
  }
 else   if (policyString.equalsIgnoreCase(FINE.name)) {
    return FINE;
  }
 else   if (policyString.equalsIgnoreCase(COARSE.name)) {
    return COARSE;
  }
 else   if (policyString.equalsIgnoreCase(EXTERNAL.name)) {
    return EXTERNAL;
  }
  return null;
}
