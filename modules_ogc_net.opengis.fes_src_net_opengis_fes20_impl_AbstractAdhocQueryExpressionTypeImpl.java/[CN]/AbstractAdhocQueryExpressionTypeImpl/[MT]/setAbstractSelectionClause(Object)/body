{
  Object oldAbstractSelectionClause=abstractSelectionClause;
  abstractSelectionClause=newAbstractSelectionClause;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Fes20Package.ABSTRACT_ADHOC_QUERY_EXPRESSION_TYPE__ABSTRACT_SELECTION_CLAUSE,oldAbstractSelectionClause,abstractSelectionClause));
}
