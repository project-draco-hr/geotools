{
  final String key=toKey(code);
  TemporalDatum datum=(TemporalDatum)cache.get(key);
  if (datum == null) {
    try {
      cache.writeLock(key);
      datum=(TemporalDatum)cache.peek(key);
      if (datum == null) {
        datum=datumAuthority.createTemporalDatum(code);
        cache.put(key,datum);
      }
    }
  finally {
      cache.writeUnLock(key);
    }
  }
  return datum;
}
