{
  final String key=toKey(code);
  TimeCS cs=(TimeCS)cache.get(key);
  if (cs == null) {
    try {
      cache.writeLock(key);
      cs=(TimeCS)cache.peek(key);
      if (cs == null) {
        cs=csAuthority.createTimeCS(code);
        cache.put(key,cs);
      }
    }
  finally {
      cache.writeUnLock(key);
    }
  }
  return cs;
}
