{
  token.setLength(0);
  while (pos < url.length()) {
    char ch=url.charAt(pos++);
    if (ch == ':' || ch == ';') {
      break;
    }
    if (ch == '/') {
      if (pos < url.length() && url.charAt(pos) == '/') {
        pos++;
      }
      break;
    }
    token.append(ch);
  }
  return pos;
}
