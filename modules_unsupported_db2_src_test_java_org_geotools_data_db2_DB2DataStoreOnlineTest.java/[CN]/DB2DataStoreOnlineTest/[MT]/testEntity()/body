{
  DB2DataStore dataStore=getDataStore();
  String[] typeNames=dataStore.getTypeNames();
  int foundCount=0;
  for (int i=0; i < typeNames.length; i++) {
    if (typeNames[i].equals("Places")) {
      foundCount++;
    }
    if (typeNames[i].equals("Roads")) {
      foundCount++;
    }
  }
  assertTrue(foundCount == 2);
}
