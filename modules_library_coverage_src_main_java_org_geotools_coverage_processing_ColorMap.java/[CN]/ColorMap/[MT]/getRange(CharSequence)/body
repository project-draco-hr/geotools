{
  if (colorRanges == null) {
    return null;
  }
  final String name=unlocalized(category);
  NumberRange range=colorRanges.get(name);
  if (range == null) {
    if (name != null && category instanceof InternationalString) {
      range=colorRanges.get(category.toString());
    }
  }
  return range;
}
