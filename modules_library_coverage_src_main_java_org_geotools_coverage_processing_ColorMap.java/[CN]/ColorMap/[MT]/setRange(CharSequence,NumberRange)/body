{
  final String name=unlocalized(category);
  if (range != null) {
    if (colorRanges == null) {
      colorRanges=new HashMap<String,NumberRange>();
    }
    colorRanges.put(name,range);
  }
 else   if (colorRanges != null) {
    colorRanges.remove(name);
    if (colorRanges.isEmpty()) {
      colorRanges=null;
    }
  }
}
