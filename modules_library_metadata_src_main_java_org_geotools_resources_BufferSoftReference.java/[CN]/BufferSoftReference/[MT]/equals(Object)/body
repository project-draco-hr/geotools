{
  if (!(other instanceof BufferSoftReference)) {
    return false;
  }
  ByteBuffer buffer=get();
  if (buffer == null) {
    return false;
  }
 else {
    ByteBuffer otherBuffer=((BufferSoftReference)other).get();
    if (otherBuffer == null) {
      return false;
    }
    return otherBuffer == buffer;
  }
}
