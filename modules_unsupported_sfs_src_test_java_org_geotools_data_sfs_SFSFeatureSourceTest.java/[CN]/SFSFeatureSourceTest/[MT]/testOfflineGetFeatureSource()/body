{
  String _jsonText="[{" + "   \"name\": \"layerAsia\"," + "   \"bbox\": [-10,-40,30,80],"+ "   \"crs\": \"urn:ogc:def:crs:EPSG:4326\","+ "   \"axisorder\": \"yx\"},"+ "{"+ "   \"name\": \"layerAmerica\","+ "   \"crs\": \"urn:ogc:def:crs:EPSG:32632\" ,"+ "   \"axisorder\": \"xy\" },"+ "{"+ "   \"name\": \"layerEurope\","+ "   \"bbox\": [15000000,49000000,18000000,52000000],"+ "   \"crs\": \"urn:ogc:def:crs:EPSG:32632\" "+ " }]";
  SFSDataStore ods=new SFSDataStore(_jsonText,null);
  SFSFeatureSource odsfs=(SFSFeatureSource)ods.getFeatureSource(FEATURESOURCE);
  assertNotNull(odsfs);
}
