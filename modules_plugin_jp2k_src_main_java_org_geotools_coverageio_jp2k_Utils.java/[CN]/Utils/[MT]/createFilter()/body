{
  IOFileFilter fileFilter=Utils.includeFilters(FileFilterUtils.suffixFileFilter("jp2"),FileFilterUtils.suffixFileFilter("JP2"),FileFilterUtils.suffixFileFilter("j2c"),FileFilterUtils.suffixFileFilter("J2C"),FileFilterUtils.suffixFileFilter("jpx"),FileFilterUtils.suffixFileFilter("JPX"),FileFilterUtils.suffixFileFilter("jp2k"),FileFilterUtils.suffixFileFilter("JP2K"),FileFilterUtils.nameFileFilter("jpeg2000"));
  return fileFilter;
}
