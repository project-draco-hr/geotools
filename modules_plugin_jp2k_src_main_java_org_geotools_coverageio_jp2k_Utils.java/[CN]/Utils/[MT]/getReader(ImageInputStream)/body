{
  Utilities.ensureNonNull("inStream",inStream);
  try {
    if (inStream instanceof FileImageInputStreamExt) {
      final File file=((FileImageInputStreamExt)inStream).getFile();
      if (FILEFILTER.accept(file))       return JP2KFormatFactory.getCachedSpi().createReaderInstance();
    }
    return null;
  }
 catch (  IOException e) {
    if (LOGGER.isLoggable(Level.WARNING))     LOGGER.warning(e.getLocalizedMessage());
    return null;
  }
}
