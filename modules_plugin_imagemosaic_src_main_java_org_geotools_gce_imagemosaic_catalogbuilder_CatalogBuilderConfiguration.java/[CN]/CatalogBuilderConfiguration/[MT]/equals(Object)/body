{
  if (this == obj)   return true;
  if (!(obj instanceof CatalogBuilderConfiguration))   return false;
  final CatalogBuilderConfiguration that=(CatalogBuilderConfiguration)obj;
  if (this.absolute != that.absolute)   return false;
  if (this.caching != that.caching)   return false;
  if (this.recursive != that.recursive)   return false;
  if (this.footprintManagement != that.footprintManagement)   return false;
  if (!(this.indexName == null && that.indexName == null) && !this.indexName.equals(that.indexName))   return false;
  if (!(this.locationAttribute == null && that.locationAttribute == null) && !this.locationAttribute.equals(that.locationAttribute))   return false;
  if (!(this.rootMosaicDirectory == null && that.rootMosaicDirectory == null) && !this.rootMosaicDirectory.equals(that.rootMosaicDirectory))   return false;
  if (!Utilities.deepEquals(this.indexingDirectories,that.indexingDirectories))   return false;
  return true;
}
