{
  Map<String,String> params;
  params=getParams();
  assertTrue("all parameters valid - should have succeeded",factory.canProcess(params));
  params=getParams();
  params.remove("database");
  assertFalse("database parameter is required",factory.canProcess(params));
  params=getParams();
  params.put("dbtype","nodb");
  assertFalse("dbtype parameter is required",factory.canProcess(params));
  params=getParams();
  params.put("dbtype","DB2");
  assertTrue("should succeed with uppercase DB2 as dbtype",factory.canProcess(params));
  params=getParams();
  params.remove("host");
  assertFalse("host parameter is required",factory.canProcess(params));
  params=getParams();
  params.remove("port");
  assertFalse("port parameter is required",factory.canProcess(params));
  params=getParams();
  params.remove("user");
  assertTrue("user parameter should be optional",factory.canProcess(params));
  params=getParams();
  params.remove("passwd");
  assertTrue("passwd parameter should be optional",factory.canProcess(params));
  params=getParams();
  params.remove("tabschema");
  assertTrue("tabschema parameter should be optional",factory.canProcess(params));
}
