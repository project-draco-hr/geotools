{
  WriterUtils cw=new WriterUtils(w);
  Map m=new HashMap();
  if (dto.isFinal()) {
    m.put("final",new Boolean(true));
  }
  try {
    cw.openTag("argument",m);
    try {
      cw.textTag("name",dto.getName());
      cw.writeln(ArgHelper.getArgumentEncoding(dto.getValue()));
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    cw.closeTag("argument");
  }
 catch (  Exception e) {
    e.printStackTrace();
  }
}
