{
  checkIfColorMapCreated();
  if (this.numberColorMapEntries != -1)   throw new IllegalStateException(Errors.format(ErrorKeys.ILLEGAL_STATE));
  if (numberColorMapEntries <= 0 || numberColorMapEntries > (extendedColors ? 65536 : 256))   throw new IllegalArgumentException(Errors.format(ErrorKeys.ILLEGAL_ARGUMENT_$2,"numberColorMapEntries",Integer.toString(numberColorMapEntries)));
  this.numberColorMapEntries=numberColorMapEntries;
  return this;
}
