{
  checkIfColorMapCreated();
  if (LinearColorMapType.validateColorMapTye(this.linearColorMapType))   throw new IllegalStateException(Errors.format(ErrorKeys.ILLEGAL_STATE));
  if (!LinearColorMapType.validateColorMapTye(colorMapType))   throw new IllegalArgumentException(Errors.format(ErrorKeys.ILLEGAL_ARGUMENT_$2,"colorMapType",Integer.toString(colorMapType)));
  this.linearColorMapType=colorMapType;
  return this;
}
