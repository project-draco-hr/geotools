{
  Filter resultFilter=CQL.toFilter("ATTR = relatePattern(the_geom, 'LINESTRING (27.3 37, 27.3 37.6)', '**1****') ");
  Assert.assertTrue(resultFilter instanceof PropertyIsEqualTo);
  Expression resultExpression=CQL.toExpression("relatePattern(the_geom, 'LINESTRING (27.3 37, 27.3 37.6)', '**1****') ");
  Assert.assertTrue(resultExpression instanceof FilterFunction_relatePattern);
}
