{
  width=(short)dx;
  height=(short)dy;
  final String name;
  final int value;
  if ((width & MASK) != dx) {
    name="width";
    value=dx;
  }
 else   if ((height & MASK) != dy) {
    name="height";
    value=dy;
  }
 else {
    return;
  }
  width=0;
  height=0;
  throw new IllegalArgumentException(Errors.format(ErrorKeys.VALUE_OUT_OF_BOUNDS_$3,name + '=' + value,0,MASK));
}
