{
  if (object == this) {
    return true;
  }
  if (object != null && object.getClass().equals(getClass())) {
    final Tile that=(Tile)object;
    if (this.x == that.x && this.y == that.y && this.xSubsampling == that.xSubsampling && this.ySubsampling == that.ySubsampling && this.imageIndex == that.imageIndex && Utilities.equals(provider,that.provider) && Utilities.deepEquals(input,that.input)) {
      return (width == 0 || that.width == 0 || width == that.width) && (height == 0 || that.height == 0 || height == that.height);
    }
  }
  return false;
}
