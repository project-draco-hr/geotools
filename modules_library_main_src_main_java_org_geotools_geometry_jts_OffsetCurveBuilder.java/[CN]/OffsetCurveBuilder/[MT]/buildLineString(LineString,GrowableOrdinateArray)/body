{
  GeometryFactory factory=ls.getFactory();
  CoordinateSequence cs=ordinates.toCoordinateSequence(factory);
  if (ls instanceof LinearRing) {
    if (cs.size() >= 4) {
      return factory.createLinearRing(cs);
    }
 else {
      return null;
    }
  }
 else {
    return factory.createLineString(cs);
  }
}
