{
  Calendar cal=Calendar.getInstance();
  cal.set(2012,6,15);
  Date d1=cal.getTime();
  cal.set(2012,8,15);
  Date d2=cal.getTime();
  Date d3=cal.getTime();
  Literal l1=ff.literal(d1);
  Literal l2=ff.literal(d2);
  assertFalse(l1.equals(l2));
  Literal l3=ff.literal(d3);
  assertTrue(l2.equals(l3));
}
