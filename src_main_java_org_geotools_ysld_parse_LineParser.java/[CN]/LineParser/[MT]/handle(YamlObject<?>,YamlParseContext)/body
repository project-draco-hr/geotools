{
  super.handle(obj,context);
  YamlMap map=obj.map();
  context.push(obj,new StrokeParser(factory){
    @Override protected void stroke(    Stroke stroke){
      sym.setStroke(stroke);
    }
  }
);
  if (map.has("offset")) {
    sym.setPerpendicularOffset(Util.expression(map.str("offset"),factory));
  }
}
