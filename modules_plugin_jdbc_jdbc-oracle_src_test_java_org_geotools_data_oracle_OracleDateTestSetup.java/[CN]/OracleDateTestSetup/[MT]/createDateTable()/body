{
  run("CREATE TABLE DATES (D DATE, DT TIMESTAMP, T TIMESTAMP)");
  run("INSERT INTO DATES VALUES (" + "TO_DATE('2009-06-28', 'yyyy-MM-dd'), " + "TO_DATE('2009-06-28 15:12:41', 'yyyy-MM-dd HH24:mi:ss'),"+ "TO_DATE('15:12:41', 'HH24:mi:ss')  )");
  run("INSERT INTO DATES VALUES (" + "TO_DATE('2009-01-15', 'yyyy-MM-dd'), " + "TO_DATE('2009-01-15 13:10:12', 'yyyy-MM-dd HH24:mi:ss'),"+ "TO_DATE('13:10:12', 'HH24:mi:ss')  )");
  run("INSERT INTO DATES VALUES (" + "TO_DATE('2009-09-29', 'yyyy-MM-dd'), " + "TO_DATE('2009-09-29 17:54:23', 'yyyy-MM-dd HH24:mi:ss'),"+ "TO_DATE('17:54:23', 'HH24:mi:ss')  )");
}
