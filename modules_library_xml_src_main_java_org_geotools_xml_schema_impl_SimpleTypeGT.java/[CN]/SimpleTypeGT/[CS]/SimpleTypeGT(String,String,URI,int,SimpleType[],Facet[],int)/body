{
  this.id=id;
  this.name=name;
  this.namespace=namespace;
  this.parents=parents;
  this.type=type;
  this.constraints=constraints;
  if (constraints != null) {
    for (int i=0; i < constraints.length; i++)     if (constraints[i] == null)     throw new NullPointerException(name + " constraint #" + i+ " is null");
  }
}
