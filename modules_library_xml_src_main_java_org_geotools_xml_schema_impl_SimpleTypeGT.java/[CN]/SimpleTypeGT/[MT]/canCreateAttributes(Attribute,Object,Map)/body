{
  if (value == null) {
    return false;
  }
  if (type == UNION) {
    for (int i=0; i < parents.length; i++) {
      if (parents[i].equals(attribute.getSimpleType()) && parents[i].canCreateAttributes(attribute,value,hints)) {
        return true;
      }
    }
    return false;
  }
  if (type == LIST) {
    return parents[0].canCreateAttributes(attribute,value,hints);
  }
  return parents[0].canCreateAttributes(attribute,value,hints);
}
