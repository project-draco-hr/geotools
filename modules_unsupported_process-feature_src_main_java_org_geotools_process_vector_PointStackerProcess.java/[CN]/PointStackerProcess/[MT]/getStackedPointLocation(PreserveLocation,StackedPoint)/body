{
  Coordinate pt=null;
  if (PreserveLocation.Single == preserveLocation) {
    if (sp.getCount() == 1) {
      pt=sp.getOriginalLocation();
    }
  }
 else   if (PreserveLocation.Superimposed == preserveLocation) {
    if (sp.getCountUnique() == 1) {
      pt=sp.getOriginalLocation();
    }
  }
  if (pt == null) {
    pt=sp.getLocation();
  }
  return pt;
}
