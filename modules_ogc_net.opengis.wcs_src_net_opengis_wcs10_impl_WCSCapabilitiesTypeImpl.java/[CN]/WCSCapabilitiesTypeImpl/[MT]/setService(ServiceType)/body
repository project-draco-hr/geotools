{
  if (newService != service) {
    NotificationChain msgs=null;
    if (service != null)     msgs=((InternalEObject)service).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Wcs10Package.WCS_CAPABILITIES_TYPE__SERVICE,null,msgs);
    if (newService != null)     msgs=((InternalEObject)newService).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Wcs10Package.WCS_CAPABILITIES_TYPE__SERVICE,null,msgs);
    msgs=basicSetService(newService,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Wcs10Package.WCS_CAPABILITIES_TYPE__SERVICE,newService,newService));
}
