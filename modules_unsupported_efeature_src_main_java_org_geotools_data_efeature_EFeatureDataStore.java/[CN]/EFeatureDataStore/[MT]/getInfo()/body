{
  if (serviceInfo == null) {
    serviceInfo=new DefaultServiceInfo();
    serviceInfo.setTitle("EFeature DataStore");
    serviceInfo.setKeywords(new HashSet<String>(Arrays.asList("EFeature","EMF","EMF Query","EMF Transaction")));
    serviceInfo.setDescription("The EFeature DataStore module adds support " + "for spatial read and write operations to EMF models.");
    serviceInfo.setSchema(java.net.URI.create(eStoreInfo.eNsURI));
    serviceInfo.setPublisher(java.net.URI.create(PUBLISHER));
  }
  serviceInfo.setSource(java.net.URI.create(eURI.toString()));
  return serviceInfo;
}
