{
  if (writer == null) {
    throw new IOException("Writer has been closed");
  }
  writer.write(position == 0 ? "=" : "|");
  if (attribute == null) {
  }
 else   if (attribute instanceof Geometry) {
    writer.write(((Geometry)attribute).toText());
  }
 else {
    writer.write(attribute.toString());
  }
}
