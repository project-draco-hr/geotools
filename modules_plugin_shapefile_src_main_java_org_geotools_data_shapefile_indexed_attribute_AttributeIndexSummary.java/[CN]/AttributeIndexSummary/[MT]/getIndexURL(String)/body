{
  File f=DataUtilities.urlToFile(summaryURL);
  BufferedReader in=new BufferedReader(new FileReader(f));
  int count=0;
  while (in.ready()) {
    String s=in.readLine();
    count++;
    if (s.equals(attribute)) {
      return new URL(filename + INDEX_EXT + SUFFIX.format(count));
    }
  }
  return null;
}
