{
  super.setUp();
  JDBCDataStore data=(JDBCDataStore)this.data;
  line3DType=DataUtilities.createType(data.getNameSpace().toString() + "." + LINE3D,ID + ":0," + GEOM+ ":LineString:srid=4326,"+ NAME+ ":String");
  line3DType.getGeometryDescriptor().getUserData().put(Hints.COORDINATE_DIMENSION,3);
  poly3DType=DataUtilities.createType(data.getNameSpace().toString() + "." + POLY3D,ID + ":0," + GEOM+ ":Polygon:srid=4326,"+ NAME+ ":String");
  poly3DType.getGeometryDescriptor().getUserData().put(Hints.COORDINATE_DIMENSION,3);
  epsg4326=CRS.decode("EPSG:4326");
}
