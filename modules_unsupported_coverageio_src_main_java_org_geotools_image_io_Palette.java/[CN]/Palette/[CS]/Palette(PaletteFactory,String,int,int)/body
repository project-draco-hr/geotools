{
  if (factory == null) {
    throw new IllegalArgumentException(Errors.format(ErrorKeys.NULL_ARGUMENT_$1,"factory"));
  }
  if (name == null) {
    throw new IllegalArgumentException(factory.getErrorResources().getString(ErrorKeys.NULL_ARGUMENT_$1,"name"));
  }
  ensureInsideBounds(numBands,0,255);
  ensureInsideBounds(visibleBand,0,numBands - 1);
  this.factory=factory;
  this.name=name.trim();
  this.numBands=numBands;
  this.visibleBand=visibleBand;
}
