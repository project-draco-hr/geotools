{
  store.dispose();
  File dir=new File("propertyTestData");
  for (  File file : dir.listFiles()) {
    file.delete();
  }
  dir.delete();
  sridStore.dispose();
  dir=new File("propertyTestData2");
  for (  File file : dir.listFiles()) {
