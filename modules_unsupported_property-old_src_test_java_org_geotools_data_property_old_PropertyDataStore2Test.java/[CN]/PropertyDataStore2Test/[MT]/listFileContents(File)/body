{
  System.out.println("Contents of " + target);
  if (target.exists()) {
    BufferedReader reader=new BufferedReader(new FileReader(target));
    String line;
    while ((line=reader.readLine()) != null) {
      System.out.println(line);
    }
    reader.close();
  }
 else {
    System.out.println(" ... does not exist");
  }
}
