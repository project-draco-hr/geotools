{
  if (dateString.endsWith("Z")) {
    return "GMT+" + 0;
  }
  int index=dateString.lastIndexOf("+");
  if (index == -1) {
    index=dateString.lastIndexOf("-");
  }
  if (index > dateString.indexOf("T")) {
    return "GMT" + dateString.substring(index);
  }
  return TimeZone.getDefault().getID();
}
