{
  int sum=0;
  for (  int cur : inserts) {
    if (cur == PreparedStatement.SUCCESS_NO_INFO) {
      return;
    }
 else     if (cur == PreparedStatement.EXECUTE_FAILED) {
      throw new IOException("Failed to insert some features");
    }
    sum+=cur;
  }
  if (sum != size) {
    throw new IOException("Failed to insert some features");
  }
}
