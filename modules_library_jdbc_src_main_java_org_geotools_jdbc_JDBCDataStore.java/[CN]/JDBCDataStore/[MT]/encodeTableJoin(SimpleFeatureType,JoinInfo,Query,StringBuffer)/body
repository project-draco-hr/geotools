{
  encodeAliasedTableName(featureType.getTypeName(),sql,query.getHints(),join.getPrimaryAlias());
  for (  JoinPart part : join.getParts()) {
    sql.append(" ");
    dialect.encodeJoin(part.getJoin().getType(),sql);
    sql.append(" ");
    encodeAliasedTableName(part.getQueryFeatureType().getTypeName(),sql,null,part.getAlias());
    sql.append(" ON ");
    Filter j=part.getJoinFilter();
    FilterToSQL toSQL=dialect instanceof PreparedStatementSQLDialect ? createPreparedFilterToSQL(null) : createFilterToSQL(null);
    toSQL.setInline(true);
    try {
      sql.append(" ").append(toSQL.encodeToString(j));
    }
 catch (    FilterToSQLException e) {
      throw new RuntimeException(e);
    }
  }
}
