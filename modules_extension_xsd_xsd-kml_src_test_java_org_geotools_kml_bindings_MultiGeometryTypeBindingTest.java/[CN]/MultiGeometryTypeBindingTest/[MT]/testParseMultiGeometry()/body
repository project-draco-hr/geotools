{
  String xml="<MultiGeometry>" + "<Point><coordinates>0,0</coordinates></Point>" + "<LineString><coordinates>0,0 1,1</coordinates></LineString>"+ "<Polygon>"+ "<outerBoundaryIs><LinearRing><coordinates>0,0 1,1 2,2 0,0</coordinates></LinearRing></outerBoundaryIs></Polygon>"+ "</MultiGeometry>";
  buildDocument(xml);
  GeometryCollection gc=(GeometryCollection)parse();
  assertEquals(3,gc.getNumGeometries());
  assertTrue(gc.getGeometryN(0) instanceof Point);
  assertTrue(gc.getGeometryN(1) instanceof LineString);
  assertTrue(gc.getGeometryN(2) instanceof Polygon);
}
