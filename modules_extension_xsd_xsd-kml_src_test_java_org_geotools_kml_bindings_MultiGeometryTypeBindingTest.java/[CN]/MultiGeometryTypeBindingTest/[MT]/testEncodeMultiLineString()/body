{
  GeometryFactory gf=new GeometryFactory();
  MultiLineString ml=gf.createMultiLineString(new LineString[]{gf.createLineString(new Coordinate[]{new Coordinate(0,0),new Coordinate(1,1)}),gf.createLineString(new Coordinate[]{new Coordinate(2,2),new Coordinate(3,3)})});
  Document dom=encode(ml,KML.MultiGeometry);
  assertEquals(2,getElementsByQName(dom,KML.LineString).getLength());
}
