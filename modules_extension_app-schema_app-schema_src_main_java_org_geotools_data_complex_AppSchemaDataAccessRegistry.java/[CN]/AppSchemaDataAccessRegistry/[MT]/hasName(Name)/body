{
  if (registry == null) {
    return false;
  }
  for (  DataAccess<FeatureType,Feature> dataAccess : registry) {
    if (dataAccess instanceof AppSchemaDataAccess && (((AppSchemaDataAccess)dataAccess).hasName(featureTypeName) || ((AppSchemaDataAccess)dataAccess).hasElement(featureTypeName))) {
      return true;
    }
  }
  return false;
}
