{
  mInfo=new HashMap<String,String>();
  mInfo.put("name","Gtopo30");
  mInfo.put("description","Gtopo30 Coverage Format");
  mInfo.put("vendor","Geotools");
  mInfo.put("docURL","http://edcdaac.usgs.gov/gtopo30/gtopo30.asp");
  mInfo.put("version","1.0");
  readParameters=new ParameterGroup(new DefaultParameterDescriptorGroup(mInfo,new GeneralParameterDescriptor[]{READ_GRIDGEOMETRY2D}));
  writeParameters=new ParameterGroup(new DefaultParameterDescriptorGroup(mInfo,new GeneralParameterDescriptor[]{GEOTOOLS_WRITE_PARAMS}));
}
