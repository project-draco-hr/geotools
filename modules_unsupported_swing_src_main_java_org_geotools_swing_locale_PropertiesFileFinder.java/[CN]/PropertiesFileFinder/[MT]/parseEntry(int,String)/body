{
  name=name.substring(prefixLength,name.indexOf(".properties"));
  String[] parts=name.split("_");
  String baseName=parts[0];
  String language="";
  String country="";
  String variant="";
  if (parts.length > 1) {
    language=parts[1];
  }
  if (parts.length > 2) {
    country=parts[2];
  }
  if (parts.length > 3) {
    variant=parts[3];
  }
  Locale locale;
  if (parts.length == 1) {
    locale=Locale.ROOT;
  }
 else {
    locale=new Locale(language,country,variant);
  }
  return new SingleFileInfo(baseName,locale);
}
