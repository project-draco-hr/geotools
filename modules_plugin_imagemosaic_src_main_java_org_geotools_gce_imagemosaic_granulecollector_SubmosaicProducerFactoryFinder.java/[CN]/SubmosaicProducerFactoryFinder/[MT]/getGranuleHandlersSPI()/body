{
  FactoryRegistry serviceRegistry=getServiceRegistry();
  serviceRegistry.scanForPlugins();
  final Iterator<SubmosaicProducerFactory> it=serviceRegistry.getServiceProviders(SubmosaicProducerFactory.class,true);
  Map<String,SubmosaicProducerFactory> acceptorFactorySPIMap=new HashMap<>();
  while (it.hasNext()) {
    SubmosaicProducerFactory submosaicProducerFactory=it.next();
    acceptorFactorySPIMap.put(submosaicProducerFactory.getClass().getName(),submosaicProducerFactory);
  }
  return acceptorFactorySPIMap;
}
