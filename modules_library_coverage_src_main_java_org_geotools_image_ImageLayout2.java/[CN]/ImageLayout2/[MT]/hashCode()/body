{
  int code=0, i=1;
  if (isValid(ImageLayout2.WIDTH_MASK))   code+=(getWidth(null) * i++);
  if (isValid(ImageLayout2.HEIGHT_MASK))   code+=(getHeight(null) * i++);
  if (isValid(ImageLayout2.MIN_X_MASK))   code+=(getMinX(null) * i++);
  if (isValid(ImageLayout2.MIN_Y_MASK))   code+=(getMinY(null) * i++);
  if (isValid(ImageLayout2.TILE_HEIGHT_MASK))   code+=(getTileHeight(null) * i++);
  if (isValid(ImageLayout2.TILE_WIDTH_MASK))   code+=(getTileWidth(null) * i++);
  if (isValid(ImageLayout2.TILE_GRID_X_OFFSET_MASK))   code+=(getTileGridXOffset(null) * i++);
  if (isValid(ImageLayout2.TILE_GRID_Y_OFFSET_MASK))   code+=(getTileGridYOffset(null) * i++);
  if (isValid(ImageLayout2.SAMPLE_MODEL_MASK))   code^=getSampleModel(null).hashCode();
  code^=validMask;
  if (isValid(ImageLayout2.COLOR_MODEL_MASK))   code^=getColorModel(null).hashCode();
  return code;
}
