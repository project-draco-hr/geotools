{
  for (  ContentState entry : state.values()) {
    if (entry == source) {
      continue;
    }
    for (    FeatureListener listener : entry.listeners) {
      try {
        listener.changed(notification);
      }
 catch (      Throwable t) {
        dataStore.LOGGER.log(Level.WARNING,"Problem issuing feature event " + notification,t);
      }
    }
  }
}
