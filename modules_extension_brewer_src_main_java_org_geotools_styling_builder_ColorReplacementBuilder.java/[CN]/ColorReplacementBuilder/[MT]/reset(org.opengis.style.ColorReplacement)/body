{
  if (replacement == null) {
    return unset();
  }
  mapping.clear();
  if (replacement.getRecoding() != null && replacement.getRecoding().getParameters().size() > 0) {
    List<Expression> params=replacement.getRecoding().getParameters();
    propertyName=params.get(0);
    for (int i=0; i < params.size(); i++) {
      mapping.add(params.get(i));
    }
  }
  unset=false;
  return this;
}
