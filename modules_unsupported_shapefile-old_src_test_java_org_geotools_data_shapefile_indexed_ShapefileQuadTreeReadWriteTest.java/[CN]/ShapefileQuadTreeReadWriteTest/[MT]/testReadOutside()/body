{
  File f=copyShapefiles("shapes/statepop.shp");
  ShapefileDataStoreFactory fac=new ShapefileDataStoreFactory();
  IndexedShapefileDataStore ds=(IndexedShapefileDataStore)createDataStore(fac,f.toURI().toURL(),true);
  CloseableIterator<Data> it=ds.queryQuadTree(new Envelope(-62,23,-61,22));
  assertNotNull(it);
  assertFalse(it.hasNext());
  it.close();
  ds.dispose();
}
