{
  StringBuilder handle=new StringBuilder("GeoTools ").append(GeoTools.getVersion()).append("(").append(GeoTools.getBuildRevision()).append(") WFS ").append("1.1.0").append(" DataStore @");
  try {
    handle.append(InetAddress.getLocalHost().getHostName());
  }
 catch (  Exception ignore) {
    handle.append("<uknown host>");
  }
  handle.append('#').append(reqHandleSeq.incrementAndGet());
  return handle.toString();
}
