{
  if (feature == null) {
    return null;
  }
 else   if (feature instanceof SimpleFeature) {
    SimpleFeature sf=(SimpleFeature)feature;
    return geometry(sf.getDefaultGeometry());
  }
 else {
    GeometryAttribute ga=feature.getDefaultGeometryProperty();
    if (ga == null) {
      return null;
    }
 else {
      return geometry(ga.getValue());
    }
  }
}
