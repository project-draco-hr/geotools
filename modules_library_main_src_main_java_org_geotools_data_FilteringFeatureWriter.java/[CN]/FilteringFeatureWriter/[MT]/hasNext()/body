{
  if (next != null) {
    return true;
  }
  if (writer == null) {
    return false;
  }
  if (current != null) {
    current=null;
  }
  SimpleFeature peek;
  while (writer.hasNext()) {
    peek=writer.next();
    if (filter.evaluate(peek)) {
      next=peek;
      return true;
    }
  }
  return false;
}
