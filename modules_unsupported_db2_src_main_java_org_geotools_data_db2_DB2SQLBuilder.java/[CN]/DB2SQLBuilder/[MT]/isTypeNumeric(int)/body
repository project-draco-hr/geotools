{
  boolean numeric=false;
  if ((columnType == Types.BIT) || (columnType == Types.TINYINT) || (columnType == Types.SMALLINT)|| (columnType == Types.INTEGER)|| (columnType == Types.BIGINT)|| (columnType == Types.FLOAT)|| (columnType == Types.REAL)|| (columnType == Types.DOUBLE)|| (columnType == Types.NUMERIC)|| (columnType == Types.DECIMAL)) {
    numeric=true;
  }
  return (numeric);
}
