{
  YsldInput in=reader(ysld);
  try {
    YsldParser parser=new YsldParser(in.reader);
    if (zCtxtFinders != null) {
      parser.setZoomContextFinders(zCtxtFinders);
    }
    if (locator != null) {
      parser.setResourceLocator(locator);
    }
    return parser.parse();
  }
  finally {
    in.close();
  }
}
