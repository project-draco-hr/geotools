{
  graphics.drawRect(col * tileWidth,row * tileHeight,tileWidth - 1,tileHeight - 1);
  graphics.drawLine(col * tileWidth,row * tileHeight,(col * tileWidth) + tileWidth - 1,(row * tileHeight) + tileHeight - 1);
  graphics.drawLine((col * tileWidth) + tileWidth - 1,row * tileWidth,col * tileWidth,(row * tileHeight) + tileHeight - 1);
}
