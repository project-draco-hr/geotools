{
  if (currentCoord == 0) {
    coords[0]=(float)allCoords[0];
    coords[1]=(float)allCoords[1];
    if (at != null)     at.transform(coords,0,coords,0,1);
    return SEG_MOVETO;
  }
 else {
    coords[0]=(float)allCoords[currentCoord * 2];
    coords[1]=(float)allCoords[currentCoord * 2 + 1];
    if (at != null)     at.transform(coords,0,coords,0,1);
    return SEG_LINETO;
  }
}
