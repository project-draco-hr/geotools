{
  if (array == null) {
    throw new NullPointerException("Vector array required");
  }
  if (array.length == 2) {
    this.x=array[0];
    this.y=array[1];
    this.z=Double.NaN;
  }
 else   if (array.length == 3) {
    this.x=array[0];
    this.y=array[1];
    this.z=array[2];
  }
 else {
    throw new IllegalArgumentException("Vector array (length " + array.length + ") is not expected length of 3");
  }
}
