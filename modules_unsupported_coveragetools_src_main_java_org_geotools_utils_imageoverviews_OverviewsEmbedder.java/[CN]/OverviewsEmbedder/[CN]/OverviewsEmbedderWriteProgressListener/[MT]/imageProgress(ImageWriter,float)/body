{
  final int tempValue=(int)(percentageDone / 5.0);
  if (tempValue > lastProgress) {
    lastProgress=tempValue;
    OverviewsEmbedder.this.fireEvent(new StringBuilder("Writing out overview ").append(overviewInProcess + 1).toString(),(overviewInProcess / numSteps + percentageDone / (100 * numSteps)) * 100.0);
  }
}
