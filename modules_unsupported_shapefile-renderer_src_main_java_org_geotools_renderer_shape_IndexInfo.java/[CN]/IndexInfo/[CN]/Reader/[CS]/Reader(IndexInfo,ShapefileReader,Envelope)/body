{
  shp=reader;
  try {
    if (info.treeType == IndexType.QIX) {
      info.qtree=info.openQuadTree();
    }
    goodRecs=info.queryTree(bbox);
  }
 catch (  Exception e) {
    ShapefileRenderer.LOGGER.log(Level.FINE,"Exception occured attempting to use indexing:",e);
    goodRecs=null;
  }
  this.info=info;
}
