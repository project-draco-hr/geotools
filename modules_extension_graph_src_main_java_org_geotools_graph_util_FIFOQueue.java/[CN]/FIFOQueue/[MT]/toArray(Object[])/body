{
  int size=size();
  if (a.length < size) {
    a=(Object[])java.lang.reflect.Array.newInstance(a.getClass().getComponentType(),size);
  }
  int j=0;
  for (int i=m_out; i < m_values.length; i++, j++) {
    if (i == m_in) {
      if (j < a.length)       a[j]=null;
      return (a);
    }
    a[j]=m_values[i];
  }
  for (int i=0; i < m_out; i++, j++) {
    a[j]=m_values[i];
  }
  if (j < a.length)   a[j]=null;
  return (a);
}
