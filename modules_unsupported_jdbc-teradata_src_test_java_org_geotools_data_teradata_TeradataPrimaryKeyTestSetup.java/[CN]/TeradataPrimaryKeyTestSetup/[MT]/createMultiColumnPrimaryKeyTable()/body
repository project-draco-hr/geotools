{
  run("CREATE TABLE \"multi\" ( \"key1\" int NOT NULL, \"key2\" varchar(20) NOT NULL, " + "\"name\" varchar(20), geom ST_Geometry)");
  run("INSERT INTO SYSSPATIAL.GEOMETRY_COLUMNS (F_TABLE_CATALOG, F_TABLE_SCHEMA, F_TABLE_NAME, " + "F_GEOMETRY_COLUMN, COORD_DIMENSION, SRID, GEOM_TYPE) VALUES ('','test','multi', 'geom', " + "2, " + getDelegate().getSrid4326() + ", 'ST_Geometry')");
  run("ALTER TABLE \"multi\" ADD PRIMARY KEY (\"key1\",\"key2\")");
  run("INSERT INTO \"multi\" VALUES (1, 'x', 'one', NULL)");
  run("INSERT INTO \"multi\" VALUES (2, 'y', 'two', NULL)");
  run("INSERT INTO \"multi\" VALUES (3, 'z', 'three', NULL)");
}
