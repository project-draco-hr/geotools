{
  Filter retFilter;
  if (filter1 != null) {
    if (filter2 != null) {
      if (splitType == AbstractFilter.LOGIC_AND) {
        retFilter=Filters.and(ff,filter1,filter2);
      }
 else {
        retFilter=Filters.or(ff,filter1,filter2);
      }
    }
 else {
      retFilter=filter1;
    }
  }
 else {
    if (filter2 != null) {
      retFilter=filter2;
    }
 else {
      retFilter=null;
    }
  }
  return retFilter;
}
