{
  int size=seq.size();
  StringBuilder sb=new StringBuilder();
  if (size > 1) {
    sb.append("[");
  }
  for (int i=0; i < seq.size(); i++) {
    sb.append("[");
    formatDecimal(seq.getX(i),sb);
    sb.append(",");
    formatDecimal(seq.getY(i),sb);
    sb.append("],");
  }
  sb.setLength(sb.length() - 1);
  if (size > 1) {
    sb.append("]");
  }
  return sb.toString();
}
