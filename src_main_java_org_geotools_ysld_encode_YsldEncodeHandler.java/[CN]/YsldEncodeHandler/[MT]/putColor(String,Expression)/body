{
  boolean special=false;
  if (expr instanceof Literal) {
    String str=ECQL.toCQL(expr);
    str=stripQuotes(str);
    if (str != null && str.startsWith("#")) {
      str=str.substring(1);
      put(key,makeNumberIfPossible(str));
      special=true;
    }
  }
  if (!special) {
    put(key,expr);
  }
  return this;
}
