{
  int count=0;
  SimpleFeatureIterator it=roads.features();
  try {
    while (it.hasNext()) {
      SimpleFeature feature=it.next();
      if (rd12Filter.evaluate(feature)) {
        count++;
      }
    }
  }
  finally {
    it.close();
  }
  SimpleFeatureCollection sub=roads.subCollection(rd12Filter);
  assertEquals(count,sub.size());
}
