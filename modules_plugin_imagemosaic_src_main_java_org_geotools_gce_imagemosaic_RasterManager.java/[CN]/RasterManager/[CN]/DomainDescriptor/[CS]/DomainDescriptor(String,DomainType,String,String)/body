{
  this.identifier=identifier;
  this.propertyName=propertyName;
  this.domainType=domainType;
  this.additionalPropertyName=additionalPropertyName;
  final String name=identifier.toUpperCase();
  this.domainParameterDescriptor=DefaultParameterDescriptor.create(name,"Additional " + identifier + " domain",List.class,null,false);
}
