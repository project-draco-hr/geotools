{
synchronized (this) {
    if (readOnly) {
      if (granuleSource == null) {
        granuleSource=new GranuleCatalogSource(granuleCatalog,typeName,hints);
      }
      return granuleSource;
    }
 else {
      if (granuleStore == null) {
        granuleStore=new GranuleCatalogStore(granuleCatalog,typeName,hints);
      }
      return granuleStore;
    }
  }
}
