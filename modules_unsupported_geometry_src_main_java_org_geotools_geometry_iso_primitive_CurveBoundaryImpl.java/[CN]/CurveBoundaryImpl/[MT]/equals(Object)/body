{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  final CurveBoundaryImpl other=(CurveBoundaryImpl)obj;
  if (endPoint == null) {
    if (other.endPoint != null)     return false;
  }
 else   if (!endPoint.equals(other.endPoint))   return false;
  if (startPoint == null) {
    if (other.startPoint != null)     return false;
  }
 else   if (!startPoint.equals(other.startPoint))   return false;
  return true;
}
