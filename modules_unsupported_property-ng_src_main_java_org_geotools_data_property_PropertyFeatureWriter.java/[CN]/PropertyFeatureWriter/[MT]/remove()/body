{
  if (live == null) {
    throw new IOException("No current feature to remove");
  }
  if (origional != null) {
    String typeName=live.getFeatureType().getTypeName();
    Transaction autoCommit=Transaction.AUTO_COMMIT;
    state.fireFeatureRemoved(featureSource,origional);
  }
  origional=null;
  live=null;
}
