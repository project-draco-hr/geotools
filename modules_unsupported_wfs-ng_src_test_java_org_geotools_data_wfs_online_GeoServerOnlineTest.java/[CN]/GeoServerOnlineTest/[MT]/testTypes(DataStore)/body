{
  if (url_100 == null)   return;
  String types[]=wfs.getTypeNames();
  String typeName="unknown";
  for (int i=0; i < types.length; i++) {
    typeName=types[i];
    if (typeName.equals("topp_geometrytype"))     continue;
    SimpleFeatureType type=wfs.getSchema(typeName);
    type.getTypeName();
    type.getName().getNamespaceURI();
    SimpleFeatureSource source=wfs.getFeatureSource(typeName);
    source.getBounds();
    SimpleFeatureCollection features=source.getFeatures();
    features.getBounds();
    features.getSchema();
    Query query=new Query(typeName,Filter.INCLUDE,20,Query.ALL_NAMES,"work already");
    features=source.getFeatures(query);
    features.size();
    SimpleFeatureIterator iterator=features.features();
    try {
      while (iterator.hasNext()) {
        SimpleFeature feature=iterator.next();
      }
    }
  finally {
      iterator.close();
    }
  }
}
