{
  if (indexUseable(ShpFileType.FIX)) {
    return true;
  }
 else {
    if (isLocal()) {
      try {
        FidIndexer.generate(shpFiles);
        return true;
      }
 catch (      IOException e) {
        LOGGER.log(Level.WARNING,"Failed to create fid index");
        return false;
      }
    }
 else {
      return false;
    }
  }
}
