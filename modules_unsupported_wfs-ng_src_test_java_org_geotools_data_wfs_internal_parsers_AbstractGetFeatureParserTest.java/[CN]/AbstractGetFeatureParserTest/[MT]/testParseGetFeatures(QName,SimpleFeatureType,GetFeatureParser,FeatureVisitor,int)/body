{
  int featureCount=0;
  SimpleFeature feature;
  try {
    for (int i=0; i < expectedFeatureCount; i++) {
      feature=parser.parse();
      assertNotNull(feature);
      featureCount++;
      assertor.visit(feature);
    }
    feature=parser.parse();
    assertNull(feature);
  }
  finally {
    parser.close();
  }
  assertEquals(expectedFeatureCount,featureCount);
}
