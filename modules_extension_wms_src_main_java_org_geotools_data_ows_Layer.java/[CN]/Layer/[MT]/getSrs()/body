{
synchronized (this) {
    if (allSRSCache == null) {
      allSRSCache=new HashSet<String>(srs);
      Layer parent=this.getParent();
      if (parent != null) {
        Set<String> parentSrs=parent.getSrs();
        if (parentSrs != null)         allSRSCache.addAll(parentSrs);
      }
    }
    return allSRSCache;
  }
}
