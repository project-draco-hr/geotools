{
  if (datumType == Datum.TEMPORAL_DATUM || datumType == Datum.VERTICAL_DATUM)   throw new IllegalArgumentException("Use a TemporalCRS/VerticalCRS to set Temporal/Vertical datums");
  if (!isProjectedOrDerived)   super.setDatum(datumType,identification);
 else   baseCRS.setDatum(datumType,identification);
}
