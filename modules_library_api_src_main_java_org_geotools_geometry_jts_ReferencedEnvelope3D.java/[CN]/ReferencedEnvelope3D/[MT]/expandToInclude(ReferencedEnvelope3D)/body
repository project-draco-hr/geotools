{
  ensureCompatibleReferenceSystem(other);
  if (other.isNull()) {
    return;
  }
  if (isNull()) {
    super.expandToInclude(other);
    minz=other.getMinZ();
    maxz=other.getMaxZ();
  }
 else {
    super.expandToInclude(other);
    if (other.minz < minz) {
      minz=other.minz;
    }
    if (other.maxz > maxz) {
      maxz=other.maxz;
    }
  }
}
