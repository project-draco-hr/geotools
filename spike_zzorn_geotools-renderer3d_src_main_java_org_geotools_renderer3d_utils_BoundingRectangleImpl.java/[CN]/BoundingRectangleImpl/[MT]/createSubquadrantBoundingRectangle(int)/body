{
  ParameterChecker.checkIntegerInRange(subquadrantIndex,"subquadrantIndex",0,4);
  double x1=myX1;
  double y1=myY1;
  double x2=myX2;
  double y2=myY2;
  if (inTopRow(subquadrantIndex)) {
    y2=myCenterY;
  }
 else {
    y1=myCenterY;
  }
  if (inLeftColumn(subquadrantIndex)) {
    x2=myCenterX;
  }
 else {
    x1=myCenterX;
  }
  return new BoundingRectangleImpl(x1,y1,x2,y2);
}
