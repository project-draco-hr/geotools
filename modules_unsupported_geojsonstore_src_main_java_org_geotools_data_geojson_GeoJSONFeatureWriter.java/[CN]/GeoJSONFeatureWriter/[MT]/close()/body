{
  if (geoJSONWriter == null) {
    throw new IOException("Writer is already closed");
  }
  if (this.currentFeature != null) {
    this.write();
  }
  while (this.hasNext()) {
    next();
    write();
  }
  geoJSONWriter.close();
  geoJSONWriter=null;
  if (delegate != null) {
    delegate.close();
    delegate=null;
  }
  Files.copy(temp.toPath(),file.toPath(),StandardCopyOption.REPLACE_EXISTING);
}
