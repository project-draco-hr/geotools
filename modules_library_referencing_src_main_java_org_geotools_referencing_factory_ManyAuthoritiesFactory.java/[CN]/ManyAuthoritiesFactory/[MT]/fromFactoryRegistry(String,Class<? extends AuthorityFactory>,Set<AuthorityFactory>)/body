{
  for (int i=0; i < OBJECT_TYPES.length; i++) {
    if (OBJECT_TYPES[i].isAssignableFrom(type)) {
      final AuthorityFactory factory;
      try {
        factory=fromFactoryRegistry(authority,FACTORY_TYPES[i]);
      }
 catch (      FactoryRegistryException e) {
        continue;
      }
      if (!exclude(factory)) {
        addTo.add(factory);
      }
    }
  }
}
