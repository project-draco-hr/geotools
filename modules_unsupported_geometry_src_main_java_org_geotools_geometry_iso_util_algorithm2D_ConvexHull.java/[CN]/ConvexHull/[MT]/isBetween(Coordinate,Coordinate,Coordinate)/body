{
  if (CGAlgorithms.computeOrientation(c1,c2,c3) != 0) {
    return false;
  }
  if (c1.x != c3.x) {
    if (c1.x <= c2.x && c2.x <= c3.x) {
      return true;
    }
    if (c3.x <= c2.x && c2.x <= c1.x) {
      return true;
    }
  }
  if (c1.y != c3.y) {
    if (c1.y <= c2.y && c2.y <= c3.y) {
      return true;
    }
    if (c3.y <= c2.y && c2.y <= c1.y) {
      return true;
    }
  }
  return false;
}
