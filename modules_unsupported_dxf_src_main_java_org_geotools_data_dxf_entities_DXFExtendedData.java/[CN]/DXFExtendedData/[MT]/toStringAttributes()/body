{
  StringBuilder sb=new StringBuilder();
  sb.append("{");
  for (  Object o : attributes) {
    try {
      if (o instanceof String)       sb.append((String)o);
 else       if (o instanceof Number)       sb.append((Number)o);
 else       sb.append(o.toString());
    }
 catch (    Exception e) {
      log.warn("Parse to string attribute problem. Skipping...");
    }
 finally {
      sb.append(";");
    }
  }
  sb.append("}");
  return sb.toString();
}
