{
  shape=variableDS.getShape();
switch (shape.length) {
case 2:
    numberOfSlices=1;
  break;
case 3:
numberOfSlices=shape[0];
break;
case 4:
numberOfSlices=0 + shape[0] * shape[1];
break;
default :
if (LOGGER.isLoggable(Level.WARNING)) LOGGER.warning("Ignoring variable: " + getName() + " with shape length: "+ shape.length);
}
}
