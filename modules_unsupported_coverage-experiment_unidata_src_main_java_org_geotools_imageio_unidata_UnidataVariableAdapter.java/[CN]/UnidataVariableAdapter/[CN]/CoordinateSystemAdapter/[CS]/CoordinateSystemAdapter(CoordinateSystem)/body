{
  this.cs=cs;
  if (cs.hasVerticalAxis()) {
    vertical=true;
  }
 else {
    Set<String> unsupported=UnidataUtilities.getUnsupportedDimensions();
    boolean present=false;
    for (    String dimension : unsupported) {
      if (cs.containsAxis(dimension)) {
        present=true;
        break;
      }
    }
    if (present) {
      vertical=true;
    }
 else {
      vertical=false;
    }
  }
}
