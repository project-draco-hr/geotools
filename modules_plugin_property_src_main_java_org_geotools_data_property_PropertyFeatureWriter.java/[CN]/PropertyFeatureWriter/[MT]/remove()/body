{
  if (live == null) {
    throw new IOException("No current feature to remove");
  }
  if (origional != null) {
    store.listenerManager.fireFeaturesRemoved(live.getFeatureType().getTypeName(),Transaction.AUTO_COMMIT,ReferencedEnvelope.reference(origional.getBounds()),false);
  }
  origional=null;
  live=null;
}
