{
  float[] testVals={Float.MAX_VALUE,Float.MIN_VALUE,Float.NEGATIVE_INFINITY,Float.POSITIVE_INFINITY,0};
  for (int i=0; i < testVals.length; i++) {
    varRowField=new RowField(new Float(testVals[i]),DATA_SHORT_FLOAT);
    assertEquals("Test RowField for float value: " + testVals[i],testVals[i],varRowField.floatValue(),0);
  }
}
