{
  int index=tiffName.lastIndexOf('$');
  if (index != -1)   tiffName=tiffName.substring(index + 1);
  index=tiffName.lastIndexOf('\n');
  if (index != -1)   tiffName=tiffName.substring(index + 1);
  index=tiffName.lastIndexOf('\r');
  if (index != -1)   tiffName=tiffName.substring(index + 1);
  return tiffName;
}
