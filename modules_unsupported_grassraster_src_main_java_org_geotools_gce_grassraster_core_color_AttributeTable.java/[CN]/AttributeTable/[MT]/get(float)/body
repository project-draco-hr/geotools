{
  int low=0;
  int high=atts.size() - 1;
  while (low <= high) {
    int i=(low + high) / 2;
    CellAttribute catt=(CellAttribute)atts.elementAt(i);
    int c=catt.compare(cat);
    if (c == 0) {
      return catt.getText();
    }
 else     if (c < 0) {
      high=i - 1;
    }
 else {
      low=i++ + 1;
    }
  }
  return null;
}
