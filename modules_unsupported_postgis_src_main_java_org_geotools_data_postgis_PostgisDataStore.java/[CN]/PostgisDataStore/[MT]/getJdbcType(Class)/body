{
  Map.Entry entry;
  Integer jdbcType=null;
  Class binding;
  for (Iterator it=TYPE_MAPPINGS.entrySet().iterator(); it.hasNext(); ) {
    entry=(Map.Entry)it.next();
    binding=(Class)entry.getValue();
    if (binding.equals(attributeTypeBinding)) {
      jdbcType=(Integer)entry.getKey();
      break;
    }
  }
  return jdbcType;
}
