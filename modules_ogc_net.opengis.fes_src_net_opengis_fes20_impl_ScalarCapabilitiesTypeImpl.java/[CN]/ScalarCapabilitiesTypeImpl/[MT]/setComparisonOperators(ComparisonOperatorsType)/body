{
  if (newComparisonOperators != comparisonOperators) {
    NotificationChain msgs=null;
    if (comparisonOperators != null)     msgs=((InternalEObject)comparisonOperators).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Fes20Package.SCALAR_CAPABILITIES_TYPE__COMPARISON_OPERATORS,null,msgs);
    if (newComparisonOperators != null)     msgs=((InternalEObject)newComparisonOperators).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Fes20Package.SCALAR_CAPABILITIES_TYPE__COMPARISON_OPERATORS,null,msgs);
    msgs=basicSetComparisonOperators(newComparisonOperators,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Fes20Package.SCALAR_CAPABILITIES_TYPE__COMPARISON_OPERATORS,newComparisonOperators,newComparisonOperators));
}
