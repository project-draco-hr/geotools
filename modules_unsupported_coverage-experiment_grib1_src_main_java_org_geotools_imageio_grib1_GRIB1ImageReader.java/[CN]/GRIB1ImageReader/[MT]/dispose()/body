{
  super.dispose();
  boundsMap.clear();
  boundsMap=null;
  coordSysMap.clear();
  coordSysMap=null;
  indexMap.clear();
  indexMap=null;
  metadataMap.clear();
  metadataMap=null;
  spatioTemporalMetadataMap.clear();
  spatioTemporalMetadataMap=null;
  numGlobalAttributes=-1;
  numRasters=-1;
  try {
    if (dataset != null) {
      dataset.close();
    }
  }
 catch (  IOException e) {
    if (LOGGER.isLoggable(Level.WARNING))     LOGGER.warning("Errors closing NetCDF dataset." + e.getLocalizedMessage());
  }
 finally {
    dataset=null;
  }
}
