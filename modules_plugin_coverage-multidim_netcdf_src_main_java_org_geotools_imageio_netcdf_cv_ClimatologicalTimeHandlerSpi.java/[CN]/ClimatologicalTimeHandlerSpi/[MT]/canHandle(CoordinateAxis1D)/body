{
  if (axis != null && "time".equalsIgnoreCase(axis.getShortName()) && CLIMATOLOGICAL_UNITS.equalsIgnoreCase(axis.getUnitsString())) {
    if (LOGGER.isLoggable(Level.FINEST)) {
      LOGGER.finest("Climatological Time Handler Spi can handle the axis");
    }
    return true;
  }
  return false;
}
