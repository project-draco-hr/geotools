    msgs=basicSetServiceIdentification(newServiceIdentification,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Ows20Package.CAPABILITIES_BASE_TYPE__SERVICE_IDENTIFICATION,newServiceIdentification,newServiceIdentification));
}
