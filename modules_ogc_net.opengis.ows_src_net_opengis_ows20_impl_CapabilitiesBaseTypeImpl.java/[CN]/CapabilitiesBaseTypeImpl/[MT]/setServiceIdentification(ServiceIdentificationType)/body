{
  if (newServiceIdentification != serviceIdentification) {
    NotificationChain msgs=null;
    if (serviceIdentification != null)     msgs=((InternalEObject)serviceIdentification).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Ows20Package.CAPABILITIES_BASE_TYPE__SERVICE_IDENTIFICATION,null,msgs);
