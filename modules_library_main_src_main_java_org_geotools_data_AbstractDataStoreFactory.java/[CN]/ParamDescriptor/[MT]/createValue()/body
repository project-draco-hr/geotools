{
  if (Double.TYPE.equals(getValueClass())) {
    return new FloatParameter(this){
      protected Object valueOf(      String text) throws IOException {
        return param.handle(text);
      }
    }
;
  }
  return new Parameter(this){
    protected Object valueOf(    String text) throws IOException {
      return param.handle(text);
    }
  }
;
}
