{
  if (wkbGeometryType < 0)   throw new IllegalArgumentException("Can't have a negative type");
  int mask25d=0x80000000;
  int unmaskedType=wkbGeometryType & ~mask25d;
  if (wkbGeometryType >= OGR_GEOM_TYPES.length) {
    LOGGER.warning("Could not recognize geometry type " + wkbGeometryType + ". Assuming it's a new type and handling as a string");
    return Geometry.class;
  }
  return OGR_GEOM_TYPES[unmaskedType];
}
