{
  DataSource ds;
  try {
    ds=getOGRDataSource(FALSE);
  }
 catch (  IOException e) {
    return new String[0];
  }
  String[] typeNames=new String[ds.GetLayerCount()];
  for (int i=0; i < typeNames.length; i++) {
    Layer l=ds.GetLayerByIndex(i);
    typeNames[i]=l.GetName();
    l.delete();
  }
  ds.delete();
  return typeNames;
}
