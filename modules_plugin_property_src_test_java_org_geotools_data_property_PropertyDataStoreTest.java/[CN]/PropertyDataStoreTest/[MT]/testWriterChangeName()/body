{
  PropertyFeatureWriter writer=(PropertyFeatureWriter)store.getFeatureWriter("road");
  int count=0;
  while (writer.hasNext()) {
    SimpleFeature f=writer.next();
    f.setAttribute(1,"name " + (count + 1));
    writer.write();
    count++;
  }
  writer.close();
  assertEquals(5,count);
  assertEquals(5,count("road"));
}
