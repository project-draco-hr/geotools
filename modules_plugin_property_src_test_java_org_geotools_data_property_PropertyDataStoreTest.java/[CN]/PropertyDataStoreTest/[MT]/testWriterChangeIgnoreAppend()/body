{
  PropertyFeatureWriter writer=(PropertyFeatureWriter)store.createFeatureWriter("road",Transaction.AUTO_COMMIT);
  SimpleFeature f;
  writer.next();
  writer.next();
  writer.next();
  writer.next();
  writer.next();
  assertFalse(writer.hasNext());
  f=writer.next();
  assertNotNull(f);
  f.setAttribute(0,new Integer(-1));
  f.setAttribute(1,"new");
  writer.close();
  assertEquals(5,count("road"));
}
