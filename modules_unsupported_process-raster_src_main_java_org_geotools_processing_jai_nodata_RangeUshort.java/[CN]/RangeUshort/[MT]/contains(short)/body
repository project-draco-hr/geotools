{
  final int valueUshort=value & 0xFFFF;
  if (isPoint) {
    return this.minValue == valueUshort;
  }
 else {
    final boolean lower;
    final boolean upper;
    if (minIncluded) {
      lower=valueUshort < minValue;
    }
 else {
      lower=valueUshort <= minValue;
    }
    if (maxIncluded) {
      upper=valueUshort > maxValue;
    }
 else {
      upper=valueUshort >= maxValue;
    }
    return !lower && !upper;
  }
}
