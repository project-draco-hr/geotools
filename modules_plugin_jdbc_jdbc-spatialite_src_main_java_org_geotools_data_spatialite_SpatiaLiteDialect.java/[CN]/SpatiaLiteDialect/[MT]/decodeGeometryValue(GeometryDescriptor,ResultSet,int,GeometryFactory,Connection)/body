{
  String string=rs.getString(column);
  if (string == null || "".equals(string.trim())) {
    return null;
  }
  String[] split=string.split(";");
  String wkt=split[0];
  try {
    return new WKTReader(factory).read(wkt);
  }
 catch (  ParseException e) {
    throw (IOException)new IOException().initCause(e);
  }
}
