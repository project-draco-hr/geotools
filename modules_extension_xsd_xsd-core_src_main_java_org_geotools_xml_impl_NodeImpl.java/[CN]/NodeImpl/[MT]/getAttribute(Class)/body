{
  if (clazz == null) {
    return null;
  }
  for (Iterator itr=attributes.iterator(); itr.hasNext(); ) {
    Node att=(Node)itr.next();
    if (att.getValue() == null) {
      continue;
    }
    if (clazz.isAssignableFrom(att.getValue().getClass())) {
      return att;
    }
  }
  return null;
}
