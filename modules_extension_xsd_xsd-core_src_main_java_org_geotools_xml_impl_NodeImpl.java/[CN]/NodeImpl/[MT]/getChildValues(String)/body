{
  ArrayList matches=new ArrayList();
  if (name == null) {
    return matches;
  }
  for (Iterator itr=children.iterator(); itr.hasNext(); ) {
    Node child=(Node)itr.next();
    if (name.equals(child.getComponent().getName())) {
      matches.add(child.getValue());
    }
  }
  return matches;
}
