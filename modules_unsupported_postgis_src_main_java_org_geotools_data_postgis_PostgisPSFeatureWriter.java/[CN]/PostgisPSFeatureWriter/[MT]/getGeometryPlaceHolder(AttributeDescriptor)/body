{
  String placeHolder;
  final FeatureTypeInfo ftInfo=queryData.getFeatureTypeInfo();
  final int srid=ftInfo.getSRID(type.getLocalName());
  if (WKBEnabled) {
    if (byteaWKB) {
      placeHolder="setSRID( ?::geometry," + srid + ")";
    }
 else {
      placeHolder="GeomFromWKB( ? , " + srid + ")";
    }
  }
 else {
    placeHolder="GeometryFromText( ?, " + srid + ")";
  }
  return placeHolder;
}
