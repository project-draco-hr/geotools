{
  OGR ogr=createOGR();
  Object dataset=ogr.OpenShared(ogrName,0);
  if (dataset != null) {
    ogr.DataSourceRelease(dataset);
    return true;
  }
  if (driverName != null) {
    try {
      Object driver=ogr.GetDriverByName(driverName);
      if (driver != null) {
        return true;
      }
    }
 catch (    Exception e) {
      LOGGER.log(Level.FINE,"Error loading driver",e);
    }
  }
  return false;
}
