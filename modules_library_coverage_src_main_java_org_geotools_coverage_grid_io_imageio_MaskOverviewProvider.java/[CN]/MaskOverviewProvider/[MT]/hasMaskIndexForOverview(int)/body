{
  if (imageIndex > 0) {
    if (imageIndex < numInternalMasks) {
      return true;
    }
 else     if (hasExternalMasks && imageIndex <= (numExternalMasks + numExternalMasksOverviews - 1)) {
      return true;
    }
  }
 else   if (imageIndex == 0 && numInternalMasks > 0 || hasExternalMasks) {
    return true;
  }
  return false;
}
