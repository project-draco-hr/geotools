{
  String st=name.getLocalPart();
  if (st.equals(TBL_SYNCH_UNITS) || st.equals(TBL_SYNCH_TABLES) || st.equals(TBL_SYNCH_UNIT_TABLES)|| st.equals(TBL_SYNCH_OUTSTANDING)|| st.equals(TBL_SYNCH_CONFLICTS)|| st.equals(TBL_SYNCH_HISTORY))   throw new IOException(st + " is a protected type.  Hands off.");
  return dataStore.getSchema(name);
}
