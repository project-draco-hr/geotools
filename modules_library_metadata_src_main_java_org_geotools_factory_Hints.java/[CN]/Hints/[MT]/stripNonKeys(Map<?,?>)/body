{
  if (hints == null) {
    return null;
  }
  @SuppressWarnings("unchecked") Map<RenderingHints.Key,Object> filtered=(Map)hints;
  for (final Iterator it=hints.keySet().iterator(); it.hasNext(); ) {
    final Object key=it.next();
    if (!(key instanceof RenderingHints.Key)) {
      if (filtered == hints) {
        filtered=new HashMap<RenderingHints.Key,Object>(filtered);
      }
      filtered.remove(key);
    }
  }
  return filtered;
}
