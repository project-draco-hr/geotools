{
  if (feature instanceof SimpleFeature && !validate((SimpleFeature)feature))   return false;
  Geometry left;
  Geometry right;
switch (literals) {
case BOTH:
    return cacheValue;
case RIGHT:
{
    left=getLeftGeometry(feature);
    right=getRightGeometry(feature);
    return preppedEvaluate(rightPreppedGeom,left);
  }
case LEFT:
{
  left=getLeftGeometry(feature);
  right=getRightGeometry(feature);
  return preppedEvaluate(leftPreppedGeom,right);
}
default :
{
left=getLeftGeometry(feature);
right=getRightGeometry(feature);
return basicEvaluate(left,right);
}
}
}
