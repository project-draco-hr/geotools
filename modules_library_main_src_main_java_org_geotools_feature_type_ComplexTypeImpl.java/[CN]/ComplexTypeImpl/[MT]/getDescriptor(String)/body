{
  PropertyDescriptor result=getDescriptor(new NameImpl(name));
  if (result == null) {
    result=getDescriptor(new NameImpl(getName().getNamespaceURI(),name));
    if (result == null) {
      for (      PropertyDescriptor pd : properties) {
        if (pd.getName().getLocalPart().equals(name)) {
          return pd;
        }
      }
    }
  }
  return result;
}
