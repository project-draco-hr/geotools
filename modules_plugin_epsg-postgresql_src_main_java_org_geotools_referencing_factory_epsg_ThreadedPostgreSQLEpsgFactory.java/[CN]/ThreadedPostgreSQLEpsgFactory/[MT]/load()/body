{
  final Properties p=new Properties();
  File file=new File(CONFIGURATION_FILE);
  if (!file.isFile()) {
    File home=new File(System.getProperty("user.home","."));
    file=new File(home,CONFIGURATION_FILE);
    if (!file.isFile()) {
      File epsgFixtures=new File(new File(home,".geotools"),"epsg");
      file=new File(epsgFixtures,"postgresql.properties");
      if (!file.isFile()) {
        return p;
      }
    }
  }
  try {
    final InputStream in=new FileInputStream(file);
    p.load(in);
    in.close();
  }
 catch (  IOException exception) {
    Logging.unexpectedException("org.geotools.referencing.factory",DataSource.class,"<init>",exception);
  }
  return p;
}
