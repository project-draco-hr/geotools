{
  createReporter();
  StringBuilder sb=new StringBuilder();
  sb.append("\n");
  sb.append(layerName);
  sb.append("\n");
  int k=1;
  for (  Number value : bandValues) {
    sb.append(String.format("  Band %d: %s\n",k++,value.toString()));
  }
  sb.append("\n\n");
  reporter.append(sb.toString());
}
