{
  Filter f=query.getFilter();
  if (isAll(f)) {
    LOG.fine("count(all)");
    return (int)collection.count();
  }
  Filter[] split=splitFilter(f);
  if (!isAll(split[1])) {
    return -1;
  }
  DBObject q=toQuery(f);
  if (LOG.isLoggable(Level.FINE)) {
    LOG.fine("count(" + q + ")");
  }
  return (int)collection.count(q);
}
