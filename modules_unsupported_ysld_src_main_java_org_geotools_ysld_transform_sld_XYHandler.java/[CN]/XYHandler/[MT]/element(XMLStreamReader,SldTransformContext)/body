{
  String name=xml.getLocalName();
  if (name.endsWith("X")) {
    context.push(new ExpressionHandler(){
      @Override protected SldTransformContext onValue(      String value,      SldTransformContext context) throws IOException {
        x=value;
        return context;
      }
    }
);
  }
  if (name.endsWith("Y")) {
    context.push(new ExpressionHandler(){
      @Override protected SldTransformContext onValue(      String value,      SldTransformContext context) throws IOException {
        y=value;
        return context;
      }
    }
);
  }
}
