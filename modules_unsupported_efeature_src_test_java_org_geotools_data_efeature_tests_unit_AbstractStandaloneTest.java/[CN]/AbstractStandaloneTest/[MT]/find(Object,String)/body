{
  EObject result=null;
  Object current=offset;
  String[] ids=tokenize(id);
  for (int i=0; (current != null) && (i < ids.length); i++) {
    id=ids[i];
    result=null;
    for (    EObject it : getContents(current)) {
      if (id.equals(EcoreUtil.getID(it))) {
        result=it;
        break;
      }
    }
    current=result;
  }
  return result;
}
