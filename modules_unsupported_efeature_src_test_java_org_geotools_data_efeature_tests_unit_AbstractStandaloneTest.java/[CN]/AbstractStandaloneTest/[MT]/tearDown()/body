{
  try {
    doTearDown();
    if (eResource != null) {
      if (eResource.isLoaded()) {
        eResource.unload();
      }
      if (eResource.getResourceSet() != null) {
        eResource.getResourceSet().getResources().remove(eResource);
      }
      eResource=null;
    }
    eDomain=null;
    eResource=null;
    eResourceURI=null;
    eResourceSet=null;
  }
  finally {
    trace("===> End   : " + getName(),TIME_TOTAL);
    EFeatureLogFormatter.setStandard();
  }
}
