{
  if (GpxUtils.isValidLongitudeValue(argValue) == true) {
    if ((argValue - 0.0) < argTolerance) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    IllegalArgumentException toThrow=new IllegalArgumentException(" The longitude value being tested was not valid.");
    throw toThrow;
  }
}
