{
  try {
    return new MrSIDReader(source,hints);
  }
 catch (  MismatchedDimensionException e) {
    final RuntimeException re=new RuntimeException();
    re.initCause(e);
    throw re;
  }
catch (  DataSourceException e) {
    final RuntimeException re=new RuntimeException();
    re.initCause(e);
    throw re;
  }
}
