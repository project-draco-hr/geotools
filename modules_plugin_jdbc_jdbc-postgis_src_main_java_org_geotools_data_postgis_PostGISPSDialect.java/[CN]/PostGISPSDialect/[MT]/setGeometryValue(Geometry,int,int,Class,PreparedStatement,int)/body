{
  if (g != null && !g.isEmpty()) {
    if (g instanceof LinearRing) {
      g=g.getFactory().createLineString(((LinearRing)g).getCoordinateSequence());
    }
    byte[] bytes=new WKBWriter(dimension).write(g);
    ps.setBytes(column,bytes);
  }
 else {
    ps.setNull(column,Types.OTHER,"Geometry");
  }
}
