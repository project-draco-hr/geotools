{
  if (source == null) {
    return false;
  }
  URL sourceUrl=getURLFromSource(source);
  if (sourceUrl == null) {
    return false;
  }
  if ((sourceUrl.getPath().endsWith(".xml") == false) && (sourceUrl.getPath().endsWith(".XML") == false))   return false;
  ByteArrayOutputStream out=new ByteArrayOutputStream();
  try {
    InputStream in=(InputStream)sourceUrl.getContent();
    int c;
    while ((c=in.read()) != -1)     out.write(c);
    in.close();
    out.close();
  }
 catch (  IOException e) {
    return false;
  }
  return out.toString().indexOf("coverageName") != -1;
}
