{
  if (GML._Geometry.equals(name) || GML.Point.equals(name) || GML.LineString.equals(name)|| GML.Polygon.equals(name)) {
    if (isEmpty(geometry)) {
      return null;
    }
    return geometry;
  }
  if (XLINK.HREF.equals(name)) {
    if (isEmpty(geometry)) {
      String id=GML3EncodingUtils.getID(geometry);
      if (id != null) {
        return "#" + id;
      }
    }
  }
  return null;
}
