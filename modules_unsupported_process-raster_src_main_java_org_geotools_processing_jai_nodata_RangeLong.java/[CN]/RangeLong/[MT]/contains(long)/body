{
  if (isPoint) {
    return this.minValue == value;
  }
 else {
    final boolean lower;
    final boolean upper;
    if (minIncluded) {
      lower=value < minValue;
    }
 else {
      lower=value <= minValue;
    }
    if (maxIncluded) {
      upper=value > maxValue;
    }
 else {
      upper=value >= maxValue;
    }
    return !lower && !upper;
  }
}
