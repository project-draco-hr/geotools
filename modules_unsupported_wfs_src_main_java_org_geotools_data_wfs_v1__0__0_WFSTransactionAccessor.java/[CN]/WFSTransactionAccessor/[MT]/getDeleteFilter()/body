{
  List l=actions;
  Iterator i=l.iterator();
  Filter deleteFilter=null;
  while (i.hasNext()) {
    Action a=(Action)i.next();
    if (a.getType() == Action.DELETE) {
      if (deleteFilter == null)       deleteFilter=a.getFilter();
 else       deleteFilter=ff.and(deleteFilter,a.getFilter());
    }
  }
  return deleteFilter;
}
