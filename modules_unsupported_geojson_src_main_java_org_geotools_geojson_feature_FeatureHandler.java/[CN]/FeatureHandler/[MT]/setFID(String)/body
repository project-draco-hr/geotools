{
  int index=f.lastIndexOf('.');
  if (index < 0) {
    index=f.indexOf('-');
    if (index >= 0) {
      separator="-";
    }
  }
 else {
    separator=".";
  }
  baseId=f.substring(0,index);
  try {
    fid=Integer.parseInt(f.substring(index + 1));
  }
 catch (  NumberFormatException e) {
    fid=0;
  }
}
