{
  if (transaction != null) {
    this.transaction=transaction;
  }
 else {
    this.transaction=null;
    if (typeNameDiff != null) {
      for (Iterator i=typeNameDiff.values().iterator(); i.hasNext(); ) {
        Diff diff=(Diff)i.next();
        diff.clear();
      }
      typeNameDiff.clear();
    }
    store=null;
  }
}
