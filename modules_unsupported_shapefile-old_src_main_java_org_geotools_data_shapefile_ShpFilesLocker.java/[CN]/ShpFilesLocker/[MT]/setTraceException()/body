{
  String name=Thread.currentThread().getName();
  String type, id;
  if (reader != null) {
    type="read";
    id=reader.id();
  }
 else {
    type="write";
    id=writer.id();
  }
  if (TRACE_ENABLED) {
    trace=new Trace("Locking " + url + " for "+ type+ " by "+ id+ " in thread "+ name);
  }
}
