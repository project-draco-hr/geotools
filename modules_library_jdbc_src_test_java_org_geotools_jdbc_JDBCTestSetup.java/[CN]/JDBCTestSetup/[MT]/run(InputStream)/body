{
  BufferedReader reader=new BufferedReader(new InputStreamReader(script));
  Connection conn=getConnection();
  try {
    Statement st=conn.createStatement();
    try {
      String line=null;
      while ((line=reader.readLine()) != null) {
        LOGGER.fine(line);
        st.execute(line);
      }
      reader.close();
    }
  finally {
      st.close();
    }
  }
  finally {
    conn.close();
  }
}
