{
  if (updateAction.getFilter().evaluate(action.getFeature())) {
    updateAction.update(action.getFeature());
    if (updateAction.getFilter() instanceof FidFilter && ((FidFilter)updateAction.getFilter()).getFids().length == 1) {
      actions.remove(i + 1);
      return i;
    }
  }
  swap(actions,i);
  return i + 1;
}
