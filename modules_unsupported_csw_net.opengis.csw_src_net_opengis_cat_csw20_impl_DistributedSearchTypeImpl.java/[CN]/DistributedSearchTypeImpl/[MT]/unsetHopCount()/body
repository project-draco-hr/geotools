{
  BigInteger oldHopCount=hopCount;
  boolean oldHopCountESet=hopCountESet;
  hopCount=HOP_COUNT_EDEFAULT;
  hopCountESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,Csw20Package.DISTRIBUTED_SEARCH_TYPE__HOP_COUNT,oldHopCount,HOP_COUNT_EDEFAULT,oldHopCountESet));
}
