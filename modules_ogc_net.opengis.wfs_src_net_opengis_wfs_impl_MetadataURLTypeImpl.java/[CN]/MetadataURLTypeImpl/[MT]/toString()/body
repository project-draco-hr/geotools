{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (value: ");
  result.append(value);
  result.append(", format: ");
  result.append(format);
  result.append(", type: ");
  result.append(type);
  result.append(')');
  return result.toString();
}
