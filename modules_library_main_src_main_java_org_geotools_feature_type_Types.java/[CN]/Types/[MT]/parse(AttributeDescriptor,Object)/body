{
  if (value == null) {
    if (descriptor.isNillable()) {
      return descriptor.getDefaultValue();
    }
  }
 else {
    Class target=descriptor.getType().getBinding();
    if (!target.isAssignableFrom(value.getClass())) {
      Object converted=Converters.convert(value,target);
      if (converted != null) {
        return converted;
      }
    }
  }
  return value;
}
