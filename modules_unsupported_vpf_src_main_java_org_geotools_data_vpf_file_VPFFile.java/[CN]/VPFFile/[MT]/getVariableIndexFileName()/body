{
  String result=null;
  String fileName=getFileName();
  if (fileName.toLowerCase().equals(FEATURE_CLASS_SCHEMA_TABLE)) {
    result=getDirectoryName().concat(File.separator).concat("fcz");
  }
 else {
    result=getDirectoryName().concat(File.separator).concat(fileName.substring(0,fileName.length() - 1) + "x");
  }
  return result;
}
