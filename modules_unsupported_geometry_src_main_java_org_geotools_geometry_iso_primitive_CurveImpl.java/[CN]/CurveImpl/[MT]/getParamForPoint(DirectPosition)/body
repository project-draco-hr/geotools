    actDistanceSquare=AlgoPointND.getDistanceSquare(p.getCoordinates(),paramForPoints1.getPosition().getCoordinates());
    if (actDistanceSquare <= minDistanceSquare) {
    }
  }
  return paramForPoints;
}
