{
  String text;
  if (tile != null) {
    text=tile.toString();
  }
 else {
    text=super.toString();
    text=text.substring(text.lastIndexOf('.') + 1);
  }
  if (!isLeaf()) {
    final StringBuilder buffer=new StringBuilder(text).append(" (").append(getChildCount()).append(" childs");
    if (hasOverlaps()) {
      buffer.append(", overlaps");
    }
    text=buffer.append(')').toString();
  }
  return text;
}
