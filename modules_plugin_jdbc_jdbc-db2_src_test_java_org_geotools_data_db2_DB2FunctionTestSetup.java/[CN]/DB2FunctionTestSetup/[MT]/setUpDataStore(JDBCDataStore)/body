{
  super.setUpDataStore(dataStore);
  SQLDialect dialect=dataStore.getSQLDialect();
  if (dialect instanceof DB2SQLDialectBasic)   ((DB2SQLDialectBasic)dialect).setFunctionEncodingEnabled(true);
  if (dialect instanceof DB2SQLDialectPrepared)   ((DB2SQLDialectPrepared)dialect).setFunctionEncodingEnabled(true);
}
