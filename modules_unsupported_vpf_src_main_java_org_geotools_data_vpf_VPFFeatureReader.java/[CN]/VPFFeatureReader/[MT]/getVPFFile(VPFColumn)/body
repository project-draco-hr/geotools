{
  String columnName=column.getName();
  VPFFile result=null;
  VPFFile temp;
  Iterator<VPFFile> iter=featureType.getFeatureClass().getFileList().iterator();
  while (iter.hasNext()) {
    temp=(VPFFile)iter.next();
    if ((temp != null) && (temp.getColumn(columnName) != null)) {
      result=temp;
      break;
    }
  }
  return result;
}
