{
  final ParameterValue<Integer> dim=new Parameter<Integer>(Provider.DIM);
  dim.setValue(getSourceDimensions());
  return new ParameterGroup(getParameterDescriptors(),new ParameterValue[]{dim,new FloatParameter(Provider.DX,dx),new FloatParameter(Provider.DY,dy),new FloatParameter(Provider.DZ,dz),new FloatParameter(Provider.SRC_SEMI_MAJOR,a),new FloatParameter(Provider.SRC_SEMI_MINOR,b),new FloatParameter(Provider.TGT_SEMI_MAJOR,a + da),new FloatParameter(Provider.TGT_SEMI_MINOR,b + db)});
}
