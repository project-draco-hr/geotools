{
  this.abridged=abridged;
  this.source3D=source3D;
  this.target3D=target3D;
  this.dx=dx;
  this.dy=dy;
  this.dz=dz;
  this.a=a;
  this.b=b;
  da=ta - a;
  db=tb - b;
  a_b=a / b;
  b_a=b / a;
  daa=da * a;
  da_a=da / a;
  df=(ta - tb) / ta - (a - b) / a;
  e2=1 - (b * b) / (a * a);
  adf=(a * df) + (a - b) * da / a;
}
