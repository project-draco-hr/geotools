{
  if (unset) {
    return null;
  }
  StyledLayerDescriptor sld=sf.createStyledLayerDescriptor();
  sld.setName(name);
  sld.setTitle(title);
  sld.setAbstract(sldAbstract);
  for (  AbstractSLDBuilder<? extends StyledLayer> builder : layers) {
    sld.addStyledLayer(builder.build());
  }
  reset();
  return sld;
}
