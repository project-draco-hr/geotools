{
  Element[] elems=all.getElements();
  int[] r=new int[elems.length];
  for (int i=0; i < r.length; i++)   r[i]=0;
  boolean c=true;
  int head=index;
  while (c) {
    c=false;
    for (int i=0; i < elems.length; i++) {
      if (elems[i].getType().getName().equalsIgnoreCase(((XMLElementHandler)elements.get(head)).getName())) {
        r[i]++;
        head++;
        i=elems.length;
        c=true;
      }
    }
  }
  for (int i=0; i < r.length; i++) {
    if ((r[i] < elems[i].getMinOccurs()) || (r[i] > elems[i].getMaxOccurs())) {
      return new int[]{index,0};
    }
  }
  return new int[]{head,1};
}
