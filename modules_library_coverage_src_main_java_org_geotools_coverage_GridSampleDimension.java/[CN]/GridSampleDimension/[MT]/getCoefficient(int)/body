{
  if (!hasQuantitative) {
    return order;
  }
  Exception cause=null;
  if (sampleToGeophysics != null)   try {
    final double value;
switch (order) {
case 0:
      value=sampleToGeophysics.transform(0);
    break;
case 1:
  value=sampleToGeophysics.derivative(Double.NaN);
break;
default :
throw new AssertionError(order);
}
if (!Double.isNaN(value)) {
return value;
}
}
 catch (TransformException exception) {
cause=exception;
}
throw new IllegalStateException(Errors.format(ErrorKeys.NON_LINEAR_RELATION),cause);
}
