{
  StringBuffer s=new StringBuffer();
  s.append("DXFInsert [");
  s.append("x: ");
  s.append(x + ", ");
  s.append("y: ");
  s.append(y + ", ");
  s.append("visibility: ");
  s.append(visibility + ", ");
  s.append("color: ");
  s.append(c + ", ");
  s.append("line type: ");
  if (lineType != null) {
    s.append(lineType._name);
  }
  s.append("]");
  return s.toString();
}
