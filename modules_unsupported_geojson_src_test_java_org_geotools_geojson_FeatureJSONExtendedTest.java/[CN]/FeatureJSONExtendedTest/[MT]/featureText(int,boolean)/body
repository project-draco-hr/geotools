{
  String text="{" + "  'type': 'Feature'," + "  'geometry': {"+ "     'type': 'Point',"+ "     'coordinates': [" + (val + 0.1) + ","+ (val + 0.1)+ "]"+ "   }, "+ "'  properties': {"+ "     'int': "+ val+ ","+ "     'double': "+ (val + 0.1)+ ",";
  if (!excludeString) {
    text+="     'string': '" + toString(val) + "',";
  }
  text+="     'date': '" + toDateString(val) + "'"+ "   },"+ "   'id':'feature."+ val+ "'"+ "}";
  return text;
}
