{
  if (crs != null) {
    final int expected=getDimension();
    final int dimension=crs.getCoordinateSystem().getDimension();
    if (dimension != expected) {
      throw new MismatchedDimensionException(Errors.format(ErrorKeys.MISMATCHED_DIMENSION_$3,crs.getName().getCode(),new Integer(dimension),new Integer(expected)));
    }
  }
}
