{
  if (other instanceof BoundingBox) {
    if (other.isNull()) {
      return;
    }
    BoundingBox bbox=(BoundingBox)other;
    ensureCompatibleReferenceSystem(bbox);
    expandToInclude(bbox.getLowerCorner());
    expandToInclude(bbox.getUpperCorner());
  }
 else {
    super.expandToInclude(other);
  }
}
