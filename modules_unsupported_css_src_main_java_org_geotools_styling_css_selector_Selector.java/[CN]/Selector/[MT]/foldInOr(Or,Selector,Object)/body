{
  List<Selector> newChildren=new ArrayList<>();
  for (  Selector s : or.getChildren()) {
    Selector combined=and(s,anded,context);
    if (combined == ACCEPT) {
      return ACCEPT;
    }
 else     if (combined != REJECT) {
      newChildren.add(combined);
    }
  }
  if (newChildren.size() == 0) {
    return REJECT;
  }
  return new Or(newChildren);
}
