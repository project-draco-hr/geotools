{
  Map<Class,List<Selector>> result=new LinkedHashMap<>();
  for (  Selector s : selectors) {
    Class<? extends Selector> clazz=s.getClass();
    List<Selector> list=result.get(clazz);
    if (list == null) {
      list=new ArrayList<>();
      result.put(clazz,list);
    }
    list.add(s);
  }
  return result;
}
