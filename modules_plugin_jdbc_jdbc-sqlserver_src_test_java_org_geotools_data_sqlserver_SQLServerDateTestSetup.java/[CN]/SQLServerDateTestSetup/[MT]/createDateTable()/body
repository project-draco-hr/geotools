{
  run("CREATE TABLE dates (d DATE, dt DATETIME, t TIME)");
  run("INSERT INTO dates VALUES (" + "CAST('2009-06-28' as DATE), " + "CAST('2009-06-28T15:12:41' as DATETIME),"+ "CAST('15:12:41' as TIME)  )");
  run("INSERT INTO dates VALUES (" + "CAST('2009-01-15' as DATE), " + "CAST('2009-01-15T13:10:12' as DATETIME),"+ "CAST('13:10:12' as TIME)  )");
  run("INSERT INTO dates VALUES (" + "CAST('2009-09-29' as DATE), " + "CAST('2009-09-29T17:54:23' as DATETIME),"+ "CAST('17:54:23' as TIME)  )");
}
