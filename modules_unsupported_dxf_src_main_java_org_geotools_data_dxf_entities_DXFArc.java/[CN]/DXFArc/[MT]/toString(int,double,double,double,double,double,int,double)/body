{
  StringBuffer s=new StringBuffer();
  s.append("DXFArc [");
  s.append("color: ");
  s.append(c + ", ");
  s.append("r: ");
  s.append(r + ", ");
  s.append("x: ");
  s.append(x + ", ");
  s.append("y: ");
  s.append(y + ", ");
  s.append("a1: ");
  s.append(a1 + ", ");
  s.append("a2: ");
  s.append(a2 + ", ");
  s.append("visibility: ");
  s.append(visibility + ", ");
  s.append("thickness: ");
  s.append(thickness);
  s.append("]");
  return s.toString();
}
