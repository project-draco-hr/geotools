{
  try {
    inputStream=url.openStream();
  }
 catch (  IOException e) {
    return false;
  }
  try {
    LOGGER.finest("inputstream is " + inputStream);
    return (inputStream != null) && (inputStream.available() > 0);
  }
 catch (  IOException e) {
    LOGGER.throwing(this.getClass().getName(),"isConnected",e);
    return false;
  }
}
