{
  if ((a.equals(temp1)) || (b.equals(temp1))) {
    return 1;
  }
 else   if ((a.equals(temp2)) || (b.equals(temp2))) {
    return 2;
  }
 else   if ((a.equals(temp3)) || (b.equals(temp3))) {
    return 3;
  }
 else {
    throw new RuntimeException("I shouldn't be here.  Either node a or node b should be temporary.");
  }
}
