{
  String eName=eInfo.getClass().getSimpleName();
  assertTrue(eName + "#isAvailable mismatch: " + "expected "+ isAvailable+ ", found "+ eInfo.isAvailable(),eInfo.isAvailable() == isAvailable);
  assertTrue(eName + "#isValid mismatch: " + "expected "+ isValid+ ", found "+ eInfo.isAvailable(),eInfo.isValid() == isValid);
  assertTrue(eName + "isDisposed mismatch: " + "expected "+ isDisposed+ ", found "+ eInfo.isDisposed(),eInfo.isDisposed() == isDisposed);
}
