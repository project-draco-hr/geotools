{
  String yaml="feature-styles: \n" + "- transform:\n" + "    input: foo\n"+ "    name: ras:Contour\n"+ "    params:\n"+ "      levels:\n"+ "      - 1000\n"+ "      - 1100\n"+ "      - 1200\n";
  StyledLayerDescriptor sld=Ysld.parse(yaml);
  FeatureTypeStyle fs=SLD.defaultStyle(sld).featureTypeStyles().get(0);
  Expression tx=fs.getTransformation();
  assertNotNull(tx);
  ProcessFunction pf=(ProcessFunction)tx;
  assertThat(pf,hasProperty("parameters",containsInAnyOrder(rtParam("foo"),rtParam("levels",literal(1000),literal(1100),literal(1200)))));
}
