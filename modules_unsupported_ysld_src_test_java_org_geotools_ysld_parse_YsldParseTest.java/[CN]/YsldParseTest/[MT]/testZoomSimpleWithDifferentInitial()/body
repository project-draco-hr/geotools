{
  String yaml="grid:\n" + "  initial-scale: 5000000\n" + "  initial-level: 3\n"+ "feature-styles: \n"+ "- name: name\n"+ "  rules:\n"+ "  - zoom: " + tuple(0,0) + "\n"+ "  - zoom: "+ tuple(3,3)+ "\n";
  StyledLayerDescriptor sld=Ysld.parse(yaml);
  FeatureTypeStyle fs=SLD.defaultStyle(sld).featureTypeStyles().get(0);
  assertThat((Iterable<Rule>)fs.rules(),hasItems(allOf(appliesToScale(5000000 * 8),not(appliesToScale(5000000 / 2 * 8)),not(appliesToScale(5000000 * 2 * 8))),allOf(appliesToScale(5000000 * 8),not(appliesToScale(5000000 / 2 * 8)),not(appliesToScale(5000000 * 2 * 8)))));
}
