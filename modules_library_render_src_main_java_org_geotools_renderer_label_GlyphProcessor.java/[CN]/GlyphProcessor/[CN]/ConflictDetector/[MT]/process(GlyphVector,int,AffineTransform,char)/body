{
  Rectangle2D labelEnvelope=tx.createTransformedShape(glyphVector.getGlyphLogicalBounds(g)).getBounds2D();
  if (Character.isWhitespace(c))   return false;
 else   if ((displayArea.contains(labelEnvelope) || labelItem.isPartialsEnabled()) && !(labelItem.isConflictResolutionEnabled() && paintedBounds.labelsWithinDistance(labelEnvelope,extraSpace)) && !groupLabels.labelsWithinDistance(labelEnvelope,minDistance))   return false;
 else   return true;
}
