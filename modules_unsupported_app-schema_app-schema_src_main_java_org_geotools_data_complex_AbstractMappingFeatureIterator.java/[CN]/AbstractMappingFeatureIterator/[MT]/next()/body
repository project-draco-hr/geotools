{
  if (!isHasNextCalled()) {
    LOGGER.warning("hasNext not called before calling next() in the iterator!");
    if (!hasNext()) {
      return null;
    }
  }
  Feature next;
  try {
    next=computeNext();
  }
 catch (  IOException e) {
    close();
    throw new RuntimeException(e);
  }
  ++featureCounter;
  return next;
}
