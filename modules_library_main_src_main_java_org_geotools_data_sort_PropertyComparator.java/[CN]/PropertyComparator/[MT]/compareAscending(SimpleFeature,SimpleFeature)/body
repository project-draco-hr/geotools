{
  Comparable o1=(Comparable)f1.getAttribute(propertyName);
  Comparable o2=(Comparable)f2.getAttribute(propertyName);
  if (o1 == null) {
    if (o2 == null) {
      return 0;
    }
 else {
      return -1;
    }
  }
 else   if (o2 == null) {
    return 1;
  }
 else {
    return o1.compareTo(o2);
  }
}
