{
  if (types != null && types.length != 0) {
    if (bandDimensionTypes == null) {
      bandDimensionTypes=new HashSet<AxisType>();
    }
 else {
      bandDimensionTypes.clear();
    }
    for (int i=0; i < types.length; i++) {
      bandDimensionTypes.add(types[i]);
    }
  }
 else {
    bandDimensionTypes=null;
  }
}
