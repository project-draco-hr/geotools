{
  RenderedImage renderedImage=coverage2D.getRenderedImage();
  System.out.println("Image dims: " + renderedImage.getWidth() + "/"+ renderedImage.getHeight());
  RectIter iter=RectIterFactory.create(renderedImage,new Rectangle(0,0,renderedImage.getWidth(),renderedImage.getHeight()));
  for (int i=0; i < renderedImage.getHeight(); i++) {
    for (int j=0; j < renderedImage.getWidth(); j++) {
      System.out.print(iter.getSampleDouble() + " \t");
      iter.nextPixel();
    }
    System.out.println();
  }
}
