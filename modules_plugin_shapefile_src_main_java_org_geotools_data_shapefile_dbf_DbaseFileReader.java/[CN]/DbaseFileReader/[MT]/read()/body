{
  boolean foundRecord=false;
  while (!foundRecord) {
    bufferCheck();
    final char deleted=(char)buffer.get();
    if (deleted == '*') {
      continue;
    }
    buffer.limit(buffer.position() + header.getRecordLength() - 1);
    buffer.get(bytes);
    buffer.limit(buffer.capacity());
    foundRecord=true;
  }
  cnt++;
}
