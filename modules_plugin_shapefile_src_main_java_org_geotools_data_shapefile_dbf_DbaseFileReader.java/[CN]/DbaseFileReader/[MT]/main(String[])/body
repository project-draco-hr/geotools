{
  final DbaseFileReader reader=new DbaseFileReader(new ShpFiles(args[0]),false,Charset.forName("ISO-8859-1"));
  System.out.println(reader.getHeader());
  int r=0;
  while (reader.hasNext()) {
    System.out.println(++r + "," + java.util.Arrays.asList(reader.readEntry()));
  }
  reader.close();
}
