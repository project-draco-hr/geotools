{
  if (entry.length - offset < header.getNumFields()) {
    throw new ArrayIndexOutOfBoundsException();
  }
  read();
  if (row.deleted) {
    return null;
  }
  final int numFields=header.getNumFields();
  for (int j=0; j < numFields; j++) {
    entry[j + offset]=readObject(fieldOffsets[j],j);
  }
  return entry;
}
