{
  ArrayList<Filter> list=new ArrayList<Filter>();
  if (filter1 == null) {
  }
 else   if (filter1 instanceof Or) {
    Or some=(Or)filter1;
    list.addAll(some.getChildren());
  }
 else {
    list.add(filter1);
  }
  if (filter2 == null) {
  }
 else   if (filter2 instanceof Or) {
    Or more=(Or)filter2;
    list.addAll(more.getChildren());
  }
 else {
    list.add(filter2);
  }
  if (list.size() == 0) {
    return Filter.EXCLUDE;
  }
 else   if (list.size() == 1) {
    return list.get(0);
  }
 else {
    return ff.or(list);
  }
}
