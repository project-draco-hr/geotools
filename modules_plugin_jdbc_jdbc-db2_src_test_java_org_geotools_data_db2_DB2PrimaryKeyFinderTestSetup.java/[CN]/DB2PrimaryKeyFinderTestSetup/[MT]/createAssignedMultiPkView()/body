  con.prepareStatement("INSERT INTO \"" + DB2TestUtil.SCHEMA.toUpperCase() + "\".gt_pk_metadata VALUES"+ "('"+ DB2TestUtil.SCHEMA+ "', 'assignedmultipk', 'key2', 1, 'assigned', NULL)").execute();
  con.close();
}
