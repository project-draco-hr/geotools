{
  Map<String,Serializable> map=new HashMap<String,Serializable>();
  map.put(OGRDataStoreFactory.OGR_NAME.key,getAbsolutePath(STATE_POP));
  DataStore ds=null;
  try {
    ds=DataStoreFinder.getDataStore(map);
    assertNotNull(ds);
    assertTrue(ds instanceof OGRDataStore);
  }
  finally {
    disposeQuietly(ds);
  }
}
