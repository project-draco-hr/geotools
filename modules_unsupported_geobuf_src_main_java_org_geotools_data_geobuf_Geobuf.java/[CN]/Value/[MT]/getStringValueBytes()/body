{
  Object ref="";
  if (valueTypeCase_ == 1) {
    ref=valueType_;
  }
  if (ref instanceof String) {
    com.google.protobuf.ByteString b=com.google.protobuf.ByteString.copyFromUtf8((String)ref);
    if (valueTypeCase_ == 1) {
      valueType_=b;
    }
    return b;
  }
 else {
    return (com.google.protobuf.ByteString)ref;
  }
}
