{
  if (index < 0 || index >= childCount()) {
    throw new IndexOutOfBoundsException(index + "out of range");
  }
  while (children.size() <= index) {
    children.add(null);
  }
  T candidate=children.get(index);
  if (candidate == null) {
    candidate=newChild(index);
    children.set(index,candidate);
  }
  return candidate;
}
