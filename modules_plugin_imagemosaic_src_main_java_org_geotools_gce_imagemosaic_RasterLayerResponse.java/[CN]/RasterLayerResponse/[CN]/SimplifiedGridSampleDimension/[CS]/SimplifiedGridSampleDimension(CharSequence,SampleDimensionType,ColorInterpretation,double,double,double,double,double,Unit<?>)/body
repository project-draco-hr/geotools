{
  super(description,!Double.isNaN(nodata) ? new Category[]{new Category(Vocabulary.formatInternational(VocabularyKeys.NODATA),new Color[]{new Color(0,0,0,0)},NumberRange.create(nodata,nodata))} : null,unit);
  this.nodata=nodata;
  this.minimum=minimum;
  this.maximum=maximum;
  this.scale=scale;
  this.offset=offset;
  this.unit=unit;
  this.type=type;
  this.color=color;
}
