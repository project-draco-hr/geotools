{
  FeatureWriter<SimpleFeatureType,SimpleFeature> writer=data.getFeatureWriter(getRoadTypeName(),Transaction.AUTO_COMMIT);
  assertEquals(roadFeatures.length,count(writer));
  try {
    writer.hasNext();
    fail("Should not be able to use a closed writer");
  }
 catch (  IOException expected) {
  }
  try {
    writer.next();
    fail("Should not be able to use a closed writer");
  }
 catch (  IOException expected) {
  }
}
