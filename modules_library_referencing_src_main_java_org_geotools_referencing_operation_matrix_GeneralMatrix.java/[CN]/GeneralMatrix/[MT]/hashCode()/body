{
  final int prime=31;
  int result=1;
  if (mat == null) {
    return prime * result;
  }
  result=prime * result + mat.numRows;
  result=prime * result + mat.numCols;
  for (  double d : mat.data) {
    long bits=Double.doubleToRawLongBits(d);
    result=prime * ((int)(bits ^ (bits >>> 32)));
  }
  return result;
}
