{
  if (values.length != mat.getNumCols()) {
    throw new IllegalArgumentException("Call setRow received an array of length " + values.length + ".  "+ "The dimensions of the matrix is "+ mat.getNumRows()+ " by "+ mat.getNumCols()+ ".");
  }
  for (int i=0; i < values.length; i++) {
    mat.set(row,i,values[i]);
  }
}
