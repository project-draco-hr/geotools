{
  Schema schema=new SchemaImpl("test");
  FeatureTypeFactoryImpl typeFactory=new FeatureTypeFactoryImpl();
  AttributeType pointType=typeFactory.createGeometryType(new NameImpl("test","pointType"),Point.class,null,false,false,Collections.EMPTY_LIST,null,null);
  schema.put(new NameImpl("test","pointType"),pointType);
  AttributeType intType=typeFactory.createAttributeType(new NameImpl("test","intType"),Integer.class,false,false,Collections.EMPTY_LIST,null,null);
  schema.put(new NameImpl("test","intType"),intType);
  builder=new SimpleFeatureTypeBuilder(new FeatureTypeFactoryImpl());
  builder.setBindings(schema);
}
