{
  double[] coord1=new double[]{0.0,0.0,0.0,100.0,100,100.0,100.0,0.0,0.0,0.0};
  double[] coord2=new double[]{50.0,50.0,50.0,250.0,250.0,250.0,250.0,50.0};
  double[][] coords=new double[][]{coord1,coord2};
  SimpleGeometry geom=new SimpleGeometry(ShapeType.ARC,coords,new Envelope(0,15,0,15));
  final PolygonShape shape=new PolygonShape(geom);
  if (DISPLAY) {
    display("MultiOverlap",shape,500,500);
  }
}
