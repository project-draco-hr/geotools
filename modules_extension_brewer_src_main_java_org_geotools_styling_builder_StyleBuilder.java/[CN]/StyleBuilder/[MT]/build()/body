{
  if (unset) {
    return null;
  }
  if (ftsBuilder == null)   ftsBuilder=new FeatureTypeStyleBuilder();
  fts.add(ftsBuilder.build());
  Style s=sf.createStyle();
  s.setName(name);
  if (styleAbstract != null)   s.getDescription().setAbstract(new SimpleInternationalString(styleAbstract));
  if (title != null)   s.getDescription().setTitle(new SimpleInternationalString(title));
  s.featureTypeStyles().addAll(fts);
  s.setDefault(isDefault);
  if (parent == null)   reset();
  return s;
}
