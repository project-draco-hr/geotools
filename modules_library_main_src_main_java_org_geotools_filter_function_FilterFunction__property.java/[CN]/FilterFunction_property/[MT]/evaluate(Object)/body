{
  String name=getExpression(0).evaluate(feature,String.class);
  if (name == null) {
    return null;
  }
  PropertyName pn=lastPropertyName;
  if (pn != null && pn.getPropertyName().equals(name)) {
    return pn.evaluate(feature);
  }
 else {
    pn=FF.property(name);
    Object result=pn.evaluate(feature);
    lastPropertyName=pn;
    return result;
  }
}
