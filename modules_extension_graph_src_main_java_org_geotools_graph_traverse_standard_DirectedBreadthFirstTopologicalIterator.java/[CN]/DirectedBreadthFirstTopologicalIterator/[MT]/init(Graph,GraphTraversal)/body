{
  m_queue=buildQueue(graph);
  graph.visitNodes(new GraphVisitor(){
    public int visit(    Graphable component){
      DirectedNode node=(DirectedNode)component;
      node.setVisited(false);
      node.setCount(0);
      if (node.getInDegree() == 0)       m_queue.enq(node);
      return (0);
    }
  }
);
}
