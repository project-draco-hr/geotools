{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", id: ");
  result.append(id);
  result.append(", remoteSchema: ");
  result.append(remoteSchema);
  result.append(')');
  return result.toString();
}
