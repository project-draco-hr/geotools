{
  if (index > count) {
    throw new ArrayIndexOutOfBoundsException(index);
  }
  if (value == null) {
    throw new IllegalArgumentException(Errors.format(ErrorKeys.NULL_ARGUMENT_$1,"value"));
  }
  if (index == count) {
    if (index == data.length) {
      data=XArray.resize(data,index + Math.min(index,256));
    }
    count++;
  }
  data[index]=value;
}
