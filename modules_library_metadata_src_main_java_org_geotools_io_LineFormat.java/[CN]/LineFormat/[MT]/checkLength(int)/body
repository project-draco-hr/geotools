{
  if (count != expected) {
    final int lower=limits[Math.min(count,expected)];
    final int upper=limits[Math.min(count,expected + 1)];
    throw new ParseException(Errors.format(count < expected ? ErrorKeys.LINE_TOO_SHORT_$2 : ErrorKeys.LINE_TOO_LONG_$3,count,expected,line.substring(lower,upper).trim()),lower);
  }
}
