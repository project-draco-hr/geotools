{
  List types=(List)cache.get(base);
  if (types == null) {
    types=new ArrayList();
    XSDTypeDefinition type=base;
    while (type != null) {
      types.add(type);
      if (type.equals(type.getBaseType())) {
        break;
      }
      type=type.getBaseType();
    }
    cache.put(base,types);
  }
  return types;
}
