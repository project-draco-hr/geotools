{
  super();
  put(EFEATURE_ID_ATTRIBUTES,new HashSet<EAttribute>(Arrays.asList(new EAttribute[]{EFeaturePackage.eINSTANCE.getEFeature_ID()})));
  put(EFEATURE_SRID_ATTRIBUTES,new HashSet<EAttribute>(Arrays.asList(new EAttribute[]{EFeaturePackage.eINSTANCE.getEFeature_SRID()})));
  put(EFEATURE_DEFAULT_ATTRIBUTES,new HashSet<EAttribute>(Arrays.asList(new EAttribute[]{EFeaturePackage.eINSTANCE.getEFeature_Default()})));
  put(EFEATURE_DEFAULT_SRID_HINT,EFeatureConstants.DEFAULT_SRID);
  put(EFEATURE_DEFAULT_GEOMETRY_NAMES,new HashSet<String>(Arrays.asList(EFeatureConstants.DEFAULT_GEOMETRY_NAME)));
  put(EFEATURE_VALUES_DETACHED,new Boolean(false));
  put(EFEATURE_SINGLETON_FEATURES,new Boolean(true));
}
