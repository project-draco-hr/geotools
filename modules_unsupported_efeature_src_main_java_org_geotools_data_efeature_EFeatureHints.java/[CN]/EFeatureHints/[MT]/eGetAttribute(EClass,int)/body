{
  Object eHints=get(eHint);
  if (eHints instanceof Set) {
    Set<?> eSet=(Set<?>)eHints;
    EList<EAttribute> eAll=eClass.getEAllAttributes();
    for (    EAttribute it : eAll) {
      if (eSet.contains(it)) {
        return it;
      }
    }
  }
 else   if (eHints instanceof EAttribute) {
    EList<EAttribute> eAll=eClass.getEAllAttributes();
    if (eAll.contains(eHints)) {
      return (EAttribute)eHints;
    }
  }
  return null;
}
