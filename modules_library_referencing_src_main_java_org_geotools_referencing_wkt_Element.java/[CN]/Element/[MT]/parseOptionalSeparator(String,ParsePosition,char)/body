{
  final int length=text.length();
  int index=position.getIndex();
  while (index < length) {
    final char c=text.charAt(index);
    if (Character.isWhitespace(c)) {
      index++;
      continue;
    }
    if (c == separator) {
      position.setIndex(++index);
      return true;
    }
    break;
  }
  position.setIndex(index);
  return false;
}
