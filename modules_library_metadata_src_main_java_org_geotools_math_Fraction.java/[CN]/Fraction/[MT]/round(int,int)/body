{
  int n=numerator / denominator;
  int r=numerator % denominator;
  if (r != 0) {
    r=Math.abs(r << 1);
    final int d=Math.abs(denominator);
    if (r > d || (r == d && (n & 1) != 0)) {
      if ((numerator ^ denominator) >= 0) {
        n++;
      }
 else {
        n--;
      }
    }
  }
  return n;
}
