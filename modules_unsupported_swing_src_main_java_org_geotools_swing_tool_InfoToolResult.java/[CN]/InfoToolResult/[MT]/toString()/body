{
  StringBuilder sb=new StringBuilder();
  int k=0;
  for (  Map<String,Object> entry : featureEntries) {
    for (    Entry<String,Object> e : entry.entrySet()) {
      sb.append(e.getKey()).append(": ").append(e.getValue()).append('\n');
    }
    if (++k < featureEntries.size()) {
      sb.append('\n');
    }
  }
  return sb.toString();
}
