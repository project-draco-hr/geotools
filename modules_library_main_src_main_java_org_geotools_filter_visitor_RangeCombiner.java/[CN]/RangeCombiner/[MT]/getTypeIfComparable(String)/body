{
  if (propertyName == null || featureType.getDescriptor(propertyName) == null) {
    return null;
  }
 else {
    Class binding=featureType.getDescriptor(propertyName).getType().getBinding();
    if (Comparable.class.isAssignableFrom(binding)) {
      return binding;
    }
 else {
      return null;
    }
  }
}
