{
  for (int i=0; i < result.size(); ) {
    Geometry g=result.get(i);
    if (g instanceof GeometryCollection) {
      GeometryCollection gc=(GeometryCollection)g;
      for (int j=0; j < gc.getNumGeometries(); j++) {
        result.add(gc.getGeometryN(j));
      }
      result.remove(i);
    }
 else {
      i++;
    }
  }
}
