{
  if (shpFiles.get(ShpFileType.DBF) == null) {
    return null;
  }
  if (shpFiles.isLocal() && !shpFiles.exists(DBF)) {
    return null;
  }
  try {
    if (indexed) {
      return new IndexedDbaseFileReader(shpFiles,store.isMemoryMapped(),store.getCharset(),store.getTimeZone());
    }
 else {
      return new DbaseFileReader(shpFiles,store.isMemoryMapped(),store.getCharset(),store.getTimeZone());
    }
  }
 catch (  IOException e) {
    return null;
  }
}
