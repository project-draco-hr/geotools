{
  Map<String,EFeatureAttribute<V>> map=new HashMap<String,EFeatureAttribute<V>>(eAttributes.size());
  for (  EAttribute it : eAttributes) {
    String eName=it.getName();
    EFeatureAttribute<V> d=EFeatureAttributeDelegate.create(eObject,it.getName(),type,eFeatureInfo);
    if (d != null) {
      map.put(eName,d);
    }
  }
  return map;
}
