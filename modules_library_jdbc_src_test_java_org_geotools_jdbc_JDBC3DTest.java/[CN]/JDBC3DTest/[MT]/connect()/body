{
  super.connect();
  line3DType=DataUtilities.createType(dataStore.getNamespaceURI() + "." + tname(LINE3D),aname(ID) + ":0," + aname(GEOM)+ ":LineString:srid=4326,"+ aname(NAME)+ ":String");
  line3DType.getGeometryDescriptor().getUserData().put(Hints.COORDINATE_DIMENSION,3);
  poly3DType=DataUtilities.createType(dataStore.getNamespaceURI() + "." + tname(POLY3D),aname(ID) + ":0," + aname(GEOM)+ ":Polygon:srid=4326,"+ aname(NAME)+ ":String");
  poly3DType.getGeometryDescriptor().getUserData().put(Hints.COORDINATE_DIMENSION,3);
  epsg4326=CRS.decode("EPSG:4326");
}
