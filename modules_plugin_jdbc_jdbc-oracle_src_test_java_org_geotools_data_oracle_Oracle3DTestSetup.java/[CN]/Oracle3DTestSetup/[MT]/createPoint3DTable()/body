{
  run("CREATE TABLE point3d (fid int, id int, " + " geom MDSYS.SDO_GEOMETRY, name VARCHAR(20), PRIMARY KEY (fid) )");
  run("CREATE SEQUENCE point3d_fid_seq START WITH 0 MINVALUE 0");
  run("INSERT INTO USER_SDO_GEOM_METADATA (TABLE_NAME, COLUMN_NAME, DIMINFO, SRID)" + " VALUES ('point3d','geom',MDSYS.SDO_DIM_ARRAY(MDSYS.SDO_DIM_ELEMENT('X',-180,180,0.5), " + "MDSYS.SDO_DIM_ELEMENT('Y',-90,90,0.5), MDSYS.SDO_DIM_ELEMENT('Z',-1000,1000,0.5)), 4326)");
  run("CREATE INDEX POINT3D_GEOM_IDX ON POINT3D(GEOM) INDEXTYPE IS MDSYS.SPATIAL_INDEX" + " PARAMETERS ('SDO_INDX_DIMS=2 LAYER_GTYPE=\"POINT\"')");
  run("INSERT INTO point3d(fid, id,geom,name) VALUES (point3d_fid_seq.nextval, 0," + "MDSYS.SDO_GEOMETRY(3001, 4326, NULL, MDSYS.SDO_ELEM_INFO_ARRAY(1,1,1), " + "MDSYS.SDO_ORDINATE_ARRAY(1, 1, 1)), 'p1')");
  run("INSERT INTO point3d(fid, id,geom,name) VALUES (point3d_fid_seq.nextval, 1," + "MDSYS.SDO_GEOMETRY(3001, 4326, NULL, MDSYS.SDO_ELEM_INFO_ARRAY(1,1,1), " + "MDSYS.SDO_ORDINATE_ARRAY(3, 0, 1)), 'p2')");
}
