{
  run("CREATE TABLE line3d (fid int, id int, " + " geom MDSYS.SDO_GEOMETRY, name VARCHAR(20), PRIMARY KEY (fid) )");
  run("CREATE SEQUENCE line3d_fid_seq START WITH 0 MINVALUE 0");
  run("INSERT INTO USER_SDO_GEOM_METADATA (TABLE_NAME, COLUMN_NAME, DIMINFO, SRID)" + " VALUES ('line3d','geom',MDSYS.SDO_DIM_ARRAY(MDSYS.SDO_DIM_ELEMENT('X',-180,180,0.5), " + "MDSYS.SDO_DIM_ELEMENT('Y',-90,90,0.5), MDSYS.SDO_DIM_ELEMENT('Z',-1000,1000,0.5)), 4326)");
  run("CREATE INDEX LINE3D_GEOM_IDX ON LINE3D(GEOM) INDEXTYPE IS MDSYS.SPATIAL_INDEX" + " PARAMETERS ('SDO_INDX_DIMS=2 LAYER_GTYPE=\"LINE\"')");
  run("INSERT INTO line3d(fid,id,geom,name) VALUES (line3d_fid_seq.nextval, 0," + "MDSYS.SDO_GEOMETRY( 3002, 4326, NULL, MDSYS.SDO_ELEM_INFO_ARRAY(1,2,1), " + "MDSYS.SDO_ORDINATE_ARRAY(1, 1, 0, 2, 2, 0, 4, 2, 1, 5, 1, 1)), 'l1')");
  run("INSERT INTO line3d(fid,id,geom,name) VALUES (line3d_fid_seq.nextval, 1," + "MDSYS.SDO_GEOMETRY( 3002, 4326, NULL, MDSYS.SDO_ELEM_INFO_ARRAY(1,2,1), " + "MDSYS.SDO_ORDINATE_ARRAY(3, 0, 1, 3, 2, 2, 3, 3, 3, 3, 4, 5)), 'l2')");
}
