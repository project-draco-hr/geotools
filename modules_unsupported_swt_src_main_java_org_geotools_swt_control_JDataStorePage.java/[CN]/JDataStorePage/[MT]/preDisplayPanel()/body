{
  for (  Entry<Param,ParamField> entry : fields.entrySet()) {
    Param param=entry.getKey();
    ParamField field=entry.getValue();
    Object value=null;
    try {
      value=param.lookUp(connectionParameters);
    }
 catch (    IOException e) {
    }
    if (value == null && param.required) {
      value=param.sample;
    }
    field.setValue(value);
  }
}
