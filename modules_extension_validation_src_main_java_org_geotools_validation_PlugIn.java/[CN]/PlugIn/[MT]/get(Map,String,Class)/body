{
  if (!map.containsKey(key)) {
    return defaultType;
  }
  Object value=map.get(key);
  if (value instanceof Class) {
    return (Class)value;
  }
  if (value instanceof String) {
    try {
      return Class.forName((String)value);
    }
 catch (    ClassNotFoundException e) {
    }
  }
  return defaultType;
}
