{
  Path path=new Path();
  path.add(m_target);
  Node parent=m_iterator.getParent(m_target);
  while (parent != null) {
    path.add(parent);
    parent=m_iterator.getParent(parent);
  }
  if (!path.getLast().equals(m_iterator.getSource())) {
    throw new WrongPathException("getPath: The path obtained doesn't begin correctly");
  }
  return (path);
}
