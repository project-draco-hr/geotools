{
  if (newProcess != process) {
    NotificationChain msgs=null;
    if (process != null)     msgs=((InternalEObject)process).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Wps10Package.EXECUTE_RESPONSE_TYPE__PROCESS,null,msgs);
    if (newProcess != null)     msgs=((InternalEObject)newProcess).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Wps10Package.EXECUTE_RESPONSE_TYPE__PROCESS,null,msgs);
    msgs=basicSetProcess(newProcess,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Wps10Package.EXECUTE_RESPONSE_TYPE__PROCESS,newProcess,newProcess));
}
