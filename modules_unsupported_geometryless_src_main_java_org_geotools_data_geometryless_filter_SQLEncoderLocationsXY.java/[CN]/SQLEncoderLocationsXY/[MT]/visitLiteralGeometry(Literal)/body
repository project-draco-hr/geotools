{
  Geometry bbox=(Geometry)expression.getValue();
  Envelope e=bbox.getEnvelopeInternal();
  double x1=e.getMinX();
  double x2=e.getMaxX();
  double y1=e.getMinY();
  double y2=e.getMaxY();
  out.write("( " + xcolumn + " < "+ x2+ " and "+ xcolumn+ " > "+ x1+ " and "+ ycolumn+ " < "+ y2+ " and "+ ycolumn+ " > "+ y1+ " )");
}
