{
  PreparedStatement ps=con.prepareStatement("select seqschema,seqname from syscat.sequences where seqschema = ? and seqname = ?");
  ps.setString(1,schemaName);
  ps.setString(2,seqName);
  boolean retval=false;
  ResultSet rs=ps.executeQuery();
  if (rs.next())   retval=true;
  rs.close();
  ps.close();
  return retval;
}
