{
  String yaml="gridset: simple:2x5000000\n" + "feature-styles: \n" + "- name: name\n"+ "  rules:\n"+ "  - zoom: (1,2)\n";
  StyledLayerDescriptor sld=Ysld.parse(yaml);
  FeatureTypeStyle fs=SLD.defaultStyle(sld).featureTypeStyles().get(0);
  Rule r=fs.rules().get(0);
  assertThat(r,appliesToScale(5000000 / 2));
  assertThat(r,appliesToScale(5000000 / 4));
  assertThat(r,not(appliesToScale(5000000)));
  assertThat(r,not(appliesToScale(5000000 / 8)));
}
