{
  StringBuilder builder=new StringBuilder();
  builder.append("(");
  String join="";
  for (  Object o : values) {
    builder.append(join);
    join=", ";
    String s=o.toString();
    if (s.startsWith("#")) {
      builder.append(String.format("'%s'",s));
    }
 else {
      builder.append(s);
    }
  }
  builder.append(")");
  return builder.toString();
}
