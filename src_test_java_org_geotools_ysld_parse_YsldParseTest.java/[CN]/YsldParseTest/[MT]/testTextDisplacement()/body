{
  String yaml="text: \n" + "  displacement: (10, 42)\n";
  StyledLayerDescriptor sld=Ysld.parse(yaml);
  TextSymbolizer p=SLD.textSymbolizer(SLD.defaultStyle(sld));
  assertThat(((PointPlacement)p.getLabelPlacement()).getDisplacement(),allOf(hasProperty("displacementX",literal(10)),hasProperty("displacementY",literal(42))));
}
