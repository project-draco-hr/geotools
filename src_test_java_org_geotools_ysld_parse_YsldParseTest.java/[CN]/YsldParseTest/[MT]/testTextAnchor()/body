{
  String yaml="text: \n" + "  anchor: " + tuple(0.75,0.25) + "\n";
  StyledLayerDescriptor sld=Ysld.parse(yaml);
  TextSymbolizer t=SLD.textSymbolizer(SLD.defaultStyle(sld));
  assertThat(((PointPlacement)t.getLabelPlacement()).getAnchorPoint(),allOf(hasProperty("anchorPointX",literal(0.75)),hasProperty("anchorPointY",literal(0.25))));
}
