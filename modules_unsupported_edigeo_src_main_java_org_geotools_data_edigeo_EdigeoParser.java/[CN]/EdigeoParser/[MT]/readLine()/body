{
  String buffer="";
  do {
    try {
      buffer=reader.readLine();
      if (buffer == null) {
        return readLine("");
      }
    }
 catch (    IOException e) {
      return readLine("");
    }
    lineNumber++;
    buffer=buffer.trim();
  }
 while (buffer.length() == 0);
  return readLine(buffer);
}
