{
  if (registry == null) {
    return false;
  }
  for (  DataAccess<FeatureType,Feature> dataAccess : registry) {
    if (dataAccess instanceof AppSchemaDataAccess && Arrays.asList(((AppSchemaDataAccess)dataAccess).getTypeNames()).contains(featureTypeName)) {
      return true;
    }
  }
  return false;
}
