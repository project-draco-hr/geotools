{
  String xml="<ows:GetCapabilities xmlns:ows=\"http://www.opengis.net/ows\" version=\"1.1.0\">" + "<ows:AcceptVersions>" + "<ows:Version>1.0.0</ows:Version>"+ "</ows:AcceptVersions>"+ "</ows:GetCapabilities>";
  buildDocument(xml);
  GetCapabilitiesType getCaps=(GetCapabilitiesType)parse();
  assertNotNull(getCaps);
  assertEquals(1,getCaps.getAcceptVersions().getVersion().size());
}
