{
  NamedIdentifier name=new NamedIdentifier(Citations.CRS,"Gregorian calendar");
  GregorianCalendar gc=new GregorianCalendar(-4713,1,1);
  Number coordinateValue=100;
  TemporalCoordinateSystem frame1=new DefaultTemporalCoordinateSystem(new NamedIdentifier(Citations.CRS,new SimpleInternationalString("Julian calendar")),null,gc.getTime(),new SimpleInternationalString("day"));
  TemporalCoordinateSystem frame2=new DefaultTemporalCoordinateSystem(new NamedIdentifier(Citations.CRS,new SimpleInternationalString("Julian calendar")),null,gc.getTime(),new SimpleInternationalString("hour"));
  temporalCoordinate1=new DefaultTemporalCoordinate(frame1,IndeterminateValue.NOW,coordinateValue);
  temporalCoordinate2=new DefaultTemporalCoordinate(frame2,IndeterminateValue.AFTER,coordinateValue);
}
