{
  FilterToECQL toECQL=new FilterToECQL();
  StringBuilder output=new StringBuilder();
  Iterator<Filter> iter=filterList.iterator();
  while (iter.hasNext()) {
    Filter filter=iter.next();
    filter.accept(toECQL,output);
    if (iter.hasNext()) {
      output.append("; ");
    }
  }
  return output.toString();
}
