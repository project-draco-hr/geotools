{
  if (anchor == null) {
    return null;
  }
 else   if (anchor instanceof AnchorPointImpl) {
    return (AnchorPointImpl)anchor;
  }
 else {
    AnchorPointImpl copy=new AnchorPointImpl();
    copy.setAnchorPointX(anchor.getAnchorPointX());
    copy.setAnchorPointY(anchor.getAnchorPointY());
    return copy;
  }
}
