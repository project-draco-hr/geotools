{
  return triggerButtonMask == ANY_BUTTON || 0 != (triggerButtonMask & event.getStateMask()) || (event.getStateMask() == 0 && (((triggerButtonMask & SWT.BUTTON1) != 0 && event.getMouseButton() == 1) || ((triggerButtonMask & SWT.BUTTON2) != 0 && event.getMouseButton() == 2) || ((triggerButtonMask & SWT.BUTTON3) != 0 && event.getMouseButton() == 3)|| ((triggerButtonMask & SWT.BUTTON4) != 0 && event.getMouseButton() == 4)|| ((triggerButtonMask & SWT.BUTTON5) != 0 && event.getMouseButton() == 5)));
}
