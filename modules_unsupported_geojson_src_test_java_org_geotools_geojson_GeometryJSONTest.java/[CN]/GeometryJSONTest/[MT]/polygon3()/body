{
  Polygon poly;
  poly=gf.createPolygon(gf.createLinearRing(array(new double[][]{{100.1,0.1,10.2},{101.1,0.1,11.2},{101.1,1.1,11.2},{100.1,1.1,10.2},{100.1,0.1,10.2}})),new LinearRing[]{gf.createLinearRing(array(new double[][]{{100.2,0.2,10.2},{100.8,0.2,11.2},{100.8,0.8,11.2},{100.2,0.8,10.2},{100.2,0.2,10.2}}))});
  return poly;
}
