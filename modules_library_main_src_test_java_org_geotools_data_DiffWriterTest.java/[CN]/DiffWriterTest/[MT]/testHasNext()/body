{
  assertTrue(writer.hasNext());
  assertEquals(2,writer.diff.getAdded().size() + writer.diff.getModified().size());
  writer.next();
  assertTrue(writer.hasNext());
  assertEquals(2,writer.diff.getAdded().size() + writer.diff.getModified().size());
  writer.next();
  assertFalse(writer.hasNext());
  assertEquals(2,writer.diff.getAdded().size() + writer.diff.getModified().size());
}
