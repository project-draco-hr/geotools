{
  assertTrue(writer.hasNext());
  assertEquals(2,writer.diff.added.size() + writer.diff.modified2.size());
  writer.next();
  assertTrue(writer.hasNext());
  assertEquals(2,writer.diff.added.size() + writer.diff.modified2.size());
  writer.next();
  assertFalse(writer.hasNext());
  assertEquals(2,writer.diff.added.size() + writer.diff.modified2.size());
}
