{
  while (writer.hasNext()) {
    writer.next();
  }
  SimpleFeature feature=writer.next();
  feature.setAttribute("name","new1");
  writer.write();
  assertEquals(2,writer.diff.getAdded().size());
  feature=writer.next();
  feature.setAttribute("name","new2");
  writer.write();
  assertEquals(3,writer.diff.getAdded().size());
}
