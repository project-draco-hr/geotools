{
  List<T> objects=listFromSignature(s);
  if (!objects.isEmpty()) {
    if (!accept(objects)) {
      rejects.add((Signature)s.clone());
      return;
    }
  }
  if (k == n) {
    List<R> combined=buildResult(objects);
    if (combined != null) {
      result.addAll(combined);
    }
  }
 else {
    s.set(k,true);
    if (!rejected(s,k)) {
      fill(s,k + 1,n,result);
    }
    s.set(k,false);
    if (!rejected(s,k)) {
      fill(s,k + 1,n,result);
    }
  }
}
