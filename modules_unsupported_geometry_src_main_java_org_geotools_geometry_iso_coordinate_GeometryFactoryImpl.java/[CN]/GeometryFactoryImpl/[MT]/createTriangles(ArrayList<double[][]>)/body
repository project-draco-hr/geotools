{
  ArrayList<Triangle> result=new ArrayList<Triangle>();
  for (  double[][] triangle : triangles) {
    double[] coord0=triangle[0];
    double[] coord1=triangle[1];
    double[] coord2=triangle[2];
    result.add(this.createTriangle(null,createDirectPosition(coord0),createDirectPosition(coord1),createDirectPosition(coord2)));
  }
  return result;
}
