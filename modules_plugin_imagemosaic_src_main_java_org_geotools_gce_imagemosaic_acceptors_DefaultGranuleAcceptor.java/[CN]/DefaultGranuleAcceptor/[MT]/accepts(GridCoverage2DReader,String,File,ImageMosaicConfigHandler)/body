{
  String targetCoverageName=mosaicConfigHandler.getTargetCoverageName(coverage,coverageName);
  MosaicConfigurationBean config=mosaicConfigHandler.getConfigurations().get(targetCoverageName);
  if (config != null) {
    RasterManager rasterManager=mosaicConfigHandler.getRasterManagerForTargetCoverage(targetCoverageName);
    return checkCRS(coverage,config,coverageName) && checkColorModel(coverage,config,rasterManager,coverageName);
  }
 else {
    return true;
  }
}
