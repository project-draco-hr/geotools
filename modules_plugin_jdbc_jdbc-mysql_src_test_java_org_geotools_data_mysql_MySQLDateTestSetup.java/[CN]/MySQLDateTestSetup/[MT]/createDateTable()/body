{
  run("CREATE TABLE dates (d DATE, dt DATETIME, t TIME)");
  run("INSERT INTO dates VALUES (" + "STR_TO_DATE('2009-06-28', '%Y-%m-%d'), " + "STR_TO_DATE('2009-06-28 15:12:41', '%Y-%m-%d %H:%i:%s'),"+ "STR_TO_DATE('15:12:41', '%H:%i:%s')  )");
  run("INSERT INTO dates VALUES (" + "STR_TO_DATE('2009-01-15', '%Y-%m-%d'), " + "STR_TO_DATE('2009-01-15 13:10:12', '%Y-%m-%d %H:%i:%s'),"+ "STR_TO_DATE('13:10:12', '%H:%i:%s')  )");
  run("INSERT INTO dates VALUES (" + "STR_TO_DATE('2009-09-29', '%Y-%m-%d'), " + "STR_TO_DATE('2009-09-29 17:54:23', '%Y-%m-%d %H:%i:%s'),"+ "STR_TO_DATE('17:54:23', '%H:%i:%s')  )");
}
