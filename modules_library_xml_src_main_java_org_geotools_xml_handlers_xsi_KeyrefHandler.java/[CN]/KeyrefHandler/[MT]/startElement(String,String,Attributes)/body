{
  id=atts.getValue("","id");
  if (id == null) {
    id=atts.getValue(namespaceURI,"id");
  }
  name=atts.getValue("","name");
  if (name == null) {
    name=atts.getValue(namespaceURI,"name");
  }
  refer=atts.getValue("","refer");
  if (refer == null) {
    refer=atts.getValue(namespaceURI,"refer");
  }
}
