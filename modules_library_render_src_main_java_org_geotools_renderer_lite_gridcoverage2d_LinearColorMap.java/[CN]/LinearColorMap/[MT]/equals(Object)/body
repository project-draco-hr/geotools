{
  if (this == o)   return true;
  if (!(o instanceof LinearColorMap))   return false;
  final LinearColorMap that=(LinearColorMap)o;
  if (!Utilities.equals(name,that.name))   return false;
  if (!Utilities.equals(defaultColor,that.defaultColor))   return false;
  if (preFilteringColor != that.preFilteringColor)   return false;
  if (!Utilities.equals(preFilteringElements,that.preFilteringElements))   return false;
  if (!Utilities.equals(standardElements,that.standardElements))   return false;
  return piecewise.equals(that.piecewise);
}
