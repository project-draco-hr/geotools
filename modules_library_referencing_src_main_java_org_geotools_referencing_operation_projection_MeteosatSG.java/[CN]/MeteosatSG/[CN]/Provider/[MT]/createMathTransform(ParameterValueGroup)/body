{
  if (isSpherical(parameters)) {
    LOGGER.log(Level.INFO,"MeteosatSG conversion assumes ellipsoidal Earth shape. " + "Be aware of possibe errors arising from mixing ellipsoidal equations with spherical coordinates.");
    return new MeteosatSG(parameters);
  }
 else {
    return new MeteosatSG(parameters);
  }
}
