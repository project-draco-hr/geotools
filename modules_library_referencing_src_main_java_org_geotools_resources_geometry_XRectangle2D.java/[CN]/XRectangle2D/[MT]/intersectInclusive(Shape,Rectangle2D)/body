{
  double x=rect.getX();
  double y=rect.getY();
  double width=rect.getWidth();
  double height=rect.getHeight();
  if (width == 0 && height == 0) {
    width=EPS;
    height=EPS;
  }
 else   if (width == 0) {
    width=height * EPS;
    x-=0.5 * width;
  }
 else   if (height == 0) {
    height=width * EPS;
    y-=0.5 * height;
  }
  return shape.intersects(x,y,width,height);
}
