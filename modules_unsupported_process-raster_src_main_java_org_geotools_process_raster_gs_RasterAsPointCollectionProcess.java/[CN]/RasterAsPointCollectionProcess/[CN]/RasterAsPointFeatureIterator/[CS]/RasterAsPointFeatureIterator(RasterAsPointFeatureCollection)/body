{
  Utilities.ensureNonNull("fc",fc);
  this.fc=fc;
  this.fb=new SimpleFeatureBuilder(fc.getSchema());
  this.size=fc.size;
  iterator=RectIterFactory.create(fc.gc2d.getRenderedImage(),null);
  iterator.startLines();
  if (iterator.finishedLines())   throw new NoSuchElementException("Index beyond size:" + index + ">"+ size);
  iterator.startPixels();
  if (iterator.finishedPixels())   throw new NoSuchElementException("Index beyond size:" + index + ">"+ size);
  temp=new double[fc.numBands];
}
