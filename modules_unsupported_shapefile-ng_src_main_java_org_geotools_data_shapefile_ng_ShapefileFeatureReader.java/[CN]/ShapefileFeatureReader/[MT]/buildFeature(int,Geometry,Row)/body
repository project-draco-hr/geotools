{
  if (dbfindexes != null) {
    for (int i=0; i < dbfindexes.length; i++) {
      if (dbfindexes[i] == -1) {
        builder.add(geometry);
      }
 else {
        builder.add(row.read(dbfindexes[i]));
      }
    }
  }
 else {
    builder.add(geometry);
  }
  return builder.buildFeature(schema.getTypeName() + "." + number);
}
