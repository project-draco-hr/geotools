{
  if (dbfindexes != null) {
    for (int i=0; i < dbfindexes.length; i++) {
      if (dbfindexes[i] == -1) {
        builder.add(geometry);
      }
 else {
        builder.add(row.read(dbfindexes[i]));
      }
    }
  }
 else   if (geometry != null) {
    builder.add(geometry);
  }
  String featureId=buildFeatureId(number);
  return builder.buildFeature(featureId);
}
