{
  StringBuilder builder=new StringBuilder(URL);
  GeoRestFilterVisitor visitor=new GeoRestFilterVisitor(true);
  PropertyIsLessThanOrEqualTo filter=FF.lessOrEqual(FF.property(PROPERTY_NAME),FF.literal(PROPERTY_VALUE));
  visitor.visit(filter,null);
  visitor.finish(builder,false);
  Assert.assertEquals(URL + "?" + PROPERTY_NAME+ "__lte="+ PROPERTY_VALUE+ "&queryable="+ PROPERTY_NAME,builder.toString());
}
