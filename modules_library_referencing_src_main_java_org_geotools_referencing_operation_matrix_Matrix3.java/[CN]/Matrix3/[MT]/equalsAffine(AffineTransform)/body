{
  return mat.data[0] == transform.getScaleX() && mat.data[1] == transform.getShearX() && mat.data[2] == transform.getTranslateX() && mat.data[3] == transform.getShearY() && mat.data[4] == transform.getScaleY() && mat.data[5] == transform.getTranslateY() && mat.data[6] == 0 && mat.data[7] == 0 && mat.data[8] == 1;
}
