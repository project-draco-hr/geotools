{
  StringBuilder sb=new StringBuilder("CURVEPOLYGON(");
  writeRing(sb,getExteriorRing());
  int holeNum=getNumInteriorRing();
  for (int k=0; k < holeNum; k++) {
    sb.append(", ");
    LineString component=getInteriorRingN(k);
    writeRing(sb,component);
  }
  sb.append(")");
  return sb.toString();
}
