{
  System.out.println("------------------------------------------");
  System.out.println("Creating a CRS from an authority factory:");
  String code="26910";
  CoordinateReferenceSystem crs=ReferencingFactoryFinder.getCRSAuthorityFactory("EPSG",null).createCoordinateReferenceSystem(code);
  System.out.println("  CRS: " + crs.toWKT());
  System.out.println("Identified CRS object:");
  printIdentifierStuff(crs);
  System.out.println("------------------------------------------");
}
