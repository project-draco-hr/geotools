{
  TextSymbolizer ts=sf.createTextSymbolizer();
  if (fill != null) {
    ts.setFill(fill);
  }
  if (halo != null) {
    ts.setHalo(halo);
  }
  if (label != null) {
    ts.setLabel(label);
  }
  if (labelPlacement != null) {
    ts.setLabelPlacement(labelPlacement);
  }
  if (geometryPropertyName != null) {
    ts.setGeometryPropertyName(geometryPropertyName);
  }
  if (fonts != null) {
    for (    Font font : fonts) {
      if (font != null) {
        ts.fonts().add(font);
      }
    }
  }
  return ts;
}
