{
  TextSymbolizer ts=sf.createTextSymbolizer();
  if (fill != null) {
    ts.setFill(fill);
  }
  if (halo != null) {
    ts.setHalo(halo);
  }
  if (label != null) {
    ts.setLabel(label);
  }
  if (labelPlacement != null) {
    ts.setPlacement(labelPlacement);
  }
  if (geometryPropertyName != null) {
    ts.setGeometryPropertyName(geometryPropertyName);
  }
  if (fonts != null) {
    ts.setFonts(fonts);
  }
  return ts;
}
