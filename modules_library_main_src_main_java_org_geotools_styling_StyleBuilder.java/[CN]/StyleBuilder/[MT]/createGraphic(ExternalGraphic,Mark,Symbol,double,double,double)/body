{
  ExternalGraphic[] egs=null;
  Mark[] marks=null;
  Symbol[] symbols=null;
  if (externalGraphic != null) {
    egs=new ExternalGraphic[]{externalGraphic};
  }
  if (mark != null) {
    marks=new Mark[]{mark};
  }
  if (symbol != null) {
    symbols=new Symbol[]{symbol};
  }
  return createGraphic(egs,marks,symbols,literalExpression(opacity),literalExpression(size),literalExpression(rotation));
}
