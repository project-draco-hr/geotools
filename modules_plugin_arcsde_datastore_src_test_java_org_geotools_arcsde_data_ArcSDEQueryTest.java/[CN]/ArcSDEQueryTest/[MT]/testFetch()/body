{
  ArcSDEQuery queryAll=getQueryAll();
  try {
    queryAll.fetch();
    fail("fetch without calling execute");
  }
 catch (  IllegalStateException e) {
  }
  queryAll.execute();
  assertNotNull(queryAll.fetch());
  queryAll.close();
  try {
    queryAll.fetch();
    fail("fetch after close!");
  }
 catch (  IllegalStateException e) {
  }
  queryAll.session.dispose();
}
