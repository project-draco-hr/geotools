{
synchronized (this) {
    final int tileIndice=getTileIndice(tileX,tileY);
    if (waitings != null && waitings[tileIndice]) {
      notify();
    }
    if (pendings == null || pendings[tileIndice] == null) {
      return;
    }
    pendings[tileIndice]=null;
  }
  fireTileUpdate(tileX,tileY,false);
}
