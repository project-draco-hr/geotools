{
  List result=new ArrayList();
  for (  Expression expr : getParameters()) {
    try {
      Object value=expr.evaluate(feature);
      if (value instanceof Collection) {
        for (        Object item : (Collection)value) {
          result.add(item);
        }
      }
 else {
        result.add(value);
      }
    }
 catch (    Exception e) {
      throw new IllegalArgumentException(e);
    }
  }
  return result;
}
