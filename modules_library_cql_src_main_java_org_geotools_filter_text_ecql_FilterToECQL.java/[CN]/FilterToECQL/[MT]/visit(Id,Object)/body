{
  StringBuilder ecql=FilterToTextUtil.asStringBuilder(extraData);
  ecql.append("IN (");
  Iterator<Identifier> iter=filter.getIdentifiers().iterator();
  while (iter.hasNext()) {
    Identifier identifier=iter.next();
    ecql.append(identifier);
    if (iter.hasNext()) {
      ecql.append(",");
    }
  }
  ecql.append(")");
  return ecql.toString();
}
