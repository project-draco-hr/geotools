{
  if (this == oth) {
    return true;
  }
  if (oth instanceof GraphicImpl) {
    GraphicImpl other=(GraphicImpl)oth;
    return Utilities.equals(this.size,other.size) && Utilities.equals(this.rotation,other.rotation) && Utilities.equals(this.opacity,other.opacity)&& Arrays.equals(this.getMarks(),other.getMarks())&& Arrays.equals(this.getExternalGraphics(),other.getExternalGraphics())&& Arrays.equals(this.getSymbols(),other.getSymbols());
  }
  return false;
}
