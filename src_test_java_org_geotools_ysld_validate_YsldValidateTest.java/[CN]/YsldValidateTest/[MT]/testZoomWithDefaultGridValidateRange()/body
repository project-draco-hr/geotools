{
  StringBuilder builder=new StringBuilder();
  builder.append("rules:\n" + "- zoom: [0,max]\n" + "- zoom: [min,0]\n"+ "- zoom: [19,max]\n"+ "- zoom: [min,19]\n"+ "- zoom: [-1, max]\n"+ "- zoom: [min, -1]\n"+ "- zoom: [min, foo]\n"+ "- zoom: [foo, max]\n"+ "- zoom: [0.5, max]\n"+ "- zoom: [min, 0.5]\n");
  List<MarkedYAMLException> errors=validate(builder.toString());
  assertThat(errors,contains(hasProperty("problemMark",problemOn(8)),hasProperty("problemMark",problemOn(9)),hasProperty("problemMark",problemOn(10)),hasProperty("problemMark",problemOn(11))));
}
