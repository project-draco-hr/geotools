{
  SimpleFeatureReader sr=null;
  try {
    sr=SortedReaderFactory.getSortedReader(fr,peopleDesc,1000,20);
    String prev=null;
    while (fr.hasNext()) {
      SimpleFeature f=fr.next();
      String id=f.getID();
      if (prev != null) {
        assertTrue(id.compareTo(prev) >= 0);
      }
      prev=id;
    }
  }
  finally {
    if (sr != null) {
      sr.close();
    }
  }
}
