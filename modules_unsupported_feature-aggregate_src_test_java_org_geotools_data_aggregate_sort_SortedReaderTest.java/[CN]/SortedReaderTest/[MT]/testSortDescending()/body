{
  SimpleFeatureReader sr=null;
  try {
    sr=new SortedFeatureReader(fr,peopleDesc,1000,20);
    double prev=-1;
    while (fr.hasNext()) {
      SimpleFeature f=fr.next();
      double curr=(Double)f.getAttribute("PERSONS");
      if (prev > 0) {
        assertTrue(curr <= prev);
      }
      prev=curr;
    }
  }
  finally {
    if (sr != null) {
      sr.close();
    }
  }
}
