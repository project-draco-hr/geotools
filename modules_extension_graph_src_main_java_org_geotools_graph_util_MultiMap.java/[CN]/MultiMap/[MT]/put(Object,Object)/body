{
  Collection c=null;
  if ((c=(Collection)m_map.get(key)) == null) {
    try {
      c=(Collection)m_collectionClass.newInstance();
    }
 catch (    InstantiationException e) {
      throw new IllegalStateException(e.getClass().getName() + ": " + e.getMessage());
    }
catch (    IllegalAccessException e) {
      throw new IllegalStateException(e.getClass().getName() + ": " + e.getMessage());
    }
    m_map.put(key,c);
  }
  c.add(value);
  return (c);
}
