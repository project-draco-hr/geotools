{
  if (reference != modificationCount) {
    throw new ConcurrentModificationException("The map entry count has been modified during the iteration");
  }
  if (current == null) {
    while (idx < table.length && table[idx] == null) {
      idx++;
    }
    if (idx == table.length) {
      return false;
    }
    current=table[idx];
    idx++;
  }
  return current != null;
}
