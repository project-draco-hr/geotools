{
  int color=color(r,g,b,a);
  int index=indexFor(hash(color),table.length);
  ColorEntry prev=null;
  for (ColorEntry e=table[index]; e != null; e=e.next) {
    if (e.color == color) {
      if (prev == null) {
        table[index]=null;
      }
 else {
        prev.next=e.next;
      }
      size--;
      modificationCount++;
      return true;
    }
 else {
      prev=e;
    }
  }
  return false;
}
