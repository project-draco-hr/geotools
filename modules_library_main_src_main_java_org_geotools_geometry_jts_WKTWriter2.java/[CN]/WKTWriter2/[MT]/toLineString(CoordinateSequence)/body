{
  StringBuffer buf=new StringBuffer();
  buf.append("LINESTRING ");
  if (seq.size() == 0)   buf.append(" EMPTY");
 else {
    buf.append("(");
    for (int i=0; i < seq.size(); i++) {
      if (i > 0)       buf.append(", ");
      buf.append(seq.getX(i) + " " + seq.getY(i));
    }
    buf.append(")");
  }
  return buf.toString();
}
