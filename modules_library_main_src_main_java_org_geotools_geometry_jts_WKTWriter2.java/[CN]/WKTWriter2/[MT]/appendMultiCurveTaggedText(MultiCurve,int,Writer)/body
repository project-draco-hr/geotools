{
  writer.write("MULTICURVE ");
  if (mc.isEmpty()) {
    writer.write("EMPTY");
  }
 else {
    writer.write("(");
    for (int i=0; i < mc.getNumGeometries(); i++) {
      appendPotentialCurveText((LineString)mc.getGeometryN(i),level + 1,true,writer);
      if (i < mc.getNumGeometries() - 1) {
        writer.write(", ");
      }
    }
    writer.write(")");
  }
}
