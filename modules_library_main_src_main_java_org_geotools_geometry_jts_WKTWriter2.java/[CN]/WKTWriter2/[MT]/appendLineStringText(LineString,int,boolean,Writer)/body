{
  if (lineString.isEmpty()) {
    writer.write("EMPTY");
  }
 else {
    if (doIndent)     indent(level,writer);
    writer.write("(");
    for (int i=0; i < lineString.getNumPoints(); i++) {
      if (i > 0) {
        writer.write(", ");
        if (coordsPerLine > 0 && i % coordsPerLine == 0) {
          indent(level + 1,writer);
        }
      }
      appendCoordinate(lineString.getCoordinateN(i),writer);
    }
    writer.write(")");
  }
}
