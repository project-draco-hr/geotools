{
  writer.write("MULTISURFACE ");
  if (ms.isEmpty()) {
    writer.write("EMPTY");
  }
 else {
    writer.write("(");
    for (int i=0; i < ms.getNumGeometries(); i++) {
      appendPotentialCurvePolygonText((Polygon)ms.getGeometryN(i),level + 1,true,writer);
      if (i < ms.getNumGeometries() - 1) {
        writer.write(", ");
      }
    }
    writer.write(")");
  }
}
