{
  if (multiPolygon.isEmpty()) {
    writer.write("EMPTY");
  }
 else {
    int level2=level;
    boolean doIndent=false;
    writer.write("(");
    for (int i=0; i < multiPolygon.getNumGeometries(); i++) {
      if (i > 0) {
        writer.write(", ");
        level2=level + 1;
        doIndent=true;
      }
      appendPolygonText((Polygon)multiPolygon.getGeometryN(i),level2,doIndent,writer);
    }
    writer.write(")");
  }
}
