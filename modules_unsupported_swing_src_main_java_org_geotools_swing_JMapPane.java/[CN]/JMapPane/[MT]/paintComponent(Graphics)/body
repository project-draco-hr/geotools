{
  super.paintComponent(g);
  if (baseImage != null) {
    if (drawingLock.tryLock()) {
      System.out.println("got lock");
      System.out.flush();
      try {
        Graphics2D g2=(Graphics2D)g;
        g2.drawImage(baseImage,imageOrigin.x,imageOrigin.y,null);
      }
  finally {
        drawingLock.unlock();
      }
    }
 else {
      System.out.println("failed lock");
      System.out.flush();
    }
  }
}
