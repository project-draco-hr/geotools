{
  CoverageClassStats p=new CoverageClassStats();
  Results r=p.execute(createCoverage(),new LinkedHashSet<Statistic>(Arrays.asList(Statistic.MEAN,Statistic.SUM)),null,4,ClassificationMethod.EQUAL_INTERVAL,null,null);
  assertResults(r,new double[]{1d,14d,27d,40d,53d},new double[][]{{5.1,46d},{21.2,106d},{Double.NaN,Double.NaN},{49d,98d}});
}
