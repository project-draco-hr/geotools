{
  Map<String,String> properties=null;
  return COVERAGE_FACTORY.create(request.name,image,new GridGeometry2D(new GridEnvelope2D(PlanarImage.wrapRenderedImage(image).getBounds()),PixelInCell.CELL_CORNER,finalGridToWorldCorner,this.targetBBox.getCoordinateReferenceSystem(),hints),sampleDimensions,null,properties);
}
