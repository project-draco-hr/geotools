{
  try {
    this.shape=(Shape)in.readObject();
  }
 catch (  IOException e) {
    throw e;
  }
  if (in.readBoolean()) {
    try {
      this.data=getFeatureMarshaller().unmarshall(in);
    }
 catch (    IllegalAttributeException e) {
      throw (IOException)new IOException().initCause(e);
    }
catch (    IOException e) {
      throw e;
    }
catch (    Exception e) {
      throw (IOException)new IOException().initCause(e);
    }
  }
 else {
    try {
      this.data=in.readObject();
    }
 catch (    IOException e) {
      throw e;
    }
  }
}
