{
  FeatureReader<SimpleFeatureType,SimpleFeature> r=dataStore.getFeatureReader(new Query(tname(("guid"))),Transaction.AUTO_COMMIT);
  r.hasNext();
  Set<UUID> uuids=new HashSet<UUID>();
  uuids.add(uuid1);
  uuids.add(uuid2);
  while (r.hasNext()) {
    SimpleFeature f=(SimpleFeature)r.next();
    assertNotNull(uuids.remove(f.getAttribute(aname("uuidProperty"))));
  }
  assertTrue(uuids.isEmpty());
  r.close();
}
