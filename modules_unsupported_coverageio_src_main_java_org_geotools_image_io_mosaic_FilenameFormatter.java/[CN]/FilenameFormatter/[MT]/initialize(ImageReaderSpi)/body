{
  if (prefix == null) {
    prefix=DEFAULT_PREFIX;
  }
  suffix="";
  final String[] suffixes=tileReaderSpi.getFileSuffixes();
  if (suffixes != null) {
    for (int i=0; i < suffixes.length; i++) {
      final String s=suffixes[i];
      if (s.length() > suffix.length()) {
        suffix=s;
      }
    }
    if (!suffix.startsWith(".")) {
      suffix="." + suffix;
    }
  }
  levelFieldSize=0;
  rowFieldSize=0;
  columnFieldSize=0;
}
