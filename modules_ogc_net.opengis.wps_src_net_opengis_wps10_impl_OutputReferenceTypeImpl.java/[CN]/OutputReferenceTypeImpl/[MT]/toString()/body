{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (encoding: ");
  result.append(encoding);
  result.append(", href: ");
  result.append(href);
  result.append(", mimeType: ");
  result.append(mimeType);
  result.append(", schema: ");
  result.append(schema);
  result.append(')');
  return result.toString();
}
