{
  final Tile tile=node.tile;
  if (tile != null) {
    final Dimension floor=tile.getSubsamplingFloor(subsampling);
    if (floor != null) {
      return floor;
    }
  }
  node=node.firstChildren();
  while (node != null) {
    final Dimension floor=getSubsamplingFloor(node,subsampling);
    if (floor != null) {
      return floor;
    }
    node=node.nextSibling();
  }
  return subsampling;
}
