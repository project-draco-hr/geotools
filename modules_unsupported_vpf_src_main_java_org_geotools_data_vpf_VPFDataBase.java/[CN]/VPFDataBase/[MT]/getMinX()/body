{
  double result=Double.NaN;
  Iterator iter=libraries.iterator();
  VPFLibrary library;
  if (iter.hasNext()) {
    library=(VPFLibrary)iter.next();
    result=library.getXmin();
  }
  while (iter.hasNext()) {
    library=(VPFLibrary)iter.next();
    result=Math.min(result,library.getXmin());
  }
  return result;
}
