{
  Style style=applyCompositeOnFirstSymbolizer("pointBlend.sld");
  BufferedImage blended=composePoint(BKG,style);
  File reference=new File("./src/test/resources/org/geotools/renderer/composite/test-data/blend1-" + composite + ".png");
  ImageIO.write(blended,"PNG",new File(reference.getParent(),"blend1-" + composite + "-point.png"));
  int threshold=50;
  if ("copy".equals(composite)) {
    threshold=200;
  }
  ImageAssert.assertEquals(reference,blended,threshold);
}
