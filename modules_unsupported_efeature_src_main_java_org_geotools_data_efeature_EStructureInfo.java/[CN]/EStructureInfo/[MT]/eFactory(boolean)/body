{
  verify(checkIsValid);
  if (eFactory == null || eFactory.get() == null) {
    eFactory=new WeakReference<EFeatureContextFactory>(EFeatureContextFactory.eDefault());
    if (eFactory.get() == null) {
      invalidate(true);
      throw new IllegalStateException("EFeatureContextFactory '" + "' not found");
    }
  }
  return eFactory.get();
}
