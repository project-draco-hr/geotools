{
  Coordinate c=new Coordinate();
  if (ordinates.size() > 0) {
    c.x=((Number)ordinates.get(0)).doubleValue();
  }
  if (ordinates.size() > 1) {
    c.y=((Number)ordinates.get(1)).doubleValue();
  }
  if (ordinates.size() > 2) {
    c.z=((Number)ordinates.get(2)).doubleValue();
  }
  return c;
}
