{
  if (eIsProxy())   return super.toString();
  StringBuffer result=new StringBuffer(super.toString());
  result.append(" (mixed: ");
  result.append(mixed);
  result.append(", parentSchema: ");
  result.append(parentSchema);
  result.append(", schemaLanguage: ");
  result.append(schemaLanguage);
  result.append(", targetNamespace: ");
  result.append(targetNamespace);
  result.append(')');
  return result.toString();
}
