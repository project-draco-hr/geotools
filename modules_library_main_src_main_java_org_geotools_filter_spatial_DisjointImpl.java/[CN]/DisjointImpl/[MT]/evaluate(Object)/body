{
  if (feature instanceof SimpleFeature && !validate((SimpleFeature)feature))   return false;
  Geometry left;
  Geometry right;
switch (literals) {
case BOTH:
    return cacheValue;
case RIGHT:
{
    return rightPreppedGeom.disjoint(getLeftGeometry(feature));
  }
case LEFT:
{
  return leftPreppedGeom.disjoint(getRightGeometry(feature));
}
default :
{
left=getLeftGeometry(feature);
right=getRightGeometry(feature);
return basicEvaluate(left,right);
}
}
}
