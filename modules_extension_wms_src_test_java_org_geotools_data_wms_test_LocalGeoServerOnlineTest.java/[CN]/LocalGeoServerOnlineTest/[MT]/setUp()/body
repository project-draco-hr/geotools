{
  super.setUp();
  System.setProperty("org.geotools.referencing.forceXY","true");
  if (wms == null) {
    if (serverURL != null) {
      try {
        wms=new WebMapServer(serverURL);
        capabilities=wms.getCapabilities();
      }
 catch (      Exception eek) {
        serverURL=null;
        throw eek;
      }
    }
  }
}
