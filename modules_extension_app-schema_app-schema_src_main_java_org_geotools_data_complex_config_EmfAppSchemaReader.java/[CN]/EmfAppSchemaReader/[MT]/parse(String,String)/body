{
  AppSchemaConfiguration configuration=new AppSchemaConfiguration(nameSpace,schemaLocation,resolver);
  Configuration gmlConfig=findGmlConfiguration(configuration);
  if (gmlConfig != null) {
    configuration.addDependency(gmlConfig);
  }
 else {
    Logger LOGGER=org.geotools.util.logging.Logging.getLogger(EmfAppSchemaReader.class.getPackage().getName());
    LOGGER.warning("Non GML based schema found in schemaURI: " + schemaLocation + "! This may result in corrupted data as some types may not be encoded correctly.");
  }
  return parse(configuration);
}
