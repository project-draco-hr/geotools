{
  AppSchemaCatalog catalog=AppSchemaCatalog.build(getClass().getResource("/test-data/mappedPolygons.oasis.xml"));
  AppSchemaConfiguration configuration=new AppSchemaConfiguration("http://www.opengis.net/swe/2.0","http://schemas.opengis.net/sweCommon/2.0/swe.xsd",new AppSchemaResolver(catalog));
  Configuration gmlConfiguration=EmfAppSchemaReader.findGmlConfiguration(configuration);
  Assert.assertNull(gmlConfiguration);
}
