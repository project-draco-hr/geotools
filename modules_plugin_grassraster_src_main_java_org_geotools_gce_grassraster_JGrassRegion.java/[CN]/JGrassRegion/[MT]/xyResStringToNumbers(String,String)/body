{
  double xres=-1.0;
  double yres=-1.0;
  if (ewres.indexOf(':') != -1) {
    xres=degreeToNumber(ewres);
  }
 else {
    xres=Double.parseDouble(ewres);
  }
  if (nsres.indexOf(':') != -1) {
    yres=degreeToNumber(nsres);
  }
 else {
    yres=Double.parseDouble(nsres);
  }
  return new double[]{xres,yres};
}
