{
  if (capsVersion == null) {
    throw new IllegalArgumentException();
  }
  Version v=new Version(capsVersion);
  if (v1_0_0.equals(v)) {
    return v1_0_0;
  }
  if (v1_1_0.equals(v)) {
    return v1_1_0;
  }
  if (v2_0_0.equals(v)) {
    return v2_0_0;
  }
  throw new IllegalArgumentException();
}
