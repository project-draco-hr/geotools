{
  assertNull(ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(-1)).evaluate(null));
  assertNull(ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(5)).evaluate(null));
  assertEquals("",ff.function("strSubstringStart",ff.literal(""),ff.literal(0)).evaluate(null));
  assertEquals("",ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(4)).evaluate(null));
  assertEquals("ABCD",ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(0)).evaluate(null));
  assertEquals("CD",ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(2)).evaluate(null));
  assertEquals("D",ff.function("strSubstringStart",ff.literal("ABCD"),ff.literal(3)).evaluate(null));
}
