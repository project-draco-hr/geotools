{
  if (LOGGER.isLoggable(Level.FINE)) {
    QName qname=getFeatureTypeQName(containerType);
    String prefixedName=qname.getPrefix() + ":" + qname.getLocalPart();
    LOGGER.fine("Conditional nested mapping found, but no feature to evaluate " + "against was provided: nested feature type cannot be determined for " + "container type \"" + prefixedName + "\" and target attribute \""+ xpath);
  }
}
