{
  for (int i=0; i < grid.length; i++) {
    for (int j=0; j < grid[0].length; j++) {
      double x=trans.x(i);
      double y=trans.y(j);
      if (grid[i][j] == noDataValue)       continue;
      float del=(float)refinedDelta(x,y,convergenceFactor);
      if (!Float.isNaN(del))       grid[i][j]=grid[i][j] + del;
    }
  }
}
