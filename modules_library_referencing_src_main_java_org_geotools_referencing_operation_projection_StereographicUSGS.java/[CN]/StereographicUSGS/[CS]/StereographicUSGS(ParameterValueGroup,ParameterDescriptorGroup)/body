{
  super(parameters,descriptor);
  if (abs(latitudeOfOrigin) < EPSILON) {
    latitudeOfOrigin=0;
    cosphi0=1.0;
    sinphi0=0.0;
    chi1=0.0;
    cosChi1=1.0;
    sinChi1=0.0;
  }
 else {
    cosphi0=cos(latitudeOfOrigin);
    sinphi0=sin(latitudeOfOrigin);
    chi1=2.0 * atan(ssfn(latitudeOfOrigin,sinphi0)) - PI / 2;
    cosChi1=cos(chi1);
    sinChi1=sin(chi1);
  }
  k0=2.0 * msfn(sinphi0,cosphi0);
}
