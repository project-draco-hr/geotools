{
  AttributeType type=new AttributeTypeImpl(new NameImpl("attType"),String.class,false,false,null,null,null);
  AttributeDescriptor attribute=new AttributeDescriptorImpl(type,new NameImpl("attribute"),0,1,true,null);
  AttributeDescriptor[] attrTypes={attribute};
  String typeName="typeName";
  org.opengis.filter.Filter filter=null;
  builder.setSchemaEnabled(false);
  String sql=builder.buildSQLQuery(typeName,mapper,attrTypes,filter,sortBy,offset,limit);
  return sql;
}
