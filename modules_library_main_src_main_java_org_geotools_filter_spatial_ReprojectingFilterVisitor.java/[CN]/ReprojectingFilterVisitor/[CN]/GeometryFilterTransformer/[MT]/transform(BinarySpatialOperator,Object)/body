{
  if (!(filter.getExpression1() instanceof PropertyName))   throw new IllegalArgumentException("Binary geometry filter, but first expression " + "is not a property name? (it's a " + filter.getExpression1().getClass() + ")");
  final CoordinateReferenceSystem propertyCrs=findPropertyCRS((PropertyName)filter.getExpression1());
  if (propertyCrs == null)   return cloneFilter(filter,extraData);
  Expression ex1=(Expression)filter.getExpression1().accept(ReprojectingFilterVisitor.this,extraData);
  Expression ex2=reproject(filter.getExpression2(),propertyCrs,true);
  return cloneFilter(filter,extraData,ex1,ex2);
}
