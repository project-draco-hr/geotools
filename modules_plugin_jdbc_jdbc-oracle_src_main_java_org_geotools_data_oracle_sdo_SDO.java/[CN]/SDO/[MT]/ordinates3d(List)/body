{
  final int NUMBER=list.size();
  final int LEN=3;
  double[] array=new double[NUMBER * LEN];
  double[] ords;
  int offset=0;
  for (int i=0; i < NUMBER; i++) {
    ords=(double[])list.get(i);
    if (ords != null) {
      array[offset++]=ords[0];
      array[offset++]=ords[1];
      array[offset++]=ords[2];
    }
 else {
      array[offset++]=Double.NaN;
      array[offset++]=Double.NaN;
      array[offset++]=Double.NaN;
    }
  }
  return array;
}
