{
  if (set == null)   return;
  for (int i=0; i < set.length; i++) {
    if (set[i] == actual)     return;
  }
  StringBuffer array=new StringBuffer();
  for (int i=0; i < set.length; i++) {
    array.append(set[i]);
    if (i < set.length) {
      array.append(",");
    }
  }
  String msg=MessageFormat.format(condition,new Object[]{new Integer(actual),array});
  throw new IllegalArgumentException(msg);
}
