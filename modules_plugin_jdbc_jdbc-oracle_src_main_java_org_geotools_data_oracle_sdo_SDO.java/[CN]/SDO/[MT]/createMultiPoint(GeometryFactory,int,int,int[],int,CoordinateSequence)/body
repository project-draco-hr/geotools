{
  final int STARTING_OFFSET=STARTING_OFFSET(elemInfo,triplet);
  final int eTYPE=ETYPE(elemInfo,triplet);
  final int INTERPRETATION=INTERPRETATION(elemInfo,triplet);
  if (!(STARTING_OFFSET >= 1) || !(STARTING_OFFSET <= coords.size()))   throw new IllegalArgumentException("ELEM_INFO STARTING_OFFSET " + STARTING_OFFSET + " inconsistent with ORDINATES length "+ coords.size());
  if (!(eTYPE == ETYPE.POINT))   throw new IllegalArgumentException("ETYPE " + eTYPE + " inconsistent with expected POINT");
  if (!(INTERPRETATION >= 1)) {
    LOGGER.warning("Could not create MultiPoint with INTERPRETATION " + INTERPRETATION + " - representing the number of points");
    return null;
  }
  final int LEN=D(GTYPE);
  int start=(STARTING_OFFSET - 1) / LEN;
  int end=start + INTERPRETATION;
  MultiPoint points=gf.createMultiPoint(subList(gf.getCoordinateSequenceFactory(),coords,start,end));
  points.setSRID(SRID);
  return points;
}
