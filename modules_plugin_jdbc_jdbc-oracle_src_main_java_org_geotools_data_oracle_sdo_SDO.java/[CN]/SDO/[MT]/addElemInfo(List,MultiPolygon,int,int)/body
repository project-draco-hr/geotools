{
  Polygon poly;
  int offset=STARTING_OFFSET;
  int LEN=D(GTYPE) + L(GTYPE);
  for (int i=0; i < polys.getNumGeometries(); i++) {
    poly=(Polygon)polys.getGeometryN(i);
    addElemInfo(elemInfoList,poly,offset,GTYPE);
    if (isRectangle(poly)) {
      offset+=(2 * LEN);
    }
 else {
      offset+=(poly.getNumPoints() * LEN);
    }
  }
}
