{
  dbf2.deleteOnExit();
  shx2.deleteOnExit();
  shp2.deleteOnExit();
  prj2.deleteOnExit();
  File fix=new File(directory,typename + ".fix");
  File qix=new File(directory,typename + ".qix");
  if (shp2.exists() && !shp2.delete())   System.out.println("failed to delete: " + shp2.getAbsolutePath());
  if (shx2.exists() && !shx2.delete())   System.out.println("failed to delete: " + shx2.getAbsolutePath());
  if (prj2.exists() && !prj2.delete())   System.out.println("failed to delete: " + prj2.getAbsolutePath());
  if (dbf2.exists() && !dbf2.delete())   System.out.println("failed to delete: " + dbf2.getAbsolutePath());
  if (fix.exists() && !fix.delete()) {
    fix.deleteOnExit();
    System.out.println("failed to delete: " + fix.getAbsolutePath());
  }
  if (qix.exists() && !qix.delete()) {
    qix.deleteOnExit();
    System.out.println("failed to delete: " + qix.getAbsolutePath());
  }
}
