{
  List<T> list=new ArrayList<T>();
  Iterator it=collections.iterator();
  while (it.hasNext()) {
    FeatureCollection col=(FeatureCollection)it.next();
    FeatureIterator it2=col.features();
    try {
      while (it2.hasNext()) {
        list.add((T)it.next());
      }
    }
  finally {
      it2.close();
    }
  }
  return list.toArray(arg0);
}
