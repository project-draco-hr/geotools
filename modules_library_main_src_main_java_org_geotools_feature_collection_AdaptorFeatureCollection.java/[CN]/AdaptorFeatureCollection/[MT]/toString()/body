{
  StringBuffer buf=new StringBuffer();
  buf.append("[");
  Iterator i=iterator();
  try {
    boolean hasNext=i.hasNext();
    while (hasNext) {
      Object o=i.next();
      buf.append(o == this ? "(this Collection)" : String.valueOf(o));
      hasNext=i.hasNext();
      if (hasNext)       buf.append(", ");
    }
    buf.append("]");
    return buf.toString();
  }
  finally {
    close(i);
  }
}
