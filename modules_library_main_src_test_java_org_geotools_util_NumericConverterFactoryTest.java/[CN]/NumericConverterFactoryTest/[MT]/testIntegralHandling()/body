{
  assertEquals("3",NumericConverterFactory.toIntegral("3"));
  assertEquals("3",NumericConverterFactory.toIntegral("3.0"));
  assertEquals("-3",NumericConverterFactory.toIntegral("-3"));
  assertEquals("-3",NumericConverterFactory.toIntegral("-3.0"));
  assertEquals("3000",NumericConverterFactory.toIntegral("3000.0"));
  assertEquals("3000",NumericConverterFactory.toIntegral("3000,0"));
}
