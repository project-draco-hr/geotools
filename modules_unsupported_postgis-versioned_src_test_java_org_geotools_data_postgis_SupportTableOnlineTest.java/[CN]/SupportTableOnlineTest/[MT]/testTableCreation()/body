{
  buildDataStore();
  Connection conn=null;
  ResultSet tables=null;
  try {
    boolean changeSets=false;
    boolean tablesChanged=false;
    boolean versionedTables=false;
    conn=pool.getConnection();
    DatabaseMetaData meta=conn.getMetaData();
    String[] tableType={"TABLE"};
    tables=meta.getTables(null,f.schema,"%",tableType);
    while (tables.next()) {
      String tableName=tables.getString(3);
      if (tableName.equals(VersionedPostgisDataStore.TBL_CHANGESETS))       changeSets=true;
      if (tableName.equals(VersionedPostgisDataStore.TBL_TABLESCHANGED))       tablesChanged=true;
      if (tableName.equals(VersionedPostgisDataStore.TBL_VERSIONEDTABLES))       versionedTables=true;
    }
    tables.close();
    assertTrue(changeSets);
    assertTrue(tablesChanged);
    assertTrue(versionedTables);
  }
  finally {
    JDBCUtils.close(tables);
    JDBCUtils.close(conn,null,null);
  }
}
