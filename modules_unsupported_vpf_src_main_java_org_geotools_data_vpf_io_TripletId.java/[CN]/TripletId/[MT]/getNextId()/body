{
  int result=0;
  int length=getTileIdLength();
  int piece;
  if (length > 0) {
    int prevLength=getIdLength() + getTileIdLength();
    try {
      for (int inx=0; inx < length; inx++) {
        piece=rawData[inx + prevLength + 1];
        if (piece < 0) {
          piece+=(2 * Byte.MAX_VALUE);
        }
        result+=(piece << (8 * inx));
      }
    }
 catch (    RuntimeException exp) {
      exp.printStackTrace();
      result=0;
    }
  }
  return result;
}
