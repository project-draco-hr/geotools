{
  file.order(ByteOrder.BIG_ENDIAN);
  file.putInt(MAGIC);
  for (int i=0; i < 5; i++) {
    file.putInt(0);
  }
  file.putInt(length);
  file.order(ByteOrder.LITTLE_ENDIAN);
  file.putInt(VERSION);
  file.putInt(type.id);
  file.putDouble(minX);
  file.putDouble(minY);
  file.putDouble(maxX);
  file.putDouble(maxY);
  file.order(ByteOrder.BIG_ENDIAN);
  for (int i=0; i < 8; i++) {
    file.putInt(0);
  }
}
