{
  FeatureWriter<SimpleFeatureType,SimpleFeature> writer=writer(tname("road"));
  SimpleFeature feature;
  try {
    while (writer.hasNext()) {
      feature=(SimpleFeature)writer.next();
      writer.remove();
    }
  }
  finally {
    writer.close();
  }
  assertEquals(0,count(tname("road")));
}
