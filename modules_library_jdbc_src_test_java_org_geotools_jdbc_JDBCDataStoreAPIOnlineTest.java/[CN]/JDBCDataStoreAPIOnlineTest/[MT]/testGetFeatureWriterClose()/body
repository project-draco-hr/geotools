{
  FeatureWriter<SimpleFeatureType,SimpleFeature> writer=dataStore.getFeatureWriter(tname("road"),Filter.INCLUDE,Transaction.AUTO_COMMIT);
  writer.close();
  try {
    assertFalse(writer.hasNext());
    fail("Should not be able to use a closed writer");
  }
 catch (  Exception expected) {
  }
  try {
    assertNull(writer.next());
    fail("Should not be able to use a closed writer");
  }
 catch (  Exception expected) {
  }
  try {
    writer.close();
  }
 catch (  Exception expected) {
  }
}
