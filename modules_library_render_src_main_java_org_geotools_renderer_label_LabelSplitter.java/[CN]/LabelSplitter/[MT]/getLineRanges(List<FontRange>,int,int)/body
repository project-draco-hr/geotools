{
  int start=-1;
  int end=ranges.size();
  for (int i=0; i < ranges.size(); i++) {
    FontRange range=ranges.get(i);
    if (start == -1) {
      if (range.endChar > prevPosition) {
        start=i;
      }
 else {
        continue;
      }
    }
    if (range.startChar > newPosition) {
      end=i;
      break;
    }
  }
  return ranges.subList(start,end);
}
