{
  Iterator mpIter=mc.getElements().iterator();
  String rString="(" + this.curveCoordToString((Curve)mpIter.next()) + ")";
  while (mpIter.hasNext()) {
    if (this.lineBreak) {
      rString+="\n\t";
    }
    rString+=", (";
    rString+=this.curveCoordToString((Curve)mpIter.next());
    rString+=")";
  }
  return rString;
}
