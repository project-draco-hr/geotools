{
  if (obj == this)   return true;
  if (obj instanceof Name) {
    NameImpl other=(NameImpl)obj;
    if (!Utilities.equals(this.namespace,other.getNamespaceURI())) {
      return false;
    }
    if (!Utilities.equals(this.local,other.getLocalPart())) {
      return false;
    }
    return true;
  }
  return false;
}
