{
synchronized (this) {
    if (crs == null) {
      try {
        crs=CRS.decode(srsName);
      }
 catch (      NoSuchAuthorityCodeException e) {
        crs=DefaultEngineeringCRS.CARTESIAN_2D;
      }
catch (      FactoryException e) {
        crs=DefaultEngineeringCRS.CARTESIAN_2D;
      }
    }
    return crs == DefaultEngineeringCRS.CARTESIAN_2D ? null : crs;
  }
}
