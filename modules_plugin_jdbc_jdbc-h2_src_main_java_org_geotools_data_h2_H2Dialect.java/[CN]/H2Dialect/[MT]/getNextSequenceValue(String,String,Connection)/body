{
  Statement st=cx.createStatement();
  try {
    String sql="SELECT NEXTVAL('" + sequenceName + "')";
    dataStore.getLogger().fine(sql);
    ResultSet rs=st.executeQuery(sql);
    try {
      rs.next();
      return rs.getInt(1);
    }
  finally {
      dataStore.closeSafe(rs);
    }
  }
  finally {
    dataStore.closeSafe(st);
  }
}
