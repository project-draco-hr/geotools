{
  for (  Object obj : c) {
    Layer element=(Layer)obj;
    if (!contains(element)) {
      continue;
    }
    if (layerListener != null) {
      element.removeMapLayerListener(layerListener);
    }
    element.dispose();
  }
  boolean removed=super.removeAll(c);
  fireLayerRemoved(null,0,size() - 1);
  return removed;
}
