{
  boolean removed=super.remove(o);
  if (removed) {
    fireLayerRemoved((Layer)o,-1,-1);
    if (o instanceof Layer) {
      Layer element=(Layer)o;
      if (layerListener != null) {
        element.removeMapLayerListener(layerListener);
      }
      element.dispose();
    }
  }
  return removed;
}
