{
  boolean removed=super.remove(element);
  if (removed) {
    fireLayerRemoved((Layer)element,-1,-1);
    if (element instanceof Layer) {
      Layer layer=(Layer)element;
      if (layerListener != null) {
        layer.removeMapLayerListener(layerListener);
      }
      layer.dispose();
    }
  }
  return removed;
}
