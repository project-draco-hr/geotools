{
  if (type != null) {
    final int ordinal=type.ordinal();
    if (ordinal >= 0 && ordinal < MAP.length) {
      final TypeMap map=MAP[ordinal];
      if (map != null) {
        return map;
      }
    }
  }
  throw new IllegalArgumentException(Errors.format(ErrorKeys.ILLEGAL_ARGUMENT_$2,"type",type));
}
