{
  BasicWaypoint testSubject1=BasicWaypoint.getBasicWaypoint(84.566,120.123,"Waypoint 001");
  DateTime now=new DateTime();
  testSubject1.setTime(now);
  DateTime timeReturned=testSubject1.getTime();
  if (timeReturned.equals(now) != true) {
    fail("The getTime method failed. The date and time" + " values did not match.");
  }
}
