{
  if (offset == 0) {
    return createPadValuesMask(padValues);
  }
  if (padValues != null) {
switch (padValues.length) {
default :
      return new MaskAndOffset(offset,padValues);
case 1:
    return createOffset(offset,padValues[0]);
case 0:
  break;
}
}
return createOffset(offset,Double.NaN);
}
