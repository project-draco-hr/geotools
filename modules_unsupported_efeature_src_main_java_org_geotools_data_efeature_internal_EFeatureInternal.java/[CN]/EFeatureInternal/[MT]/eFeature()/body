{
  InternalEObject eImpl=this.eImpl.get();
  if (eImpl == null) {
    throw (NullPointerException)(new NullPointerException("EFeature implementation " + eStructure.eClassName() + " is finalized (garbage collected).")).fillInStackTrace();
  }
 else   if (eImpl instanceof EFeature) {
    return eImpl;
  }
  return EFeatureDelegate.create(eStructure,eImpl,true);
}
