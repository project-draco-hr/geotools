{
  Collection<? extends EFeatureProperty<?,? extends Property>> eList=getPropertyMap().values();
  List<EFeatureProperty<V,Property>> eSelected=new ArrayList<EFeatureProperty<V,Property>>(eList.size());
  for (  EFeatureProperty<?,? extends Property> it : eList) {
    if (type.isAssignableFrom(it.getValueType())) {
      eSelected.add((EFeatureProperty<V,Property>)it);
    }
  }
  return eSelected;
}
