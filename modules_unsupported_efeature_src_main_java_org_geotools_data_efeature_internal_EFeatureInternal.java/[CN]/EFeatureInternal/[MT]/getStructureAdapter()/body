{
  if (eStructurelistener == null || eStructurelistener.get() == null) {
    eStructurelistener=new WeakReference<EFeatureListener<EStructureInfo<?>>>(new EFeatureListener<EStructureInfo<?>>(){
      public boolean onChange(      EStructureInfo<?> eInfo,      int property,      Object oldValue,      Object newValue){
        if (property == EFeatureInfo.SRID) {
          bounds=null;
          if (eImpl().eNotificationRequired()) {
            eImpl().eNotify(new ENotificationImpl(eImpl(),Notification.SET,EFeaturePackage.EFEATURE__SRID,oldValue,newValue));
          }
        }
        return true;
      }
    }
);
  }
  return eStructurelistener.get();
}
