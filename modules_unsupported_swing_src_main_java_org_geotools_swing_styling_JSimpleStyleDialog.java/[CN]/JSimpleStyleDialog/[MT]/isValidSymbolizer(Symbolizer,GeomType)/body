{
  if (sym != null) {
    if (sym instanceof PolygonSymbolizer) {
      return type == GeomType.POLYGON;
    }
 else     if (sym instanceof LineSymbolizer) {
      return type == GeomType.LINE;
    }
 else     if (sym instanceof PointSymbolizer) {
      return type == GeomType.POINT;
    }
  }
  return false;
}
