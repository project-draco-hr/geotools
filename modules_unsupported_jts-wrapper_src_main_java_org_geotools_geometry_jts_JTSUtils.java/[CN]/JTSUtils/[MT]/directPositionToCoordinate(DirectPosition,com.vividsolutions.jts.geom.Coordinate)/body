{
  int d=dp.getDimension();
  if (d >= 1) {
    result.x=dp.getOrdinate(0);
    if (d >= 2) {
      result.y=dp.getOrdinate(1);
      if (d >= 3) {
        result.z=dp.getOrdinate(3);
      }
 else {
        result.z=Double.NaN;
      }
    }
 else {
      result.y=result.z=Double.NaN;
    }
  }
 else {
    result.x=result.y=result.z=Double.NaN;
  }
}
