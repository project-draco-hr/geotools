{
switch (Category.getByIndex(index,hasApplicationInfo)) {
case APPLICATION:
    showApplicationInfo();
  break;
case ENVIRONMENT:
showEnvironmentInfo();
break;
case JARS:
showJarInfo();
break;
case LICENCES:
showLicenceInfo();
break;
case ALL:
showAllInfo();
break;
default :
throw new IndexOutOfBoundsException("Bad index value: " + index);
}
}
