{
  int count=0;
  final String[] left=new String[Math.min(max,arguments.length)];
  for (int i=0; i < arguments.length; i++) {
    final String arg=arguments[i];
    if (arg != null) {
      if (count >= max) {
        illegalArgument(new IllegalArgumentException(Errors.getResources(locale).getString(ErrorKeys.UNEXPECTED_PARAMETER_$1,arguments[i])));
      }
      left[count++]=arg;
    }
  }
  return XArray.resize(left,count);
}
