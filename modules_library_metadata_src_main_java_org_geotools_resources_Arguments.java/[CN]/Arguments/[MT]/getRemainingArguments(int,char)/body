{
  final String[] arguments=getRemainingArguments(max);
  for (int i=0; i < arguments.length; i++) {
    String argument=arguments[i];
    if (argument != null) {
      argument=argument.trim();
      if (argument.length() != 0) {
        if (argument.charAt(0) == forbiddenPrefix) {
          illegalArgument(new IllegalArgumentException(Errors.getResources(locale).getString(ErrorKeys.UNKNOW_PARAMETER_$1,argument)));
        }
      }
    }
  }
  return arguments;
}
