{
  String mapperName=getClass().toString();
  String colInfo="";
  if (getColumnCount() > 0) {
    colInfo=getColumnName(0) + ":" + getColumnType(0)+ ":"+ getColumnSize(0)+ ":"+ getColumnDecimalDigits(0);
  }
  String s=mapperName + ":" + getColumnCount()+ ":"+ colInfo+ ":"+ returnFIDColumnsAsAttributes()+ ":"+ hasAutoIncrementColumns()+ ":"+ "";
  return s;
}
