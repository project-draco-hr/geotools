{
  Function size=ff.function("categorize",ff.property("pop"),ff.literal(8),ff.literal(50000),ff.literal(12),ff.literal(100000),ff.literal(16));
  Style style=new GraphicBuilder().size(size).mark().name("circle").fill().color(new Color(0,51,204)).buildStyle();
  StyleCollector collector=new StyleCollector();
  style.accept(collector);
  assertSimpleStyle(collector);
  PointSymbolizer ps=(PointSymbolizer)collector.symbolizers.get(0);
  Graphic graphic=ps.getGraphic();
  assertTrue(graphic.getSize() instanceof CategorizeFunction);
}
