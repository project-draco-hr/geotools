{
  int skip=sourceBands[bandIndex];
  if (++bandIndex >= sourceBands.length) {
    return true;
  }
  skip=sourceBands[bandIndex] - skip;
  if (skip < 0) {
    iterator.startBands();
    skip=sourceBands[bandIndex];
  }
  while (--skip >= 0) {
    if (iterator.nextBandDone()) {
      return true;
    }
  }
  return false;
}
