{
  Class bestClass=bestClass(objects);
  if (bestClass == String.class) {
    return new String("");
  }
 else   if (bestClass == Double.class) {
    return new Double(0);
  }
 else   if (bestClass == Float.class) {
    return new Float(0);
  }
 else   if (bestClass == Long.class) {
    return new Long(0);
  }
 else   if (bestClass == Integer.class) {
    return new Integer(0);
  }
 else {
    return null;
  }
}
