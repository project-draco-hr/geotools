{
  Point p1=new GeometryFactory().createPoint(new Coordinate(0,0));
  Point p2=new GeometryFactory().createPoint(new Coordinate(1,1));
  Node node=createNode(association,new ElementInstance[]{geometry},new Object[]{new GeometryFactory().createMultiPoint(new Point[]{p1,p2})},null,null);
  GMLGeometryAssociationTypeBinding s=(GMLGeometryAssociationTypeBinding)container.getComponentInstanceOfType(GMLGeometryAssociationTypeBinding.class);
  GMLMultiPointPropertyTypeBinding s1=(GMLMultiPointPropertyTypeBinding)container.getComponentInstanceOfType(GMLMultiPointPropertyTypeBinding.class);
  MultiPoint p=(MultiPoint)s1.parse(association,node,s.parse(association,node,null));
  assertNotNull(p);
}
