{
  if (params == null) {
    throw new NullPointerException("params can't be null");
  }
  final int argCount=getArgCount();
  final int paramsSize=params.size();
  if (paramsSize < argCount || paramsSize > argCount + 1) {
    throw new IllegalArgumentException(String.format("Function %s expected %d or %d arguments but got %d",name,argCount,argCount + 1,paramsSize));
  }
  this.params=new ArrayList(params);
}
