{
  try {
    PrimaryKey keys=dataStore.getPrimaryKeyFinder().getPrimaryKey(dataStore,schemaName,tableName,mCurrentConnection);
    if (keys != null) {
      List<PrimaryKeyColumn> colsKey=keys.getColumns();
      if (colsKey.size() == 1) {
        PrimaryKeyColumn colKey=colsKey.get(0);
        return colKey.getName();
      }
 else {
        return null;
      }
    }
 else {
      return null;
    }
  }
 catch (  SQLException e) {
    return null;
  }
}
