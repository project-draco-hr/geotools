{
  final TableWriter table=new TableWriter(null," ");
  table.write("Source Point");
  table.write(':');
  table.nextColumn();
  table.write("1.234");
  table.nextLine();
  table.write("Target Point");
  table.write(':');
  table.nextColumn();
  table.write("2.345");
  assertEquals("Source Point: 1.234\nTarget Point: 2.345\n",table.toString());
  table.close();
}
