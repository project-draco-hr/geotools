{
  while (level != null) {
    while (y < level.getNumYTiles()) {
      while (x < level.getNumXTiles()) {
        try {
          tile=level.getTile(x++,y);
        }
 catch (        MalformedURLException e) {
          Logging.unexpectedException(GridTileIterator.class,"next",e);
          continue;
        }
        if (tile != null) {
          return;
        }
      }
      x=0;
      y++;
    }
    y=0;
    level=level.getFinerLevel();
  }
  tile=null;
}
