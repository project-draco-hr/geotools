{
  if (newMax != max) {
    NotificationChain msgs=null;
    if (max != null)     msgs=((InternalEObject)max).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Wcs10Package.VALUE_RANGE_TYPE__MAX,null,msgs);
    if (newMax != null)     msgs=((InternalEObject)newMax).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Wcs10Package.VALUE_RANGE_TYPE__MAX,null,msgs);
    msgs=basicSetMax(newMax,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Wcs10Package.VALUE_RANGE_TYPE__MAX,newMax,newMax));
