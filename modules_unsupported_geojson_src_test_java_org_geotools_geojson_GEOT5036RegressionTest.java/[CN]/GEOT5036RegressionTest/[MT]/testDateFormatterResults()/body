{
  SimpleDateFormat sdf=GeoJSONUtil.DATE_FORMAT;
  ExecutorService exec=Executors.newFixedThreadPool(8);
  for (  Date key : expectationMap.keySet()) {
    Task task=new Task(key);
    expectationMap.put(key,exec.submit(task));
  }
  exec.shutdown();
  for (  Map.Entry<Date,Future<String>> entry : expectationMap.entrySet()) {
    assertEquals("incorrect date string was returned: ",entry.getValue().get(),"\"" + sdf.format(entry.getKey()) + "\"");
  }
}
