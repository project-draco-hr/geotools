{
  currentGeomColumnName=null;
  this.fidColumn=fidColumn;
  this.srids=srids;
  Set geomCols=srids.keySet();
  if (geomCols.size() > 0) {
    currentGeomColumnName=(String)geomCols.iterator().next();
  }
  LOGGER.fine("SQLEncoderOracle: Geometric Column is: " + currentGeomColumnName);
  setSqlNameEscape("\"");
}
