{
  Expression property=ff.property("gsml:specification/gsml:GeologicUnit/gml:description");
  Filter filter=ff.like(property,"Olivine basalt, tuff, microgabbro, minor sedimentary rocks");
  FeatureCollection<FeatureType,Feature> filteredResults=mfDataAccess.getFeatureSource(MAPPED_FEATURE).getFeatures(filter);
  assertEquals(3,size(filteredResults));
}
