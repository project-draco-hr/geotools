{
  String xml="<Polygon>" + "<outerBoundaryIs>" + "<LinearRing><coordinates>1,1 2,2 3,3 1,1</coordinates></LinearRing>"+ "</outerBoundaryIs>"+ "<innerBoundaryIs>"+ "<LinearRing><coordinates>1,1 2,2 3,3 1,1</coordinates></LinearRing>"+ "</innerBoundaryIs>"+ "</Polygon>";
  buildDocument(xml);
  Polygon p=(Polygon)parse();
  assertEquals(1,p.getNumInteriorRing());
}
