{
  Polygon p=new GeometryFactory().createPolygon(new GeometryFactory().createLinearRing(new Coordinate[]{new Coordinate(1,1),new Coordinate(2,2),new Coordinate(3,3),new Coordinate(1,1)}),new LinearRing[]{new GeometryFactory().createLinearRing(new Coordinate[]{new Coordinate(1,1),new Coordinate(2,2),new Coordinate(3,3),new Coordinate(1,1)})});
  Document dom=encode(p,KML.Polygon);
  assertNotNull(getElementByQName(dom,new QName(KML.NAMESPACE,"outerBoundaryIs")));
  assertNotNull(getElementByQName(dom,new QName(KML.NAMESPACE,"innerBoundaryIs")));
}
