{
  String oldSchemaLanguage=schemaLanguage;
  schemaLanguage=newSchemaLanguage;
  boolean oldSchemaLanguageESet=schemaLanguageESet;
  schemaLanguageESet=true;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Csw20Package.DESCRIBE_RECORD_TYPE__SCHEMA_LANGUAGE,oldSchemaLanguage,schemaLanguage,!oldSchemaLanguageESet));
}
