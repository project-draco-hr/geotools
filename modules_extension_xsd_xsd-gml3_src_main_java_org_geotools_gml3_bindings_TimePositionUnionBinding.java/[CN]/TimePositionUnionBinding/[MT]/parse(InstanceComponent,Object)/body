{
  Date date=null;
  try {
    Calendar c=(Calendar)new XSDateTimeBinding().parse(instance,value);
    if (c != null) {
      date=c.getTime();
    }
  }
 catch (  Exception e) {
  }
  if (date == null) {
    try {
      date=(Date)new XSDateBinding().parse(instance,value);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
    ;
  }
  if (date == null) {
    throw new IllegalArgumentException("Unable to parse " + value);
  }
  return new DefaultPosition(date);
}
