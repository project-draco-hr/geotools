{
  UnitFormat unitFormat=UnitFormat.getInstance();
  Unit uom=null;
  try {
    uom=(Unit)unitFormat.parseObject("m");
  }
 catch (  ParseException ex1) {
    uom=null;
  }
  final Category nan=new Category(Vocabulary.format(VocabularyKeys.NODATA),new Color[]{new Color(0,0,0,0)},NumberRange.create(0,0),NumberRange.create((short)-9999,(short)-9999));
  final GridSampleDimension band=new GridSampleDimension("digital-elevation",new Category[]{nan},uom);
  return band.geophysics(true);
}
