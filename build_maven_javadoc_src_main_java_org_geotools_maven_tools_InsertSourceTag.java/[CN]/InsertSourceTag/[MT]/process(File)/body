{
  if (file.isDirectory()) {
    for (    File child : file.listFiles()) {
      process(child);
    }
  }
 else {
    if (file.getName().endsWith(".java")) {
      try {
        System.out.println(file.getPath());
        processFile(file);
      }
 catch (      Exception ex) {
        ex.printStackTrace();
        System.exit(1);
      }
    }
  }
}
