{
  SimpleFeature[] dynFeatures=new SimpleFeature[3];
  dynFeatures[0]=SimpleFeatureBuilder.build(roadType,new Object[]{new Integer(1),line(new int[]{1,1,2,2,4,2,5,1}),"r1"},"r1");
  dynFeatures[1]=SimpleFeatureBuilder.build(roadType,new Object[]{new Integer(2),line(new int[]{3,0,3,2,3,3,3,4}),"r2"},"r2");
  dynFeatures[2]=SimpleFeatureBuilder.build(roadType,new Object[]{new Integer(3),line(new int[]{3,2,4,2,5,3}),"r3"},"r3");
  assertOrderSame(dynFeatures);
}
