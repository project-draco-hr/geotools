{
  if (currentShape > UNKNOWN && currentShape > shxReader.getRecordCount() - 1)   return false;
  int position=buffer.position();
  buffer.position(getNextOffset());
  if (buffer.remaining() < 8)   return false;
  boolean hasNext=true;
  if (checkRecno) {
    buffer.order(ByteOrder.BIG_ENDIAN);
    int declaredRecNo=buffer.getInt();
    hasNext=declaredRecNo == record.number + 1;
  }
  buffer.position(position);
  return hasNext;
}
