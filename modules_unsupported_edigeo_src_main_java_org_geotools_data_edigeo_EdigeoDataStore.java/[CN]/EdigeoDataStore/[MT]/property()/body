{
  String typeSpec="";
  if (ftAtt != null) {
    Iterator<String> it=ftAtt.keySet().iterator();
    while (it.hasNext()) {
      String key=it.next();
      String type=edigeoType.get(ftAtt.get(key).get("type"));
      typeSpec+=key + ":" + type+ ",";
    }
  }
  typeSpec+="the_geom:" + scdObj.get(pciObj).get("type");
  return typeSpec;
}
