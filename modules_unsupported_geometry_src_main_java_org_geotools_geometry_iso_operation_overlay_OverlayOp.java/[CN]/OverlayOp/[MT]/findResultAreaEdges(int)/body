{
  for (Iterator it=graph.getEdgeEnds().iterator(); it.hasNext(); ) {
    DirectedEdge de=(DirectedEdge)it.next();
    Label label=de.getLabel();
    if (label.isArea() && !de.isInteriorAreaEdge() && isResultOfOp(label.getLocation(0,Position.RIGHT),label.getLocation(1,Position.RIGHT),opCode)) {
      de.setInResult(true);
    }
  }
}
