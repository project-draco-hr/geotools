{
  String localTypeName=remoteTypeName.getLocalPart();
  if (!XMLConstants.DEFAULT_NS_PREFIX.equals(remoteTypeName.getPrefix())) {
    localTypeName=remoteTypeName.getPrefix() + (gmlCompatibleTypenames ? NAME_SEPARATOR_GML_COMPATIBLE : NAME_SEPARATOR) + localTypeName;
  }
  return localTypeName;
}
