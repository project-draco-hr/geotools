{
  List<String> csvRecord=new ArrayList<String>();
  for (  Property property : feature.getProperties()) {
    Object value=property.getValue();
    if (value == null) {
      csvRecord.add("");
    }
 else {
      String txt=value.toString();
      csvRecord.add(txt);
    }
  }
  return csvRecord.toArray(new String[csvRecord.size() - 1]);
}
