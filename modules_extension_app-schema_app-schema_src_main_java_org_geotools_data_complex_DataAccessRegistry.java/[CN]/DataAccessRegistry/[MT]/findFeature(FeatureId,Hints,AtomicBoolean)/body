{
  for (  DataAccess<FeatureType,Feature> dataAccess : registry) {
    if (dataAccess instanceof AppSchemaDataAccess) {
      Feature feature=((AppSchemaDataAccess)dataAccess).findFeature(id,hints,stopFlag);
      if (feature != null) {
        return feature;
      }
synchronized (stopFlag) {
        if (stopFlag.get()) {
          return null;
        }
      }
    }
  }
  return null;
}
