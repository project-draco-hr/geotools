{
  EFeatureContextInfo eContextInfo=eContext(checkIsValid).eStructure();
  EFeatureDataStoreInfo eStoreInfo=eContextInfo.eGetDataStoreInfo(eDomainID,eNsURI);
  if (eStoreInfo != null) {
    return eStoreInfo.eGetFolderInfo(eFolderName).eGetFeatureInfo(eFeatureName);
  }
  return eContextInfo.eFeatureInfoCache().get(eNsURI,eDomainID,eFolderName,eFeatureName);
}
