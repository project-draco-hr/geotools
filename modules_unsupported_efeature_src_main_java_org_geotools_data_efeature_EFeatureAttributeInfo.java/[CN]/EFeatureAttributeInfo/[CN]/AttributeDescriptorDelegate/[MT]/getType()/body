{
  if (isAvailable() && attributeType == null) {
    final Class<?> cls=eAttribute().getEAttributeType().getInstanceClass();
    attributeType=new AttributeTypeImpl(getName(),cls,eIsID,false,Collections.<Filter>emptyList(),null,null){
      @Override public Object parse(      Object value) throws IllegalArgumentException {
        return DataBuilder.parse(cls,value);
      }
    }
;
  }
  return attributeType;
}
