{
  if (newScalarCapabilities != scalarCapabilities) {
    NotificationChain msgs=null;
    if (scalarCapabilities != null)     msgs=((InternalEObject)scalarCapabilities).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Fes20Package.FILTER_CAPABILITIES_TYPE__SCALAR_CAPABILITIES,null,msgs);
