{
  if (newIdCapabilities != idCapabilities) {
    NotificationChain msgs=null;
    if (idCapabilities != null)     msgs=((InternalEObject)idCapabilities).eInverseRemove(this,EOPPOSITE_FEATURE_BASE - Fes20Package.FILTER_CAPABILITIES_TYPE__ID_CAPABILITIES,null,msgs);
    if (newIdCapabilities != null)     msgs=((InternalEObject)newIdCapabilities).eInverseAdd(this,EOPPOSITE_FEATURE_BASE - Fes20Package.FILTER_CAPABILITIES_TYPE__ID_CAPABILITIES,null,msgs);
    msgs=basicSetIdCapabilities(newIdCapabilities,msgs);
    if (msgs != null)     msgs.dispatch();
  }
 else   if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.SET,Fes20Package.FILTER_CAPABILITIES_TYPE__ID_CAPABILITIES,newIdCapabilities,newIdCapabilities));
}
