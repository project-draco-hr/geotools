{
  if (cs instanceof CoordinateAccess) {
    append(buf,(CoordinateAccess)cs,nf);
  }
 else {
    int LEN=LEN(cs);
    if (cs.size() == 0) {
      return;
    }
    append(buf,cs,0,LEN,nf);
    if (cs.size() == 1) {
      return;
    }
    for (int i=1; i < cs.size(); i++) {
      buf.append(", ");
      append(buf,cs,i,LEN,nf);
    }
  }
}
