{
  if (sysId != null && sysId.endsWith("dtd")) {
    return new InputSource(new StringReader(""));
  }
  try {
    if (false) {
      throw new IOException();
    }
    return super.resolveEntity(pubId,sysId);
  }
 catch (  IOException e) {
    SAXException se=new SAXException(e.getLocalizedMessage());
    se.initCause(e);
    throw se;
  }
}
