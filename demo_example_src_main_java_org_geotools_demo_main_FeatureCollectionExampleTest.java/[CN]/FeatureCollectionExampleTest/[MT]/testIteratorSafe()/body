{
  int count=0;
  Iterator<SimpleFeature> iterator=features.iterator();
  try {
    while (iterator.hasNext()) {
      try {
        SimpleFeature feature=iterator.next();
        count++;
      }
 catch (      RuntimeException dataProblem) {
        if (iterator.hasNext())         continue;
        throw dataProblem;
      }
    }
  }
  finally {
    features.close(iterator);
  }
  assertTrue(count != 0);
}
