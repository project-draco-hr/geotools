{
  if (data == null) {
    data=new double[length];
  }
 else {
    int currLength=data.length;
    if (length > currLength) {
      int newLength=(currLength * 3) / 2 + 1;
      if (newLength < length) {
        newLength=length;
      }
      double[] newData=new double[newLength];
      System.arraycopy(data,0,newData,0,currLength);
      this.data=newData;
    }
  }
}
