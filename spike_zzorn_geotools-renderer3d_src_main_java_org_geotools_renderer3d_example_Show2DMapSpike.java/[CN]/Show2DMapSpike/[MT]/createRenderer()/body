{
  GTRenderer renderer;
  if (false) {
    renderer=new StreamingRenderer();
    HashMap hints=new HashMap();
    hints.put("memoryPreloadingEnabled",Boolean.TRUE);
    renderer.setRendererHints(hints);
  }
 else {
    renderer=new StreamingRenderer();
    HashMap hints=new HashMap();
    hints.put("memoryPreloadingEnabled",Boolean.FALSE);
    renderer.setRendererHints(hints);
  }
  return renderer;
}
