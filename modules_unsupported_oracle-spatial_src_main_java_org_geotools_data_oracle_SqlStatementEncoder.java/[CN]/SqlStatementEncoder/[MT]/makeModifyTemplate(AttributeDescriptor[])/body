{
  StringBuffer buffer=new StringBuffer("UPDATE ");
  buffer.append(tableName);
  buffer.append(" SET ");
  for (int i=0; i < attributeTypes.length; i++) {
    buffer.append(attributeTypes[i].getLocalName());
    buffer.append(" = ? ");
    if (i < (attributeTypes.length - 1)) {
      buffer.append(", ");
    }
 else {
      buffer.append(" ");
    }
  }
  return buffer.toString();
}
