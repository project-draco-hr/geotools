{
  if (name.indexOf(".") == -1 && name.indexOf("_") == -1) {
    try {
      Field field=Types.class.getField(name);
      Integer integer=(Integer)field.get(null);
      intMap.put(integer,javaType);
    }
 catch (    Exception e) {
      e.printStackTrace();
    }
  }
  sqlMap.put(name,javaType);
  typeMap.put(javaType,name);
}
