{
  if (code != null) {
    appendSeparator(false);
    setColor(CODELIST_COLOR);
    final String name=code.name();
    final boolean needQuotes=(name.indexOf(' ') >= 0);
    if (needQuotes) {
      buffer.append(symbols.quote);
    }
    buffer.append(name);
    if (needQuotes) {
      buffer.append(symbols.quote);
      setInvalidWKT(code.getClass());
    }
    resetColor();
  }
}
