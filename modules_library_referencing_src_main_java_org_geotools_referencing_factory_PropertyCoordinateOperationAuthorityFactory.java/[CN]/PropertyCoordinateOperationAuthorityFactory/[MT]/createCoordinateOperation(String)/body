{
  String[] crsPair=trimAuthority(code).split(",");
  if (crsPair.length == 2) {
    Set<CoordinateOperation> coordopset=createFromCoordinateReferenceSystemCodes(trimAuthority(crsPair[0]),trimAuthority(crsPair[1]));
    if (!coordopset.isEmpty()) {
      return coordopset.iterator().next();
    }
  }
  return null;
}
