{
  StyleBuilder sb=new StyleBuilder();
  ColorMap colorMap=sb.createColorMap(new String[]{"1","2","3","4"},new double[]{0,100,2000,5000},new Color[]{Color.RED,Color.WHITE,Color.GREEN,Color.BLUE},ColorMap.TYPE_RAMP);
  RasterSymbolizer rasterSymbolizer=sb.createRasterSymbolizer(colorMap,1d);
  return rasterSymbolizer;
}
