{
  ReferencedEnvelope mapExtent=new ReferencedEnvelope(0.00023726353151687,180.00025801626,-269.99984914217,-89.999828389438,DefaultGeographicCRS.WGS84);
  Rectangle screenSize=new Rectangle(256,256);
  AffineTransform w2s=RendererUtilities.worldToScreenTransform(mapExtent,screenSize);
  GridCoverageRenderer renderer=new GridCoverageRenderer(DefaultGeographicCRS.WGS84,mapExtent,screenSize,w2s);
  RasterSymbolizer rasterSymbolizer=new StyleBuilder().createRasterSymbolizer();
  RenderedImage image=renderer.renderImage(worldReader,null,rasterSymbolizer,Interpolation.getInstance(Interpolation.INTERP_NEAREST),Color.RED,256,256);
  assertNull(image);
}
