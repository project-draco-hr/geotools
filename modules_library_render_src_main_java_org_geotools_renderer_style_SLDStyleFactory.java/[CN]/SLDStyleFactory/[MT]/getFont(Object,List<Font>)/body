{
  if (fonts != null) {
    for (    Font curr : fonts) {
      String requestedFont=evalToString(curr.getFontFamily(),feature,null);
      java.awt.Font javaFont=FontCache.getDefaultInstance().getFont(requestedFont);
      if (javaFont != null) {
        return styleFont(feature,curr,javaFont);
      }
    }
  }
  java.awt.Font result=new java.awt.Font("Serif",java.awt.Font.PLAIN,12);
  if ((fonts != null) && (!fonts.isEmpty())) {
    return styleFont(feature,fonts.get(0),result);
  }
 else {
    return result;
  }
}
