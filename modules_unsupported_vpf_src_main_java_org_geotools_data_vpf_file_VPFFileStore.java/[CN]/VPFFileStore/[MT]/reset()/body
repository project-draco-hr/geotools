{
  Iterator<SimpleFeatureType> iter=files.values().iterator();
  VPFFile file=null;
  while (iter.hasNext()) {
    try {
      SimpleFeatureType schema=iter.next();
      file=(VPFFile)schema.getUserData().get(VPFFile.class);
      file.close();
    }
 catch (    Exception exc) {
      exc.printStackTrace();
    }
  }
  files.clear();
}
