{
  if (capabilities == null) {
    capabilities=new QueryCapabilities(){
      public boolean isOffsetSupported(){
        return true;
      }
      public boolean isReliableFIDSupported(){
        return true;
      }
      public boolean supportsSorting(      org.opengis.filter.sort.SortBy[] sortAttributes){
        return true;
      }
    }
;
  }
  return capabilities;
}
