{
  final StringBuilder buffer=new StringBuilder();
  buffer.append("Description of a rasterGranuleLoader ").append("\n").append("BBOX:\t\t").append(granuleBBOX.toString()).append("file:\t\t").append(granuleFile).append("gridToWorld:\t\t").append(baseGridToWorld);
  int i=1;
  for (  final Level level : granuleLevels.values()) {
    i++;
    buffer.append("Description of level ").append(i++).append("\n").append(level.toString()).append("\n");
  }
  return super.toString();
}
