{
  id=atts.getValue("","id");
  if (id == null) {
    id=atts.getValue(namespaceURI,"id");
  }
  name=atts.getValue("","name");
  if (name == null) {
    name=atts.getValue(namespaceURI,"name");
  }
  ref=atts.getValue("","ref");
  if (ref == null) {
    ref=atts.getValue(namespaceURI,"ref");
  }
}
