{
  int i=0;
  int low=0;
  int high=atts.size() - 1;
  while (low <= high) {
    i=(low + high) / 2;
    CellAttribute catt=(CellAttribute)atts.elementAt(i);
    int c=catt.compare(cat);
    if (c == 0) {
      low=high + 1;
    }
 else     if (c < 0) {
      high=i - 1;
    }
 else {
      low=i++ + 1;
    }
  }
  atts.insertElementAt(new CellAttribute(cat,value),i);
}
