{
  IndexRecord rec=findRecord(reqAttribute);
  if (rec == null)   return new ArrayList(0);
  goTo(foundPos + 1);
  ArrayList l=new ArrayList();
  while (rec.getAttribute().equals(reqAttribute)) {
    l.add(new Long(rec.getFeatureID()));
    if (!hasNext())     break;
    rec=next();
  }
  return l;
}
