{
  scanForPlugins();
  final Iterator<FootprintLoaderSpi> it=getServiceRegistry().getServiceProviders(FootprintLoaderSpi.class,true);
  final Set<FootprintLoaderSpi> loaders=new HashSet<FootprintLoaderSpi>();
  while (it.hasNext()) {
    loaders.add(it.next());
  }
  return Collections.unmodifiableSet(loaders);
}
