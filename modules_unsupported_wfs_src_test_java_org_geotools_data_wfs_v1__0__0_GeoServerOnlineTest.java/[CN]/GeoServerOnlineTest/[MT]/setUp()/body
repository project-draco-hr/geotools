{
  url=new URL(SERVER_URL);
  if (url != null && url.toString().indexOf("localhost") != -1) {
    InputStream stream=null;
    try {
      stream=url.openStream();
    }
 catch (    Throwable t) {
      System.err.println("Warning you local geoserver is not available. test disabled ");
      url=null;
    }
 finally {
      if (stream != null)       try {
        stream.close();
      }
 catch (      IOException e) {
      }
    }
  }
}
