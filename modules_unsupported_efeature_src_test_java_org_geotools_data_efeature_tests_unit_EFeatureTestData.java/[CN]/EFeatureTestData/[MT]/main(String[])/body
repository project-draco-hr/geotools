{
  String[] s=generatePointWKTs(10,0,20);
  for (  String it : s) {
    System.out.println(it);
  }
  s=generateLineStringWKTs(10,0,20,1,5);
  for (  String it : s) {
    System.out.println(it);
  }
  s=generatePolygonWKTs(10,0,20,1,5,1,10);
  for (  String it : s) {
    System.out.println(it);
  }
}
