{
  double d=c1 * c1 - 4 * c2 * c0;
  if (d > 0) {
    d=Math.sqrt(d);
    if (c1 < 0) {
      d=-d;
    }
    final double q=0.5 * (d - c1);
    return new double[]{q / c2,(q != 0) ? c0 / q : -0.5 * (d + c1) / c2};
  }
 else   if (d == 0) {
    return new double[]{-c1 / (2 * c2)};
  }
 else {
    return NO_REAL_ROOT;
  }
}
