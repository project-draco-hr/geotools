{
  int n=c.length;
  while (n != 0 && c[--n] == 0) {
  }
switch (n) {
case 0:
    return NO_REAL_ROOT;
case 1:
  return new double[]{-c[0] / c[1]};
case 2:
return quadraticRoots(c[0],c[1],c[2]);
case 3:
return cubicRoots(c[0],c[1],c[2],c[3]);
default :
throw new UnsupportedOperationException(String.valueOf(n));
}
}
