{
  BinarySpatialOperator gf=null;
  encoder.setSelectivityClause(null);
  gf=createBeyondFilter(spatialColumn,geometryLiteral,10.0);
  encoder.setSelectivityClause(null);
  sqlString=this.encoder.encodeToString(gf);
  assertEquals("Beyond","WHERE db2gse.ST_Distance(\"Geom\", db2gse.ST_LINESTRING('LINESTRING (0 0, 300 300)', 1)) > 10.0",sqlString);
  gf=createDistanceFilter(spatialColumn,geometryLiteral,10.0);
  encoder.setSelectivityClause(null);
  sqlString=this.encoder.encodeToString(gf);
  assertEquals("DWithin","WHERE db2gse.ST_Distance(\"Geom\", db2gse.ST_LINESTRING('LINESTRING (0 0, 300 300)', 1)) < 10.0",sqlString);
  encoder.setSelectivityClause("SELECTIVITY 0.01");
  sqlString=this.encoder.encodeToString(gf);
  assertEquals("DWithin","WHERE db2gse.ST_Distance(\"Geom\", db2gse.ST_LINESTRING('LINESTRING (0 0, 300 300)', 1)) < 10.0 SELECTIVITY 0.01",sqlString);
}
