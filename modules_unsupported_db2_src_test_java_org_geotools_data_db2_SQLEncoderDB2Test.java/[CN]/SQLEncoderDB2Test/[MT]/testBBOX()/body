{
  encoder.setSelectivityClause(null);
  BBOX bbox=createBBOXFilter(spatialColumn,bboxEnv);
  encoder.setSelectivityClause(null);
  sqlString=this.encoder.encodeToString(bbox);
  assertEquals("geometry literal","WHERE db2gse.EnvelopesIntersect(\"Geom\", 0.0, 0.0, 300.0, 300.0, 1) = 1",sqlString);
  encoder.setSelectivityClause("SELECTIVITY 0.001");
  sqlString=this.encoder.encodeToString(bbox);
  assertEquals("geometry literal","WHERE db2gse.EnvelopesIntersect(\"Geom\", 0.0, 0.0, 300.0, 300.0, 1) = 1 SELECTIVITY 0.001",sqlString);
}
