{
  final int[] ARGB;
  if (colors != null && colors.length != 0) {
    ARGB=new int[colors.length];
    for (int i=0; i < ARGB.length; i++) {
      final Color color=colors[i];
      if (color != null) {
        ARGB[i]=color.getRGB();
      }
 else {
      }
    }
  }
 else {
    ARGB=DEFAULT;
  }
  return ARGB;
}
