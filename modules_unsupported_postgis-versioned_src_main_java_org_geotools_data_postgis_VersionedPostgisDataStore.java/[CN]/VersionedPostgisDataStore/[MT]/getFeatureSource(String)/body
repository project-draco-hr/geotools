{
  if (isVersioned(typeName))   return new VersionedPostgisFeatureStore(getSchema(typeName),this);
  SimpleFeatureSource source=wrapped.getFeatureSource(typeName);
  if (TBL_CHANGESETS.equals(typeName))   return new WrappingPostgisFeatureSource(source,this);
  if (source instanceof SimpleFeatureLocking)   return new WrappingPostgisFeatureLocking((SimpleFeatureLocking)source,this);
 else   if (source instanceof SimpleFeatureStore)   return new WrappingPostgisFeatureStore((SimpleFeatureStore)source,this);
 else   return new WrappingPostgisFeatureSource((SimpleFeatureSource)source,this);
}
