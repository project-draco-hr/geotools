{
  if (expression instanceof MathExpression) {
    return "leftValue";
  }
 else   if ((expression instanceof AttributeExpression) || (expression instanceof LiteralExpression)) {
    return "";
  }
 else   if (expression instanceof FunctionExpression) {
    return "accumulate";
  }
 else {
    throw new IllegalFilterException("Created illegal expression: " + expression.getClass().toString());
  }
}
