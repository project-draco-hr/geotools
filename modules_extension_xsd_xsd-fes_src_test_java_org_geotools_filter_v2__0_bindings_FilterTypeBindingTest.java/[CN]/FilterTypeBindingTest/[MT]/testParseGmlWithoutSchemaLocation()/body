{
  String xml="<fes:Filter xmlns:gml='http://www.opengis.net/gml/3.2' xmlns:fes='http://www.opengis.net/fes/2.0'> " + " <fes:BBOX> " + "  <fes:ValueReference>it.geosolutions:the_geom</fes:ValueReference> "+ "  <gml:Envelope srsName='urn:ogc:def:crs:EPSG::2154'> "+ "   <gml:lowerCorner>834021 6504682</gml:lowerCorner> "+ "   <gml:upperCorner>943396 6604240</gml:upperCorner> "+ "  </gml:Envelope> "+ " </fes:BBOX> "+ "</fes:Filter> ";
  buildDocument(xml);
  BBOX f=(BBOX)parse();
  assertTrue(f.getExpression1() instanceof PropertyName);
  assertTrue(f.getExpression2() instanceof Literal);
}
