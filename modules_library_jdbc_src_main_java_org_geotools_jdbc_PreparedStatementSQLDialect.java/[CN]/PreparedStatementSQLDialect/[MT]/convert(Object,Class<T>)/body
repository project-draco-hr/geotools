{
  if (value == null) {
    return (T)value;
  }
  if (!binding.isInstance(value)) {
    Object converted=Converters.convert(value,binding);
    if (converted != null) {
      value=converted;
    }
 else {
      dataStore.getLogger().warning("Unable to convert " + value + " to "+ binding.getName());
    }
  }
  return (T)value;
}
