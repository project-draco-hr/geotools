{
  if (source == null)   return null;
  String string=(String)source;
  try {
    Date parsed=format1.parse(string);
    return target.cast(parsed);
  }
 catch (  Exception ignore) {
  }
  try {
    Date parsed=format2.parse(string);
    return target.cast(parsed);
  }
 catch (  Exception ignore) {
  }
  return null;
}
