{
  super.setUp();
  setUpInternal();
  skipOnFailure=Boolean.parseBoolean(fixture.getProperty(SKIP_ON_FAILURE_KEY,SKIP_ON_FAILURE_DEFAULT));
  try {
    connect();
  }
 catch (  Exception e) {
    if (skipOnFailure) {
      fixture=null;
      e.printStackTrace();
    }
 else {
      throw e;
    }
  }
}
