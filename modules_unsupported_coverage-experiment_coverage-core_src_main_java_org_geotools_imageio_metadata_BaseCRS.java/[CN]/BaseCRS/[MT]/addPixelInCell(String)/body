{
  if (datum.getSelectedChoice() == Datum.IMAGE_DATUM)   if ("cell_corner".equals(value) || "cell_center".equals(value))   datum.getChild().setString(SpatioTemporalMetadataFormat.MD_DTM_ID_PIXELINCELL,value);
 else   throw new IllegalArgumentException("Pixel in Cell must be one of the {cell_corner; cell_center}");
 else   throw new IllegalArgumentException("Could not set Pixel in Cell for non-Image Datum type");
}
