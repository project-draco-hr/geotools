{
  int disposed;
  do {
    disposed=0;
    for (final Iterator<GridCoverage2D> it=views.values().iterator(); it.hasNext(); ) {
      final GridCoverage2D coverage=it.next();
      if (coverage.disposeImage(force)) {
        it.remove();
        disposed++;
      }
    }
  }
 while (disposed != 0);
  return views.values();
}
