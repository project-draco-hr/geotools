  be=(OptEdge)itr.next();
  assertTrue(be.equals(m_other1) || be.equals(m_other2) || be.equals(m_other3)|| be.equals(m_other4));
  assertTrue(!itr.hasNext());
  addSame();
  itr=m_edge.getRelated();
  assertTrue(itr.hasNext());
