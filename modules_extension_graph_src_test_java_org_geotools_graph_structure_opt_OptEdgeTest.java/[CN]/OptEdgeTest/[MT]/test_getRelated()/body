  assertTrue(be.equals(m_other1) || be.equals(m_other2) || be.equals(m_other3)|| be.equals(m_other4)|| be.equals(m_same));
  assertTrue(itr.hasNext());
  be=(OptEdge)itr.next();
  assertTrue(be.equals(m_other1) || be.equals(m_other2) || be.equals(m_other3)|| be.equals(m_other4)|| be.equals(m_same));
  assertTrue(itr.hasNext());
  be=(OptEdge)itr.next();
  assertTrue(be.equals(m_other1) || be.equals(m_other2) || be.equals(m_other3)|| be.equals(m_other4)|| be.equals(m_same));
