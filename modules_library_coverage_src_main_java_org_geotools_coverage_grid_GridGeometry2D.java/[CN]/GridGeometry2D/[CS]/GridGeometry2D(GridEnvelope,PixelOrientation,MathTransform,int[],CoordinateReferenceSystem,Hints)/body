{
  this(gridRange,anchor,(gridToCRS != null) && (gridToCRS.getSourceDimensions() == 2) && (gridToCRS.getTargetDimensions() == 2)&& PixelOrientation.UPPER_LEFT.equals(anchor) ? PixelInCell.CELL_CORNER : PixelInCell.CELL_CENTER,gridToCRS,getMathTransform2D(gridToCRS,gridRange,dimensions,hints),dimensions,crs);
}
