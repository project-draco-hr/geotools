{
  if (unset) {
    return null;
  }
  Stroke stroke=strokeBuilder.build();
  if (stroke == null) {
    stroke=Stroke.DEFAULT;
  }
  LineSymbolizer ls=sf.createLineSymbolizer(stroke,null);
  if (geometry != null) {
    ls.setGeometry(geometry);
  }
  if (uom != null) {
    ls.setUnitOfMeasure(uom);
  }
  if (parent == null) {
    reset();
  }
  return ls;
}
