{
  if (unset) {
    return null;
  }
  PolygonSymbolizer ps=sf.createPolygonSymbolizer(stroke.build(),fill.build(),null);
  if (geometry != null) {
    ps.setGeometry(geometry);
  }
  if (uom != null) {
    ps.setUnitOfMeasure(uom);
  }
  ps.getOptions().putAll(options);
  if (parent == null) {
    reset();
  }
  return ps;
}
