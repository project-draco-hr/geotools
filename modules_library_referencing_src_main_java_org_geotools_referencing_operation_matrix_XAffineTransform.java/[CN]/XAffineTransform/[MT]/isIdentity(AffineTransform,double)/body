{
  if (tr.isIdentity()) {
    return true;
  }
  tolerance=Math.abs(tolerance);
  return Math.abs(tr.getScaleX() - 1) <= tolerance && Math.abs(tr.getScaleY() - 1) <= tolerance && Math.abs(tr.getShearX()) <= tolerance && Math.abs(tr.getShearY()) <= tolerance && Math.abs(tr.getTranslateX()) <= tolerance && Math.abs(tr.getTranslateY()) <= tolerance;
}
