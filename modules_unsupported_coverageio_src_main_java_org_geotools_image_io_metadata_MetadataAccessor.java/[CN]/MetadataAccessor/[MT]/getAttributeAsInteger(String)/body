{
  String value=getAttributeAsString(attribute);
  if (value != null) {
    value=trimFractionalPart(value);
    try {
      return Integer.valueOf(value);
    }
 catch (    NumberFormatException e) {
      warning("getAttributeAsInteger",ErrorKeys.UNPARSABLE_NUMBER_$1,value);
    }
  }
  return null;
}
