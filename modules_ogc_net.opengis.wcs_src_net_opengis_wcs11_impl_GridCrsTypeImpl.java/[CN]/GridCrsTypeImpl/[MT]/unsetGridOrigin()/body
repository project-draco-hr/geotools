{
  Object oldGridOrigin=gridOrigin;
  boolean oldGridOriginESet=gridOriginESet;
  gridOrigin=GRID_ORIGIN_EDEFAULT;
  gridOriginESet=false;
  if (eNotificationRequired())   eNotify(new ENotificationImpl(this,Notification.UNSET,Wcs111Package.GRID_CRS_TYPE__GRID_ORIGIN,oldGridOrigin,GRID_ORIGIN_EDEFAULT,oldGridOriginESet));
}
