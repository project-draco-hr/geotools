{
  if (feature.getAttribute(colNames[1]) == null) {
    try {
      String fid=feature.getID();
      String id;
      if (fid.startsWith(tableName + ".") && UUID_PATTERN.matcher(fid.substring(tableName.length() + 1)).matches()) {
        id=fid.substring(tableName.length() + 1);
      }
 else {
        id=wrapped.createID(conn,feature,statement);
      }
      feature.setAttribute(colNames[1],id);
    }
 catch (    Exception e) {
      throw new DataSourceException("Could not generate key for the " + "unset primary key column " + colNames[1],e);
    }
  }
  return super.createID(conn,feature,statement);
}
