{
  if (!(obj instanceof AffineTransform)) {
    return false;
  }
  AffineTransform a=(AffineTransform)obj;
  return Utilities.equals(getScaleX(),a.getScaleX()) && Utilities.equals(getScaleY(),a.getScaleY()) && Utilities.equals(getShearX(),a.getShearY())&& Utilities.equals(getTranslateX(),a.getTranslateX())&& Utilities.equals(getTranslateY(),a.getTranslateY());
}
