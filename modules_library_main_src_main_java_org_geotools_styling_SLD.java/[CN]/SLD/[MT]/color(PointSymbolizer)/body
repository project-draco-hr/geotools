{
  if (symbolizer == null) {
    return null;
  }
  Graphic graphic=symbolizer.getGraphic();
  if (graphic == null) {
    return null;
  }
  for (  GraphicalSymbol gs : graphic.graphicalSymbols()) {
    if (gs != null && gs instanceof Mark) {
      Mark mark=(Mark)gs;
      Stroke stroke=mark.getStroke();
      if (stroke != null) {
        Color colour=color(stroke);
        if (colour != null) {
          return colour;
        }
      }
    }
  }
  return null;
}
