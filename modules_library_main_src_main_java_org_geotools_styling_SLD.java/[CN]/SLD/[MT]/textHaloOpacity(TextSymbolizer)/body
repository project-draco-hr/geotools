{
  if (symbolizer == null) {
    return Double.NaN;
  }
  Halo halo=symbolizer.getHalo();
  if (halo == null) {
    return Double.NaN;
  }
  Fill fill=halo.getFill();
  if (fill == null) {
    return Double.NaN;
  }
  Expression expr=fill.getOpacity();
  if (expr == null) {
    return Double.NaN;
  }
  return Filters.asDouble(expr);
}
