{
  Edge[] tEdges=t.getEdges();
  Edge shared=null;
  for (int i=0; i < 3; i++) {
    if (tEdges[i].equals(edge1)) {
      shared=edge1;
    }
 else     if (tEdges[i].equals(edge2)) {
      shared=edge2;
    }
 else     if (tEdges[i].equals(edge3)) {
      shared=edge3;
    }
  }
  return shared;
}
