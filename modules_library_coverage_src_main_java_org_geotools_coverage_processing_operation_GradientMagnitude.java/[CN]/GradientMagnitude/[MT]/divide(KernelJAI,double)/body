{
  if (denominator != 1) {
    final float[] data=kernel.getKernelData();
    final int length=data.length;
    for (int i=0; i < length; i++) {
      data[i]/=denominator;
    }
    kernel=new KernelJAI(kernel.getWidth(),kernel.getHeight(),kernel.getXOrigin(),kernel.getYOrigin(),data);
  }
  return kernel;
}
