{
  if (image instanceof PropertySource) {
    add((PropertySource)image,null);
  }
 else {
    final String[] names=image.getPropertyNames();
    if (names != null) {
      for (int i=0; i < names.length; i++) {
        final String name=names[i];
        add(name,image.getProperty(name));
      }
    }
  }
}
