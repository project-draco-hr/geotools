{
  String user=null;
  Transaction transaction=getTransaction();
  if (transaction != null) {
    user=(String)transaction.getProperty(GEOGIT_AUTHOR);
    if (user == null) {
      user=(String)transaction.getProperty("VersioningCommitAuthor");
    }
  }
  if (user == null) {
    user="anonymous";
  }
  return user;
}
