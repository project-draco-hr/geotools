{
  try {
    final ImageMosaicReader reader=new ImageMosaicReader(source,hints);
    return reader;
  }
 catch (  MalformedURLException e) {
    if (LOGGER.isLoggable(Level.WARNING))     LOGGER.log(Level.WARNING,e.getLocalizedMessage(),e);
    return null;
  }
catch (  IOException e) {
    if (LOGGER.isLoggable(Level.WARNING))     LOGGER.log(Level.WARNING,e.getLocalizedMessage(),e);
    return null;
  }
}
